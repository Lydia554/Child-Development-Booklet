{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useAuthStore } from '@/stores/authStore';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      name: '',\n      birthdate: '',\n      errorMessage: ''\n    };\n  },\n  methods: {\n    async registerChild() {\n      try {\n        const authStore = useAuthStore();\n\n        // Kreiranje deteta u bazi\n        const response = await axios.post('http://localhost:5000/api', {\n          name: this.name,\n          birthdate: this.birthdate\n        }, {\n          headers: {\n            Authorization: `Bearer ${sessionStorage.getItem('token')}`\n          }\n        });\n        if (response.data && response.data._id) {\n          console.log(\"‚úÖ Dete registrovano:\", response.data);\n\n          // Dodaj dete u authStore i sessionStorage\n          authStore.addChildId(response.data._id);\n          authStore.setCurrentChildId(response.data._id);\n          sessionStorage.setItem('childId', response.data._id);\n\n          // Preusmeri na dashboard\n          this.$router.push('/dashboard');\n        } else {\n          this.errorMessage = \"Gre≈°ka prilikom registracije deteta.\";\n        }\n      } catch (error) {\n        console.error(\"‚ùå Gre≈°ka pri registraciji deteta:\", error);\n        this.errorMessage = \"Neuspe≈°na registracija. Poku≈°ajte ponovo.\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["useAuthStore","axios","data","name","birthdate","errorMessage","methods","registerChild","authStore","response","post","headers","Authorization","sessionStorage","getItem","_id","console","log","addChildId","setCurrentChildId","setItem","$router","push","error"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildRegistrationForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"registration-container\">\r\n    <h2>Registracija Deteta</h2>\r\n\r\n    <div v-if=\"errorMessage\" class=\"error-message\">\r\n      {{ errorMessage }}\r\n    </div>\r\n\r\n    <form @submit.prevent=\"registerChild\">\r\n      <input type=\"text\" v-model=\"name\" placeholder=\"Ime deteta\" required />\r\n      <input type=\"date\" v-model=\"birthdate\" required />\r\n      <button type=\"submit\">Registruj Dete</button>\r\n    </form>\r\n\r\n    <div class=\"dashboard-link\">\r\n      <router-link to=\"/dashboard\">Idi na Dashboard</router-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useAuthStore } from '@/stores/authStore';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      name: '',\r\n      birthdate: '',\r\n      errorMessage: '',\r\n    };\r\n  },\r\n  methods: {\r\n    async registerChild() {\r\n      try {\r\n        const authStore = useAuthStore();\r\n\r\n        // Kreiranje deteta u bazi\r\n        const response = await axios.post('http://localhost:5000/api', {\r\n          name: this.name,\r\n          birthdate: this.birthdate\r\n        }, {\r\n          headers: { Authorization: `Bearer ${sessionStorage.getItem('token')}` }\r\n        });\r\n\r\n        if (response.data && response.data._id) {\r\n          console.log(\"‚úÖ Dete registrovano:\", response.data);\r\n          \r\n          // Dodaj dete u authStore i sessionStorage\r\n          authStore.addChildId(response.data._id);\r\n          authStore.setCurrentChildId(response.data._id);\r\n          sessionStorage.setItem('childId', response.data._id);\r\n\r\n          // Preusmeri na dashboard\r\n          this.$router.push('/dashboard');\r\n        } else {\r\n          this.errorMessage = \"Gre≈°ka prilikom registracije deteta.\";\r\n        }\r\n      } catch (error) {\r\n        console.error(\"‚ùå Gre≈°ka pri registraciji deteta:\", error);\r\n        this.errorMessage = \"Neuspe≈°na registracija. Poku≈°ajte ponovo.\";\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n$primary-color: #007bff;\r\n$primary-hover: #0056b3;\r\n$error-bg: #f8d7da;\r\n$error-border: #f5c6cb;\r\n$error-text: #721c24;\r\n$text-color: #333;\r\n$background-light: #f7f7f7;\r\n$border-color: #ccc;\r\n\r\n.child-registration-container {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: $background-light;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n\r\n  h2 {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: $text-color;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .error-message {\r\n    margin-bottom: 20px;\r\n    padding: 10px;\r\n    background-color: $error-bg;\r\n    border: 1px solid $error-border;\r\n    color: $error-text;\r\n    border-radius: 5px;\r\n    text-align: center;\r\n  }\r\n\r\n  .child-registration-form {\r\n    display: flex;\r\n    flex-direction: column;\r\n\r\n    .form-group {\r\n      margin-bottom: 15px;\r\n\r\n      label {\r\n        font-size: 16px;\r\n        font-weight: bold;\r\n        color: $text-color;\r\n        margin-bottom: 5px;\r\n        display: block;\r\n      }\r\n\r\n      input {\r\n        width: 100%;\r\n        padding: 12px;\r\n        font-size: 16px;\r\n        border: 1px solid $border-color;\r\n        border-radius: 5px;\r\n        box-sizing: border-box;\r\n      }\r\n\r\n      input[type=\"date\"] {\r\n        padding: 10px;\r\n        font-size: 16px;\r\n      }\r\n    }\r\n\r\n    .submit-group {\r\n      display: flex;\r\n      justify-content: center;\r\n\r\n      .submit-btn {\r\n  padding: 12px 20px;\r\n  background-color: #4caf50;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.submit-btn:disabled {\r\n  background-color: #cccccc;\r\n  cursor: not-allowed;\r\n}\r\n    }\r\n\r\n        &:hover {\r\n          background-color: $primary-hover;\r\n        }\r\n      }\r\n}\r\n\r\n/* üì± Mobile Styles: Optimized for Phones */\r\n@media (max-width: 768px) {\r\n  .child-registration-container {\r\n    padding: 15px;\r\n  }\r\n\r\n  h2 {\r\n    font-size: 22px;\r\n  }\r\n\r\n  .form-group label {\r\n    font-size: 15px;\r\n  }\r\n\r\n  input {\r\n    font-size: 15px;\r\n    padding: 10px;\r\n  }\r\n\r\n  .submit-btn {\r\n    width: 100%;\r\n    font-size: 18px;\r\n    padding: 16px;\r\n  }\r\n}\r\n\r\n/* üì± Extra Small Mobile Screens (Max 480px) */\r\n@media (max-width: 480px) {\r\n  .child-registration-container {\r\n    padding: 12px;\r\n  }\r\n\r\n  h2 {\r\n    font-size: 20px;\r\n  }\r\n\r\n  .form-group label {\r\n    font-size: 14px;\r\n  }\r\n\r\n  input {\r\n    font-size: 14px;\r\n    padding: 10px;\r\n  }\r\n\r\n  .submit-btn {\r\n    font-size: 16px;\r\n    padding: 14px;\r\n  }\r\n}\r\n\r\n/* üì± Ultra-Small Mobile Screens (Max 360px) */\r\n@media (max-width: 360px) {\r\n  .child-registration-container {\r\n    padding: 10px;\r\n  }\r\n\r\n  h2 {\r\n    font-size: 18px;\r\n  }\r\n\r\n  .form-group label {\r\n    font-size: 14px;\r\n  }\r\n\r\n  input {\r\n    font-size: 14px;\r\n    padding: 8px;\r\n  }\r\n\r\n  .submit-btn {\r\n    font-size: 15px;\r\n    padding: 12px;\r\n  }\r\n}\r\n</style>\r\n\r\n"],"mappings":";AAqBA,SAASA,YAAW,QAAS,oBAAoB;AACjD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMC,SAAQ,GAAIR,YAAY,CAAC,CAAC;;QAEhC;QACA,MAAMS,QAAO,GAAI,MAAMR,KAAK,CAACS,IAAI,CAAC,2BAA2B,EAAE;UAC7DP,IAAI,EAAE,IAAI,CAACA,IAAI;UACfC,SAAS,EAAE,IAAI,CAACA;QAClB,CAAC,EAAE;UACDO,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUC,cAAc,CAACC,OAAO,CAAC,OAAO,CAAC;UAAG;QACxE,CAAC,CAAC;QAEF,IAAIL,QAAQ,CAACP,IAAG,IAAKO,QAAQ,CAACP,IAAI,CAACa,GAAG,EAAE;UACtCC,OAAO,CAACC,GAAG,CAAC,sBAAsB,EAAER,QAAQ,CAACP,IAAI,CAAC;;UAElD;UACAM,SAAS,CAACU,UAAU,CAACT,QAAQ,CAACP,IAAI,CAACa,GAAG,CAAC;UACvCP,SAAS,CAACW,iBAAiB,CAACV,QAAQ,CAACP,IAAI,CAACa,GAAG,CAAC;UAC9CF,cAAc,CAACO,OAAO,CAAC,SAAS,EAAEX,QAAQ,CAAC