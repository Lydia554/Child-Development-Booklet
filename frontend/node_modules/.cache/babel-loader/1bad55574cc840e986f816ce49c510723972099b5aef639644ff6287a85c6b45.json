{"ast":null,"code":"import { useAuthStore } from '@/stores/authStore';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      loading: true,\n      currentChild: null,\n      errorMessage: ''\n    };\n  },\n  async created() {\n    await this.fetchCurrentChild();\n  },\n  methods: {\n    async fetchCurrentChild() {\n      try {\n        const authStore = useAuthStore();\n        const childId = authStore.childId || sessionStorage.getItem('childId');\n        if (!childId) {\n          console.warn(\"⚠️ No child ID found in store or session.\");\n          this.errorMessage = \"Nema registrovanog deteta. Molimo registrujte dete.\";\n          return;\n        }\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${sessionStorage.getItem('token')}`\n          }\n        });\n        if (response.data) {\n          this.currentChild = response.data;\n        } else {\n          console.warn(\"⚠️ No child data received.\");\n          this.errorMessage = \"Podaci o detetu nisu pronađeni.\";\n        }\n      } catch (error) {\n        console.error(\"❌ Error fetching child data:\", error);\n        this.errorMessage = \"Greška prilikom učitavanja podataka. Pokušajte ponovo.\";\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["useAuthStore","axios","data","loading","currentChild","errorMessage","created","fetchCurrentChild","methods","authStore","childId","sessionStorage","getItem","console","warn","response","get","headers","Authorization","error"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\views\\DashboardPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard-container\">\r\n    <h2>Dobrodošli na Dashboard</h2>\r\n\r\n    <!-- Loading State -->\r\n    <div v-if=\"loading\">Loading...</div>\r\n\r\n    <!-- Error Handling -->\r\n    <div v-else-if=\"errorMessage\" class=\"error-message\">\r\n      {{ errorMessage }}\r\n    </div>\r\n\r\n    <!-- Main Content (Only if Child Exists) -->\r\n    <div v-else-if=\"currentChild\">\r\n      <h3>Opcije za dete: {{ currentChild.name }}</h3>\r\n\r\n      <div class=\"dashboard-links\">\r\n        <div class=\"links-grid\">\r\n          <router-link :to=\"`/milestone-form/${currentChild._id}`\" class=\"dashboard-link\">\r\n            Dodaj Kljucni Momenat\r\n          </router-link>\r\n          <router-link :to=\"`/milestone-list/${currentChild._id}`\" class=\"dashboard-link\">\r\n            Pregled Kljucnog Momenta\r\n          </router-link>\r\n          <router-link :to=\"`/child-development-form/${currentChild._id}`\" class=\"dashboard-link\">\r\n            Unos razvoja deteta\r\n          </router-link>\r\n          <router-link :to=\"`/child-development-list/${currentChild._id}`\" class=\"dashboard-link\">\r\n            Pregled razvoja\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Development Period Links -->\r\n      <div class=\"development-periods\">\r\n        <h4>Periodi razvoja:</h4>\r\n        <div class=\"period-links\">\r\n          <router-link :to=\"`/child-development/${currentChild._id}/0`\" class=\"period-link\">\r\n            0-6 meseci\r\n          </router-link>\r\n          <router-link :to=\"`/child-development/${currentChild._id}/6`\" class=\"period-link\">\r\n            6-12 meseci\r\n          </router-link>\r\n          <router-link :to=\"`/child-development/${currentChild._id}/12`\" class=\"period-link\">\r\n            12-18 meseci\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Overview Link -->\r\n      <div class=\"overview-link\">\r\n        <router-link :to=\"`/child-overview/${currentChild._id}`\" class=\"dashboard-link\">\r\n          Pregled sve Djece\r\n        </router-link>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- No Child Found -->\r\n    <div v-else class=\"error-message\">\r\n      <p>Nema podataka o detetu. Molimo proverite unos ili registrujte dete.</p>\r\n      <router-link to=\"/child-registration\" class=\"dashboard-link\">\r\n        Registruj Dete\r\n      </router-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useAuthStore } from '@/stores/authStore';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      currentChild: null,\r\n      errorMessage: '',\r\n    };\r\n  },\r\n  async created() {\r\n    await this.fetchCurrentChild();\r\n  },\r\n  methods: {\r\n    async fetchCurrentChild() {\r\n      try {\r\n        const authStore = useAuthStore();\r\n        const childId = authStore.childId || sessionStorage.getItem('childId');\r\n\r\n        if (!childId) {\r\n          console.warn(\"⚠️ No child ID found in store or session.\");\r\n          this.errorMessage = \"Nema registrovanog deteta. Molimo registrujte dete.\";\r\n          return;\r\n        }\r\n\r\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\r\n          headers: {\r\n            Authorization: `Bearer ${sessionStorage.getItem('token')}`\r\n          }\r\n        });\r\n\r\n        if (response.data) {\r\n          this.currentChild = response.data;\r\n        } else {\r\n          console.warn(\"⚠️ No child data received.\");\r\n          this.errorMessage = \"Podaci o detetu nisu pronađeni.\";\r\n        }\r\n      } catch (error) {\r\n        console.error(\"❌ Error fetching child data:\", error);\r\n        this.errorMessage = \"Greška prilikom učitavanja podataka. Pokušajte ponovo.\";\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.dashboard-container {\r\n  max-width: 1000px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.error-message {\r\n  background: #f8d7da;\r\n  color: #721c24;\r\n  padding: 15px;\r\n  border: 1px solid #f5c6cb;\r\n  border-radius: 5px;\r\n  text-align: center;\r\n}\r\n\r\n.dashboard-links {\r\n  margin-top: 20px;\r\n}\r\n\r\n.links-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\r\n  gap: 20px;\r\n  margin-top: 20px;\r\n}\r\n\r\n.dashboard-link {\r\n  display: block;\r\n  padding: 15px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  text-decoration: none;\r\n  border-radius: 8px;\r\n  text-align: center;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.dashboard-link:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.development-periods {\r\n  margin-top: 30px;\r\n}\r\n\r\n.period-links {\r\n  display: flex;\r\n  gap: 15px;\r\n  margin-top: 10px;\r\n}\r\n\r\n.period-link {\r\n  padding: 10px 15px;\r\n  background-color: #28a745;\r\n  color: white;\r\n  text-decoration: none;\r\n  border-radius: 6px;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.period-link:hover {\r\n  background-color: #218838;\r\n}\r\n\r\n.overview-link {\r\n  margin-top: 30px;\r\n}\r\n</style>\r\n"],"mappings":"AAoEA,SAASA,YAAW,QAAS,oBAAoB;AACjD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAChC,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,iBAAiBA,CAAA,EAAG;MACxB,IAAI;QACF,MAAME,SAAQ,GAAIT,YAAY,CAAC,CAAC;QAChC,MAAMU,OAAM,GAAID,SAAS,CAACC,OAAM,IAAKC,cAAc,CAACC,OAAO,CAAC,SAAS,CAAC;QAEtE,IAAI,CAACF,OAAO,EAAE;UACZG,OAAO,CAACC,IAAI,CAAC,2CAA2C,CAAC;UACzD,IAAI,CAACT,YAAW,GAAI,qDAAqD;UACzE;QACF;QAEA,MAAMU,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,sCAAsCN,OAAO,EAAE,EAAE;UAChFO,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUP,cAAc,CAACC,OAAO,CAAC,OAAO,CAAC;UAC1D;QACF,CAAC,CAAC;QAEF,IAAIG,QAAQ,CAACb,IAAI,EAAE;UACjB,IAAI,CAACE,YAAW,GAAIW,QAAQ,CAACb,IAAI;QACnC,OAAO;UACLW,OAAO,CAACC,IAAI,CAAC,4BAA4B,CAAC;UAC1C,IAAI,CAACT,YAAW,GAAI,iCAAiC;QACvD;MACF,EAAE,OAAOc,KAAK,EAAE;QACdN,OAAO,CAACM,KAAK,CAAC,8BAA8B,EAAEA,K