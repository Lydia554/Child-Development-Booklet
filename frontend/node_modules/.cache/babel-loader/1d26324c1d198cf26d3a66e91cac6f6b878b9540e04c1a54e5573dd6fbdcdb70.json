{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useAuthStore } from './stores/authStore';\nimport { useRouter } from 'vue-router';\nexport default {\n  setup() {\n    const authStore = useAuthStore();\n    const router = useRouter();\n    authStore.checkAuth(); // Provjerava da li je korisnik autentifikovan\n\n    const logout = () => {\n      authStore.logout(); // Poziva logout akciju\n      router.push('/login'); // Preusmerava na login\n    };\n    return {\n      authStore,\n      logout\n    }; // Dostupno u template-u\n  }\n};","map":{"version":3,"names":["useAuthStore","useRouter","setup","authStore","router","checkAuth","logout","push"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\App.vue"],"sourcesContent":["<template>\n  <div>\n    <nav>\n      <!-- Links only visible when the user is authenticated and not on the child-registration page -->\n      <router-link v-if=\"authStore.isAuthenticated && $route.path !== '/child-registration'\" :to=\"`/child-registration`\">Registracija deteta</router-link>\n      \n      <!-- Show child-specific links only if childId is available -->\n      <router-link v-if=\"authStore.isAuthenticated && authStore.childId\" :to=\"`/milestone-form/${authStore.childId}`\">Dodaj Milestone</router-link>\n      <router-link v-if=\"authStore.isAuthenticated && authStore.childId\" :to=\"`/milestone-list/${authStore.childId}`\">Pregled Milestone-a</router-link>\n      <router-link v-if=\"authStore.isAuthenticated && authStore.childId\" :to=\"`/child-development-form/${authStore.childId}`\">Unos razvoja deteta</router-link>\n      <router-link v-if=\"authStore.isAuthenticated && authStore.childId\" :to=\"`/child-development-list/${authStore.childId}`\">Pregled razvoja</router-link>\n      <router-link v-if=\"authStore.isAuthenticated && authStore.childId\" :to=\"`/child-overview/${authStore.childId}`\">Pregled sve Djece</router-link>\n\n      <router-link v-if=\"!authStore.isAuthenticated\" to=\"/login\">Prijavite se</router-link>\n      <router-link v-if=\"!authStore.isAuthenticated\" to=\"/\">Registrujte se</router-link>\n\n      <button v-if=\"authStore.isAuthenticated\" @click=\"logout\" class=\"logout-btn\">Odjavi se</button>\n    </nav>\n\n    <router-view />\n  </div>\n</template>\n\n<script>\nimport { useAuthStore } from './stores/authStore';  \nimport { useRouter } from 'vue-router';  \n\nexport default {\n  setup() {\n    const authStore = useAuthStore();\n    const router = useRouter();\n    authStore.checkAuth();  // Provjerava da li je korisnik autentifikovan\n\n    const logout = () => {\n      authStore.logout(); // Poziva logout akciju\n      router.push('/login'); // Preusmerava na login\n    };\n\n    return { authStore, logout };  // Dostupno u template-u\n  }\n};\n</script>\n\n\n<style scoped>\n\nnav {\n  margin-bottom: 20px;\n}\n\nnav a {\n  margin-right: 10px;\n  text-decoration: none;\n  color: #007bff;\n}\n\nnav a:hover {\n  text-decoration: underline;\n}\n\n.logout-btn {\n  padding: 8px 16px;\n  background-color: #690aa8;  \n  color: white;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  font-size: 16px;\n  transition: background-color 0.3s, transform 0.3s;\n  margin-left: 20px;\n}\n\n.logout-btn:hover {\n  background-color: #250891;  \n  transform: scale(1.05);  \n}\n\n.logout-btn:focus {\n  outline: none;\n}\n</style>\n"],"mappings":";AAwBA,SAASA,YAAW,QAAS,oBAAoB;AACjD,SAASC,SAAQ,QAAS,YAAY;AAEtC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIH,YAAY,CAAC,CAAC;IAChC,MAAMI,MAAK,GAAIH,SAAS,CAAC,CAAC;IAC1BE,SAAS,CAACE,SAAS,CAAC,CAAC,EAAG;;IAExB,MAAMC,MAAK,GAAIA,CAAA,KAAM;MACnBH,SAAS,CAACG,MAAM,CAAC,CAAC,EAAE;MACpBF,MAAM,CAACG,IAAI,CAAC,QAAQ,CAAC,EAAE;IACzB,CAAC;IAED,OAAO;MAAEJ,SAAS;MAAEG;IAAO,CAAC,EAAG;EACjC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}