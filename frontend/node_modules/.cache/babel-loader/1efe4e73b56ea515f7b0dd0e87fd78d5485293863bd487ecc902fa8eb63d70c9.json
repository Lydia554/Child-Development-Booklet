if=\"loading\">⏳ Učitavanje...</div>\r\n      <div v-else>\r\n        <div v-for=\"info in developmentData\" :key=\"info.ageRange\" class=\"info-section\">\r\n          <h3>{{ info.ageRange }} 🍼</h3>\r\n  \r\n          <div class=\"content-box milestones\">\r\n            <h4>🌟 Razvojne Prekretnice</h4>\r\n            <ul>\r\n              <li v-for=\"milestone in info.milestones\" :key=\"milestone.description\">\r\n                <strong>✅ {{ milestone.category }}:</strong> {{ milestone.description }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n  \r\n          <div class=\"content-box concerns\">\r\n            <h4>⚠️ Šta ako dete ne dostigne ove prekretnice?</h4>\r\n            <ul>\r\n              <li v-for=\"concern in info.concerns\" :key=\"concern.sign\">\r\n                <strong>❌ {{ concern.sign }}:</strong> {{ concern.recommendation }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n  \r\n          <div class=\"content-box advice\">\r\n            <h4>💡 Opšti Saveti</h4>\r\n            <p>{{ info.advice }}</p>\r\n          </div>\r\n  \r\n          <div class=\"content-box activities\">\r\n            <h4>🎲 Predložene Aktivnosti</h4>\r\n            <ul>\r\n              <li v-for=\"activity in info.exampleActivities\" :key=\"activity.title\">\r\n                <strong>🎈 {{ activity.title }}:</strong> {{ activity.description }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        loading: false, \r\n        developmentData: [\r\n          {\r\n            ageRange: \"0-6 meseci\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Podigne glavu dok leži na stomaku\" },\r\n              { category: \"Govor i Jezik\", description: \"Počinje da guče i ispušta zvuke\" },\r\n              { category: \"Kognitivni razvoj\", description: \"Prati predmete očima i prepoznaje lica\" },\r\n            ],\r\n            advice: \"Roditelji treba da provode puno vremena pričajući i igrajući se s bebom, uključujući kontakt koža-na-kožu.\",\r\n            exampleActivities: [\r\n              { title: \"Igra ogledalom\", description: \"Držite bebu ispred ogledala i pravite smešne izraze lica.\" },\r\n              { title: \"Zvukovi i reakcije\", description: \"Koristite zvečke i nežne zvukove kako biste pratili reakciju deteta.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"6-12 meseci\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Počinje da sedi bez podrške\" },\r\n              { category: \"Govor i Jezik\", description: \"Reaguje na svoje ime i ispušta različite glasove\" },\r\n              { category: \"Kognitivni razvoj\", description: \"Ispušta predmete namerno da vidi šta će se desiti\" },\r\n            ],\r\n            advice: \"Podstaknite bebu da istražuje okruženje koristeći sigurne igračke i igre sa roditeljima.\",\r\n            exampleActivities: [\r\n              { title: \"Skrivanje predmeta\", description: \"Sakrijte igračku ispod marame i pustite bebu da je pronađe.\" },\r\n              { title: \"Muzičke igre\", description: \"Igrajte muziku i pljeskajte kako bi dete pratilo ritam.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"12-18 meseci\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Hoda bez pomoći\" },\r\n              { category: \"Govor i Jezik\", description: \"Izgovara jednostavne reči poput 'mama' i 'tata'\" },\r\n            ],\r\n            advice: \"Pričajte s detetom stalno i čitajte mu knjige sa slikama.\",\r\n            exampleActivities: [\r\n              { title: \"Čitanje slikovnica\", description: \"Koristite knjige sa velikim slikama i pokazujte predmete.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"18-24 meseca\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Penje se uz stepenice\" },\r\n              { category: \"Govor i Jezik\", description: \"Kombinuje dve reči za jednostavne rečenice\" },\r\n            ],\r\n            advice: \"Podstaknite dete na samostalnu igru i istraživanje.\",\r\n            exampleActivities: [\r\n              { title: \"Igranje sa lutkama\", description: \"Podstičite dete da priča i igra se sa omiljenim igračkama.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"24-30 meseci\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Trči bez pada\" },\r\n              { category: \"Govor i Jezik\", description: \"Koristi rečenice od 3 reči\" },\r\n            ],\r\n            advice: \"Podstaknite dete na igranje u prirodi i razvijanje govora kroz priču.\",\r\n            exampleActivities: [\r\n              { title: \"Razgovor u igri\", description: \"Podstičite dete na priču dok se igra igračkama.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"30-36 meseci\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Koristi makaze za papir\" },\r\n              { category: \"Govor i Jezik\", description: \"Postavlja jednostavna pitanja\" },\r\n            ],\r\n            advice: \"Omogućite detetu kreativne aktivnosti kao što su crtanje i sečenje papira.\",\r\n            exampleActivities: [\r\n              { title: \"Seckanje papira\", description: \"Dajte detetu bezbedne makaze za papir.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"36-42 meseca\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Samostalno se oblači\" },\r\n              { category: \"Govor i Jezik\", description: \"Priča kraće priče\" },\r\n            ],\r\n            advice: \"Podstaknite dete na samostalnost i pričanje priča.\",\r\n            exampleActivities: [\r\n              { title: \"Oblačenje lutaka\", description: \"Neka dete oblači lutke i uči o oblačenju.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"42-48 meseci\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Crta jednostavne oblike\" },\r\n              { category: \"Govor i Jezik\", description: \"Prepoznaje i imenuje boje\" },\r\n            ],\r\n            advice: \"Igrajte igre boja i oblika sa detetom.\",\r\n            exampleActivities: [\r\n              { title: \"Crtanje krugova\", description: \"Neka dete crta jednostavne oblike bojicama.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"48-54 meseca\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Samostalno crta jednostavne figure\" },\r\n              { category: \"Govor i Jezik\", description: \"Razlikuje osnovne oblike i brojeve\" },\r\n            ],\r\n            advice: \"Podstičite dete na kreativno izražavanje kroz crtanje i igranje bojama.\",\r\n            exampleActivities: [\r\n              { title: \"Crtanje figura\", description: \"Neka dete crta ljude i životinje po svom izboru.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"54-60 meseci\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Vozi tricikl\" },\r\n              { category: \"Govor i Jezik\", description: \"Priča jednostavne priče sa početkom, sredinom i krajem\" },\r\n            ],\r\n            advice: \"Podstičite dete na fizičke aktivnosti i pričanje priča.\",\r\n            exampleActivities: [\r\n              { title: \"Igra sa triciklom\", description: \"Podstaknite dete na vožnju tricikla po parku.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"60-66 meseci\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Preskače konopac\" },\r\n              { category: \"Govor i Jezik\", description: \"Razume osnovne koncepte vremena (juče, danas, sutra)\" },\r\n            ],\r\n            advice: \"Podstičite dete na igre koje razvijaju koordinaciju i vremenske koncepte.\",\r\n            exampleActivities: [\r\n              { title: \"Igra preskakanja\", description: \"Koristite konopac za vežbanje preskakanja.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"66-72 meseca\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Hvata i baca loptu sa preciznošću\" },\r\n              { category: \"Govor i Jezik\", description: \"Koristi složenije rečenice i razume više reči\" },\r\n            ],\r\n            advice: \"Podstičite dete na timske igre i komunikaciju.\",\r\n            exampleActivities: [\r\n              { title: \"Igre loptom\", description: \"Igrajte igre dodavanja lopte kako biste poboljšali koordinaciju.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"72-78 meseci\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Vešto koristi makaze i olovku\" },\r\n              { category: \"Govor i Jezik\", description: \"Razume osnovne koncepte pisanja i čitanja\" },\r\n            ],\r\n            advice: \"Pripremite dete za školu kroz igre učenja i pisanja.\",\r\n            exampleActivities: [\r\n              { title: \"Pisanje slova\", description: \"Vežbajte zajedno osnovne oblike slova na papiru.\" },\r\n            ],\r\n          }\r\n        ]\r\n      };\r\n    }\r\n  };\r\n</script>\r\n\r\n\r\n  \r\n  <style lang=\"scss\" scoped>\r\n  .development-info {\r\n    max-width: 900px;\r\n    margin: 20px auto;\r\n    text-align: center;\r\n    font-family: \"Poppins\", sans-serif;\r\n    background-color: #f8f9fa;\r\n    padding: 25px;\