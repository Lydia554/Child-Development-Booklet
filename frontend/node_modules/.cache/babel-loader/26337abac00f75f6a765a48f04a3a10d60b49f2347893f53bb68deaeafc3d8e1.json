n  data() {\r\n    return {\r\n      child: {},\r\n      developmentData: [],\r\n      loading: true,\r\n      errorMessage: \"\",\r\n    };\r\n  },\r\n  async created() {\r\n    await this.fetchChildData();\r\n    await this.fetchDevelopmentData();\r\n  },\r\n  methods: {\r\n    async fetchChildData() {\r\n      try {\r\n        const response = await axios.get(`http://localhost:5000/api/children/${this.childId}`);\r\n        this.child = response.data;\r\n      } catch (error) {\r\n        console.error(\"Greška pri dobijanju podataka o detetu:\", error);\r\n        this.errorMessage = \"Nije moguće učitati podatke o detetu.\";\r\n      }\r\n    },\r\n    async fetchDevelopmentData() {\r\n      try {\r\n        const response = await axios.get(`http://localhost:5000/api/child-development/${this.childId}`);\r\n        this.developmentData = response.data;\r\n      } catch (error) {\r\n        console.error(\"Greška pri dobijanju podataka o razvoju:\", error);\r\n        this.errorMessage = \"Nije moguće učitati razvojne podatke.\";\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n    downloadPDF() {\r\n      const doc = new jsPDF();\r\n      doc.text(`Razvoj deteta: ${this.child.name}`, 10, 10);\r\n      \r\n      const tableColumn = [\"Period\", \"Fizički\", \"Emocionalni\", \"Intelektualni\", \"Socijalni\"];\r\n      const tableRows = [];\r\n\r\n      this.developmentData.forEach(period => {\r\n        const row = [\r\n          `${period.startAge}-${period.endAge} meseci`,\r\n          period.physicalDevelopment,\r\n          period.emotionalDevelopment,\r\n          period.intellectualDevelopment,\r\n          period.socialDevelopment,\r\n        ];\r\n        tableRows.push(row);\r\n      });\r\n\r\n      doc.autoTable(tableColumn, tableRows, { startY: 20 });\r\n      doc.save(`Razvoj_${this.child.name}.pdf`);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.overview-container {\r\n  max-width: 900px;\r\n  margin: 0 a