{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  props: ['childId'],\n  data() {\n    return {\n      milestones: []\n    };\n  },\n  created() {\n    // Store childId in localStorage\n    if (this.childId) {\n      localStorage.setItem('childId', this.childId);\n    }\n    const storedId = localStorage.getItem('childId');\n    if (!this.childId && !storedId) {\n      alert('Molimo registrujte dete pre nastavka.');\n      this.$router.push('/child-registration');\n      return;\n    }\n    this.fetchMilestones();\n  },\n  methods: {\n    async fetchMilestones() {\n      const id = this.childId || localStorage.getItem('childId');\n      if (!id) {\n        console.error('Child ID is missing');\n        return;\n      }\n      try {\n        const token = localStorage.getItem('token');\n        if (!token) {\n          console.error('User not authenticated');\n          return;\n        }\n\n        // Make an API call to fetch milestones based on childId\n        const response = await axios.get(`http://localhost:5000/api/milestones/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.milestones = response.data; // Assuming response contains milestones directly\n      } catch (error) {\n        console.error('Error fetching milestones:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","props","data","milestones","created","childId","localStorage","setItem","storedId","getItem","alert","$router","push","fetchMilestones","methods","id","console","error","token","response","get","headers","Authorization"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\MilestoneList.vue"],"sourcesContent":["<template>\r\n  <div class=\"development-container\">\r\n    <h2>Pregled Milestone-a</h2>\r\n    <div v-if=\"milestones.length\">\r\n      <div v-for=\"(milestone, index) in milestones\" :key=\"index\" class=\"development-item\">\r\n        <h3>{{ milestone.milestoneType }} - {{ milestone.milestoneDate }}</h3>\r\n        <p><strong>Napredak:</strong> {{ milestone.progress }}</p>\r\n        <p><strong>Opisi:</strong> {{ milestone.notes }}</p>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else>\r\n      <p class=\"no-data\">Nema podataka o Milestone-ima za ovo dete.</p>\r\n    </div>\r\n\r\n    <router-link :to=\"`/milestone-form/${childId}`\" class=\"add-milestone-button\">Dodaj Milestone</router-link>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: ['childId'],\r\n  data() {\r\n    return {\r\n      milestones: [],\r\n    };\r\n  },\r\n  created() {\r\n    // Store childId in localStorage\r\n    if (this.childId) {\r\n      localStorage.setItem('childId', this.childId);\r\n    }\r\n    const storedId = localStorage.getItem('childId');\r\n    if (!this.childId && !storedId) {\r\n      alert('Molimo registrujte dete pre nastavka.');\r\n      this.$router.push('/child-registration');\r\n      return;\r\n    }\r\n    this.fetchMilestones();\r\n  },\r\n  methods: {\r\n    async fetchMilestones() {\r\n      const id = this.childId || localStorage.getItem('childId');\r\n      if (!id) {\r\n        console.error('Child ID is missing');\r\n        return;\r\n      }\r\n      try {\r\n        const token = localStorage.getItem('token');\r\n        if (!token) {\r\n          console.error('User not authenticated');\r\n          return;\r\n        }\r\n\r\n        // Make an API call to fetch milestones based on childId\r\n        const response = await axios.get(`http://localhost:5000/api/milestones/${id}`, {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n        this.milestones = response.data; // Assuming response contains milestones directly\r\n      } catch (error) {\r\n        console.error('Error fetching milestones:', error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n\r\n<style scoped lang=\"scss\">\r\n.milestone-container {\r\n  max-width: 700px;\r\n  margin: 0 auto;\r\n  padding: 40px;\r\n  background-color: #f7f7f7;\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  font-family: 'Arial', sans-serif;\r\n  text-align: center;\r\n}\r\n\r\nh2 {\r\n  font-size: 28px;\r\n  color: #333;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.milestone-list {\r\n  list-style: none;\r\n  padding: 0;\r\n  margin-bottom: 30px;\r\n  text-align: left;\r\n}\r\n\r\n.milestone-item {\r\n  background-color: #fff;\r\n  padding: 15px;\r\n  margin-bottom: 15px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.milestone-item strong {\r\n  font-size: 18px;\r\n  color: #333;\r\n}\r\n\r\nbutton {\r\n  padding: 8px 12px;\r\n  border-radius: 6px;\r\n  border: none;\r\n  font-size: 14px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.edit-btn {\r\n  background-color: #007bff;\r\n  color: white;\r\n  margin-left: 10px;\r\n}\r\n\r\n.edit-btn:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.delete-btn {\r\n  background-color: #dc3545;\r\n  color: white;\r\n}\r\n\r\n.delete-btn:hover {\r\n  background-color: #c82333;\r\n}\r\n\r\n.milestone-form {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 20px;\r\n  justify-content: center;\r\n  margin-top: 20px;\r\n}\r\n\r\ninput,\r\ntextarea {\r\n  padding: 12px;\r\n  border-radius: 6px;\r\n  border: 1px solid #ddd;\r\n  font-size: 14px;\r\n  width: 100%;\r\n  transition: border-color 0.3s;\r\n}\r\n\r\ninput:focus,\r\ntextarea:focus {\r\n  border-color: #007bff;\r\n  outline: none;\r\n}\r\n\r\ntextarea {\r\n  resize: vertical;\r\n  min-height: 100px;\r\n}\r\n\r\n.form-buttons {\r\n  display: flex;\r\n  justify-content: center;\r\n  gap: 15px;\r\n}\r\n\r\n.submit-btn {\r\n  background-color: #007bff;\r\n  color: white;\r\n  paddi