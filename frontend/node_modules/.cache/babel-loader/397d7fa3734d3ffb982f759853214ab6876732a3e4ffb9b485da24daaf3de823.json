{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      formData: {\n        startAge: null,\n        endAge: null,\n        physicalDevelopment: '',\n        emotionalDevelopment: '',\n        intellectualDevelopment: '',\n        socialDevelopment: ''\n      },\n      error: null,\n      loading: false\n    };\n  },\n  async created() {\n    try {\n      const startAge = parseInt(this.$route.query.startAge);\n      const endAge = parseInt(this.$route.query.endAge);\n      this.formData.startAge = startAge;\n      this.formData.endAge = endAge;\n      console.log('Form initialized with period:', this.formData);\n      await this.loadDevelopmentData();\n    } catch (error) {\n      console.error('Error in created:', error);\n      this.error = 'Greška pri učitavanju podataka.';\n    }\n  },\n  methods: {\n    async loadDevelopmentData() {\n      try {\n        const token = sessionStorage.getItem('token');\n        const childId = this.$route.params.childId;\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (Array.isArray(response.data)) {\n          const periodData = response.data.find(data => parseInt(data.startAge) === this.formData.startAge && parseInt(data.endAge) === this.formData.endAge);\n          if (periodData) {\n            this.formData = {\n              ...periodData\n            }; // ✅ Ensure correct data is loaded\n          }\n        }\n      } catch (error) {\n        console.error('Error loading development data:', error);\n        this.error = 'Greška pri učitavanju postojećih podataka.';\n      }\n    },\n    async saveDevelopment() {\n      try {\n        this.loading = true;\n        const childId = this.$route.params.childId;\n        await axios.post(`http://localhost:5000/api/child-development/${childId}`, this.formData, {\n          headers: {\n            'Authorization': `Bearer ${sessionStorage.getItem('token')}`,\n            'Content-Type': 'application/json'\n          }\n        });\n        this.$router.push(`/child-development-list/${childId}`);\n      } catch (error) {\n        console.error('Error saving development:', error);\n        this.error = 'Greška pri čuvanju podataka.';\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","formData","startAge","endAge","physicalDevelopment","emotionalDevelopment","intellectualDevelopment","socialDevelopment","error","loading","created","parseInt","$route","query","console","log","loadDevelopmentData","methods","token","sessionStorage","getItem","childId","params","response","get","headers","Authorization","Array","isArray","periodData","find","saveDevelopment","post","$router","push"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"development-form\">\r\n    <h2>Unos Razvoja Deteta</h2>\r\n    <h3>Period: {{ formData.startAge }}-{{ formData.endAge }} meseci</h3>\r\n\r\n    <div v-if=\"error\" class=\"error-message\">\r\n      {{ error }}\r\n    </div>\r\n\r\n    <form @submit.prevent=\"saveDevelopment\">\r\n      <div class=\"form-group\">\r\n        <label for=\"physicalDevelopment\">Fizički razvoj:</label>\r\n        <textarea\r\n          id=\"physicalDevelopment\"\r\n          v-model=\"formData.physicalDevelopment\"\r\n          required\r\n        ></textarea>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"emotionalDevelopment\">Emocionalni razvoj:</label>\r\n        <textarea\r\n          id=\"emotionalDevelopment\"\r\n          v-model=\"formData.emotionalDevelopment\"\r\n          required\r\n        ></textarea>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"intellectualDevelopment\">Intelektualni razvoj:</label>\r\n        <textarea\r\n          id=\"intellectualDevelopment\"\r\n          v-model=\"formData.intellectualDevelopment\"\r\n          required\r\n        ></textarea>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"socialDevelopment\">Socijalni razvoj:</label>\r\n        <textarea\r\n          id=\"socialDevelopment\"\r\n          v-model=\"formData.socialDevelopment\"\r\n          required\r\n        ></textarea>\r\n      </div>\r\n\r\n      <button type=\"submit\" :disabled=\"loading\">\r\n        {{ loading ? 'Čuvanje...' : 'Sačuvaj' }}\r\n      </button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      formData: {\r\n        startAge: null,\r\n        endAge: null,\r\n        physicalDevelopment: '',\r\n        emotionalDevelopment: '',\r\n        intellectualDevelopment: '',\r\n        socialDevelopment: ''\r\n      },\r\n      error: null,\r\n      loading: false\r\n    };\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      const startAge = parseInt(this.$route.query.startAge);\r\n      const endAge = parseInt(this.$route.query.endAge);\r\n      this.formData.startAge = startAge;\r\n      this.formData.endAge = endAge;\r\n      \r\n      console.log('Form initialized with period:', this.formData);\r\n      await this.loadDevelopmentData();\r\n    } catch (error) {\r\n      console.error('Error in created:', error);\r\n      this.error = 'Greška pri učitavanju podataka.';\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    async loadDevelopmentData() {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const childId = this.$route.params.childId;\r\n\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` }\r\n          }\r\n        );\r\n\r\n        if (Array.isArray(response.data)) {\r\n          const periodData = response.data.find(data => \r\n            parseInt(data.startAge) === this.formData.startAge && \r\n            parseInt(data.endAge) === this.formData.endAge\r\n          );\r\n\r\n          if (periodData) {\r\n            this.formData = { ...periodData }; // ✅ Ensure correct data is loaded\r\n          }\r\n        }\r\n      } catch (error) {\r\n        console.error('Error loading development data:', error);\r\n        this.error = 'Greška pri učitavanju postojećih podataka.';\r\n      }\r\n    },\r\n\r\n    async saveDevelopment() {\r\n      try {\r\n        this.loading = true;\r\n        const childId = this.$route.params.childId;\r\n\r\n        await axios.post(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          this.formData,\r\n          {\r\n            headers: { \r\n              'Authorization': `Bearer ${sessionStorage.getItem('token')}`,\r\n              'Content-Type': 'application/json'\r\n            }\r\n          }\r\n        );\r\n\r\n        this.$router.push(`/child-development-list/${childId}`);\r\n      } catch (error) {\r\n        console.error('Error saving development:', error);\r\n        this.error = 'Greška pri čuvanju podataka.';\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.development-form {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 20px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n  font-weight: bold;\r\n}\r\n\r\ntextarea {\r\n  width: 100%;\r\n  min-height: 100px;\r\n  padding: 10px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n}\r\n\r\nbutton {\r\n  padding: 10px 20px;\r\n  background-color: #4CAF50;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:disabled {\r\n  background-color: #cccccc;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.error-message {\r\n  color: red;\r\n  margin-bottom: 20px;\r\n  padding: 10px;\r\n  background-color: #ffebee;\r\n  border-radius: 4px;\r\n}\r\n</style>\r\n"],"mappings":";;;AAsDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,QAAQ,EAAE,IAAI;QACdC,MAAM,EAAE,IAAI;QACZC,mBAAmB,EAAE,EAAE;QACvBC,oBAAoB,EAAE,EAAE;QACxBC,uBAAuB,EAAE,EAAE;QAC3BC,iBAAiB,EAAE;MACrB,CAAC;MACDC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EAED,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMR,QAAO,GAAIS,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACC,KAAK,CAACX,QAAQ,CAAC;MACrD,MAAMC,MAAK,GAAIQ,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACC,KAAK,CAACV,MAAM,CAAC;MACjD,IAAI,CAACF,QAAQ,CAACC,QAAO,GAAIA,QAAQ;MACjC,IAAI,CAACD,QAAQ,CAACE,MAAK,GAAIA,MAAM;MAE7BW,OAAO,CAACC,GAAG,CAAC,+BAA+B,EAAE,IAAI,CAACd,QAAQ,CAAC;MAC3D,MAAM,IAAI,CAACe,mBAAmB,CAAC,CAAC;IAClC,EAAE,OAAOR,KAAK,EAAE;MACdM,OAAO,CAACN,KAAK,CAAC,mBAAmB,EAAEA,KAAK,CAAC;MACzC,IAAI,CAACA,KAAI,GAAI,iCAAiC;IAChD;EACF,CAAC;EAEDS,OAAO,EAAE;IACP,MAAMD,mBAAmBA,CAAA,EAAG;MAC1B,IAAI;QACF,MAAME,KAAI,GAAIC,cAAc,CAACC,OAAO,CAAC,OAAO,CAAC;QAC7C,MAAMC,OAAM,GAAI,IAAI,CAACT,MAAM,CAACU,MAAM,CAACD,OAAO;QAE1C,MAAME,QAAO,GAAI,MAAMxB,KAAK,CAACyB,GAAG,CAC9B,+CAA+CH,OAAO,EAAE,EACxD;UACEI,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUR,KAAK;UAAG;QAC9C,CACF,CAAC;QAED,IAAIS,KAAK,CAACC,OAAO,CAACL,QAAQ,CAACvB,IAAI,CAAC,EAAE;UAChC,MAAM6B,UAAS,GAAIN,QAAQ,CAACvB,IAAI,CAAC8B,IAAI,CAAC9B,IAAG,IACvCW,QAAQ,CAACX,IAAI,CAACE,QAAQ,MAAM,IAAI,CAACD,QAAQ,CAACC,QAAO,IACjDS,QAAQ,CAACX,IAAI,CAACG,MAAM,MAAM,IAAI,CAACF,QAAQ,CAACE,MAC1C,CAAC;UAED,IAAI0B,UAAU,EAAE;YACd,IAAI,CAAC5B,QAAO,GAAI;cAAE,GAAG4B;YAAW,CAAC,EAAE;UACrC;QACF;MACF,EAAE,OAAOrB,KAAK,EAAE;QACdM,OAAO,CAACN,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;QACvD,IAAI,CAACA,KAAI,GAAI,4CAA4C;MAC3D;IACF,CAAC;IAED,MAAMuB,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,IAAI,CAACtB