    :class=\"getPeriodClass(period)\"\r\n                @click=\"togglePeriod(period)\">\r\n          {{ formatPeriod(period.start, period.end) }}\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Display records under selected period -->\r\n    <div v-if=\"selectedPeriod\" class=\"period-records\">\r\n      <h3>Razvoj za period: {{ formatPeriod(selectedPeriod.start, selectedPeriod.end) }}</h3>\r\n\r\n\r\n      <!-- Display Milestones -->\r\n      <div v-if=\"milestones.length\" class=\"milestones-section\">\r\n        <h4>Milestones:</h4>\r\n        <ul>\r\n          <li v-for=\"milestone in milestones\" :key=\"milestone._id\">\r\n            <strong>{{ milestone.milestoneType }}</strong> - {{ formatDate(milestone.milestoneDate) }}: {{ milestone.progress }}\r\n          </li>\r\n        </ul>\r\n      </div>\r\n\r\n      <!-- Display Development Data -->\r\n      <div v-if=\"developmentData.length\" class=\"development-section\">\r\n        <h4>Development Data:</h4>\r\n        <ul>\r\n          <li v-for=\"period in developmentData.periods\" :key=\"period._id\">\r\n            <strong>Period {{ period.startAge }} - {{ period.endAge }} months:</strong>\r\n            <ul>\r\n              <li><strong>Physical:</strong> {{ period.physicalDevelopment }}</li>\r\n              <li><strong>Emotional:</strong> {{ period.emotionalDevelopment }}</li>\r\n              <li><strong>Intellectual:</strong> {{ period.intellectualDevelopment }}</li>\r\n              <li><strong>Social:</strong> {{ period.socialDevelopment }}</li>\r\n            </ul>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n\r\n      <div v-else class=\"no-records\">\r\n        <p>Nema unetih podataka za ovaj period.</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { useAuthStore } from \"../stores/auth\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      childName: null,\r\n      childAge: null,\r\n      availablePeriods: [],\r\n      developmentData: [],\r\n      milestoneData: [],\r\n      selectedPeriod: null,\r\n      currentChild: null,\r\n      milestones: [],\r\n      loading: true,\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    currentPeriodDevelopment() {\r\n      return this.selectedPeriod\r\n        ? this.developmentData.find(\r\n            (d) =>\r\n              d.startAge === this.selectedPeriod.start &&\r\n              d.endAge === this.selectedPeriod.end\r\n          )\r\n        : null;\r\n    },\r\n\r\n    currentPeriodMilestones() {\r\n      return this.selectedPeriod\r\n        ? this.milestoneData.filter(\r\n            (m) =>\r\n              m.age >= this.selectedPeriod.start &&\r\n              m.age < this.selectedPeriod.end\r\n          )\r\n        : [];\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    async fetchChildData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/children/${childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          }\r\n        );\r\n\r\n        if (response.data) {\r\n          this.childName = response.data.name;\r\n          this.childAge = this.calculateAgeInMonths(\r\n            new Date(response.data.birthDate)\r\n          );\r\n        }\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\r\n      }\r\n    },\r\n\r\n    async fetchMilestones(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/milestones/${childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          }\r\n        );\r\n        this.milestoneData = response.data;\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri učitavanju ključnih momenata:\", error);\r\n      }\r\n    },\r\n\r\n    async fetchDevelopmentData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          }\r\n        );\r\n        this.developmentData = response.data;\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri učitavanju podataka o razvoju:\", error);\r\n      }\r\n    },\r\n\r\n    calculateAgeInMonths(birthDate) {\r\n      const birth = new Date(birthDate);\r\n      const now = new Date();\r\n      return (\r\n        (now.getFullYear() - birth.getFullYear()) * 12 +\r\n        (now.getMonth() - birth.getMonth())\r\n      );\r\n    },\r\n\r\n    togglePeriod(period) {\r\n      this.selectedPeriod =\r\n        this.selectedPeriod?.start === period.start ? null : period;\r\n    },\r\n\r\n    formatPeriod(start, end) {\r\n      return `${start}-${end} meseci`;\r\n    },\r\n\r\n    getPeriodClass(period) {\r\n      return {\r\n        \"period-button\": true,\r\n        selected: this.selectedPeriod && this.selectedPeriod.start === period.start,\r\n      };\r\n    },\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      const childId = this.$route.params.childId;\r\n      if (childId) {\r\n        await this.fetchChildData(childId);\r\n        await this.fetchMilestones(childId);\r\n        await this.fetchDevelopmentData(childId);\r\n\r\n        // Generate available periods\r\n        for (let i = 0; i <= 72; i += 6) {\r\n          this.availablePeriods.push({ start: i, end: i + 6 });\r\n        }\r\n      }\r\n    } catch (error) {\r\n      console.error(\"❌ Greška u created():\", error);\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.development-container {\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.period-selector h3 {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.periods-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  gap: 10px;\r\n}\r\n\r\n.period-button {\r\n  padding: 12px 18px;\r\n  font-size: 18px;\r\n  border: none;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n  background-color: #ddd;\r\n  transition: 0.3s;\r\n}\r\n\r\n.period-button.selected {\r\n  background-color: #4caf50;\r\n  color: white;\r\n}\r\n\r\n.period-button:hover {\r\n  transform: scale(1.05);\r\n}\r\n\r\n.period-records {\r\n  margin-top: 20px;\r\n  padding: 20px;\r\n  background: #f8f9fa;\r\n  border-radius: 8px;\r\n  text-align: left;\r\n  max-width: 600px;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n\r\n.development-section,\r\n.milestones-section {\r\n  margin-top: 20px;\r\n}\r\n\r\n.milestone-item {\r\n  background: white;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  margin-bottom: 10px;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAuB;;EAI3BA,KAAK,EAAC;AAAiB;;EAErBA,KAAK,EAAC;AAAc;mBAP/B;;EAAAC,GAAA;EAkB+BD,KAAK,EAAC;;;EAlBrCC,GAAA;EAuBoCD,KAAK,EAAC;;;EAvB1CC,GAAA;EAiCyCD,KAAK,EAAC;;;EAjC/CC,GAAA;EAgDkBD,KAAK,EAAC;;;uBA/CtBE,mBAAA,CAmDM,OAnDNC,UAmDM,GAlDJC,mBAAA,CAAgE,YAA5D,sBAAoB,GAAAC,gBAAA,CAAGC,KAAA,CAAAC,SAAS,sCAEpCC,mBAAA,qBAAwB,EACxBJ,mBAAA,CAUM,OAVNK,UAUM,G,0BATJL,mBAAA,CAA0B,YAAtB,mBAAiB,sBACrBA,mBAAA,CAOM,OAPNM,UAOM,I,kBANJR,mBAAA,CAKSS,SAAA,QAbjBC,WAAA,CAQiCN,KAAA,CAAAO,gBAAgB,EAA1BC,MAAM;yBAArBZ,mBAAA,CAKS;MAJAD,GAAG,KAAKa,MAAM,CAACC,KAAK,IAAID,MAAM,CAACE,GAAG;MAClChB,KAAK,EAVtBiB,eAAA,CAUwBC,QAAA,CAAAC,cAAc,CAACL,MAAM;MAC5BM,OAAK,EAAAC,MAAA,IAAEH,QAAA,CAAAI,YAAY,CAACR,MAAM;wBAC9BI,QAAA,CAAAK,YAAY,CAACT,MAAM,CAACC,KAAK,EAAED,MAAM,CAACE,GAAG,iCAZlDQ,UAAA;sCAiBIhB,mBAAA,2CAA8C,EACnCF,KAAA,CAAAmB,cAAc,I,cAAzBvB,mBAAA,CAiCM,OAjCNwB,UAiCM,GAhCJtB,mBAAA,CAAuF,YAAnF,oBAAkB,GAAAC,gBAAA,CAAGa,QAAA,CAAAK,YAAY,CAACjB,KAAA,CAAAmB,cAAc,CAACV,KAAK,EAAET,KAAA,CAAAmB,cAAc,CAACT,GAAG,mBAG9ER,mBAAA,wBAA2B,EAChBF,KAAA,CAAAqB,UAAU,CAACC,MAAM,I,cAA5B1B,mBAAA,CAOM,OAPN2B,UAOM,G,0BANJzB,mBAAA,C