{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      childName: null,\n      childAge: null,\n      availablePeriods: [],\n      developmentData: [],\n      milestones: [],\n      selectedPeriod: null\n    };\n  },\n  computed: {\n    currentPeriodDevelopment() {\n      return this.selectedPeriod ? this.developmentData.find(d => d.startAge === this.selectedPeriod.start && d.endAge === this.selectedPeriod.end) : null;\n    },\n    currentPeriodMilestones() {\n      return this.selectedPeriod ? this.milestoneData.filter(m => m.age >= this.selectedPeriod.start && m.age < this.selectedPeriod.end) : [];\n    }\n  },\n  methods: {\n    async fetchChildData(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (response.data) {\n          this.childName = response.data.name;\n          this.childAge = this.calculateAgeInMonths(new Date(response.data.birthDate));\n        }\n      } catch (error) {\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\n      }\n    },\n    async fetchMilestones(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/milestones/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.milestoneData = response.data;\n      } catch (error) {\n        console.error(\"❌ Greška pri učitavanju ključnih momenata:\", error);\n      }\n    },\n    async fetchDevelopmentData(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.developmentData = response.data;\n      } catch (error) {\n        console.error(\"❌ Greška pri učitavanju podataka o razvoju:\", error);\n      }\n    },\n    calculateAgeInMonths(birthDate) {\n      const birth = new Date(birthDate);\n      const now = new Date();\n      return (now.getFullYear() - birth.getFullYear()) * 12 + (now.getMonth() - birth.getMonth());\n    },\n    togglePeriod(period) {\n      this.selectedPeriod = this.selectedPeriod?.start === period.start ? null : period;\n    },\n    formatPeriod(start, end) {\n      return `${start}-${end} meseci`;\n    },\n    getPeriodClass(period) {\n      return {\n        \"period-button\": true,\n        selected: this.selectedPeriod && this.selectedPeriod.start === period.start\n      };\n    }\n  },\n  async created() {\n    try {\n      const childId = this.$route.params.childId;\n      if (childId) {\n        await this.fetchChildData(childId);\n        await this.fetchMilestones(childId);\n        await this.fetchDevelopmentData(childId);\n\n        // Generate available periods\n        for (let i = 0; i <= 72; i += 6) {\n          this.availablePeriods.push({\n            start: i,\n            end: i + 6\n          });\n        }\n      }\n    } catch (error) {\n      console.error(\"❌ Greška u created():\", error);\n    }\n  }\n};","map":{"version":3,"names":["axios","data","childName","childAge","availablePeriods","developmentData","milestones","selectedPeriod","computed","currentPeriodDevelopment","find","d","startAge","start","endAge","end","currentPeriodMilestones","milestoneData","filter","m","age","methods","fetchChildData","childId","token","sessionStorage","getItem","response","get","headers","Authorization","name","calculateAgeInMonths","Date","birthDate","error","console","fetchMilestones","fetchDevelopmentData","birth","now","getFullYear","getMonth","togglePeriod","period","formatPeriod","getPeriodClass","selected","created","$route","params","i","push"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentPeriod.vue"],"sourcesContent":["<template>\r\n  <div class=\"development-container\">\r\n    <h2>Praćenje Razvoja za {{ childName || \"Nepoznato dete\" }}</h2>\r\n\r\n    <!-- Period Selector -->\r\n    <div class=\"period-selector\">\r\n      <h3>Izaberite period:</h3>\r\n      <div class=\"periods-list\">\r\n        <button v-for=\"period in availablePeriods\"\r\n                :key=\"`${period.start}-${period.end}`\"\r\n                :class=\"getPeriodClass(period)\"\r\n                @click=\"togglePeriod(period)\">\r\n          {{ formatPeriod(period.start, period.end) }}\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Display records under selected period -->\r\n    <div v-if=\"selectedPeriod\" class=\"period-records\">\r\n      <h3>Razvoj za period: {{ formatPeriod(selectedPeriod.start, selectedPeriod.end) }}</h3>\r\n\r\n      <!-- Development Data -->\r\n      <div v-if=\"currentPeriodDevelopment\" class=\"development-section\">\r\n        <h4>Razvoj deteta</h4>\r\n        <ul>\r\n          <li><strong>Fizički:</strong> {{ currentPeriodDevelopment.physicalDevelopment || \"Nema podataka\" }}</li>\r\n          <li><strong>Emocionalni:</strong> {{ currentPeriodDevelopment.emotionalDevelopment || \"Nema podataka\" }}</li>\r\n          <li><strong>Intelektualni:</strong> {{ currentPeriodDevelopment.intellectualDevelopment || \"Nema podataka\" }}</li>\r\n          <li><strong>Socijalni:</strong> {{ currentPeriodDevelopment.socialDevelopment || \"Nema podataka\" }}</li>\r\n        </ul>\r\n      </div>\r\n\r\n      <!-- Milestones Data -->\r\n      <div v-if=\"currentPeriodMilestones.length\" class=\"milestones-section\">\r\n        <h4>Prekretnice za ovaj period</h4>\r\n        <div class=\"milestone-list\">\r\n          <div v-for=\"milestone in currentPeriodMilestones\" :key=\"milestone._id\" class=\"milestone-item\">\r\n            <h3>{{ milestone.milestoneType }} - {{ formatDate(milestone.milestoneDate) }}</h3>\r\n            <p><strong>Napredak:</strong> {{ milestone.progress }}</p>\r\n            <p><strong>Napomene:</strong> {{ milestone.notes }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div v-else class=\"no-records\">\r\n        <p>Nema unetih podataka za ovaj period.</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      childName: null,\r\n      childAge: null,\r\n      availablePeriods: [],\r\n      developmentData: [],\r\n      milestones: [],\r\n      selectedPeriod: null,\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    currentPeriodDevelopment() {\r\n      return this.selectedPeriod\r\n        ? this.developmentData.find(\r\n            (d) =>\r\n              d.startAge === this.selectedPeriod.start &&\r\n              d.endAge === this.selectedPeriod.end\r\n          )\r\n        : null;\r\n    },\r\n\r\n    currentPeriodMilestones() {\r\n      return this.selectedPeriod\r\n        ? this.milestoneData.filter(\r\n            (m) =>\r\n              m.age >= this.selectedPeriod.start &&\r\n              m.age < this.selectedPeriod.end\r\n          )\r\n        : [];\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    async fetchChildData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/children/${childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          }\r\n        );\r\n\r\n        if (response.data) {\r\n          this.childName = response.data.name;\r\n          this.childAge = this.calculateAgeInMonths(\r\n            new Date(response.data.birthDate)\r\n          );\r\n        }\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\r\n      }\r\n    },\r\n\r\n    async fetchMilestones(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/milestones/${childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          }\r\n        );\r\n        this.milestoneData = response.data;\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri učitavanju ključnih momenata:\", error);\r\n      }\r\n    },\r\n\r\n    async fetchDevelopmentData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          }\r\n        );\r\n        this.developmentData = response.data;\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri učitavanju podataka o razvoju:\", error);\r\n      }\r\n    },\r\n\r\n    calculateAgeInMonths(birthDate) {\r\n      const birth = new Date(birthDate);\r\n      const now = new Date();\r\n      return (\r\n        (now.getFullYear() - birth.getFullYear()) * 12 +\r\n        (now.getMonth() - birth.getMonth())\r\n      );\r\n    },\r\n\r\n    togglePeriod(period) {\r\n      this.selectedPeriod =\r\n        this.selectedPeriod?.start === period.start ? null : period;\r\n    },\r\n\r\n    formatPeriod(start, end) {\r\n      return `${start}-${end} meseci`;\r\n    },\r\n\r\n    getPeriodClass(period) {\r\n      return {\r\n        \"period-button\": true,\r\n        selected: this.selectedPeriod && this.selectedPeriod.start === period.start,\r\n      };\r\n    },\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      const childId = this.$route.params.childId;\r\n      if (childId) {\r\n        await this.fetchChildData(childId);\r\n        await this.fetchMilestones(childId);\r\n        await this.fetchDevelopmentData(childId);\r\n\r\n        // Generate available periods\r\n        for (let i = 0; i <= 72; i += 6) {\r\n          this.availablePeriods.push({ start: i, end: i + 6 });\r\n        }\r\n      }\r\n    } catch (error) {\r\n      console.error(\"❌ Greška u created():\", error);\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.development-container {\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.period-selector h3 {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.periods-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  gap: 10px;\r\n}\r\n\r\n.period-button {\r\n  padding: 12px 18px;\r\n  font-size: 18px;\r\n  border: none;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n  background-color: #ddd;\r\n  transition: 0.3s;\r\n}\r\n\r\n.period-button.selected {\r\n  background-color: #4caf50;\r\n  color: white;\r\n}\r\n\r\n.period-button:hover {\r\n  transform: scale(1.05);\r\n}\r\n\r\n.period-records {\r\n  margin-top: 20px;\r\n  padding: 20px;\r\n  background: #f8f9fa;\r\n  border-radius: 8px;\r\n  text-align: left;\r\n  max-width: 600px;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n\r\n.development-section,\r\n.milestones-section {\r\n  margin-top: 20px;\r\n}\r\n\r\n.milestone-item {\r\n  background: white;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  margin-bottom: 10px;\r\n}\r\n</style>\r\n"],"mappings":";;;;AAoDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,QAAQ,EAAE,IAAI;MACdC,gBAAgB,EAAE,EAAE;MACpBC,eAAe,EAAE,EAAE;MACnBC,UAAU,EAAE,EAAE;MACdC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EAEDC,QAAQ,EAAE;IACRC,wBAAwBA,CAAA,EAAG;MACzB,OAAO,IAAI,CAACF,cAAa,GACrB,IAAI,CAACF,eAAe,CAACK,IAAI,CACtBC,CAAC,IACAA,CAAC,CAACC,QAAO,K