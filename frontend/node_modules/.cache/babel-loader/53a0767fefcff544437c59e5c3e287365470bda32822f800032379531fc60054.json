{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  props: ['childId', 'startAge'],\n  // make sure childId and startAge are passed correctly\n\n  data() {\n    return {\n      newDevelopment: {\n        childId: this.childId,\n        startAge: this.startAge,\n        // use the startAge to determine the period\n        endAge: parseInt(this.startAge) + 6,\n        // assuming each period is 6 months\n        physicalDevelopment: '',\n        emotionalDevelopment: '',\n        intellectualDevelopment: '',\n        socialDevelopment: ''\n      },\n      errorMessage: ''\n    };\n  },\n  methods: {\n    async submitDevelopmentForm() {\n      // Validate the required fields\n      if (!this.newDevelopment.physicalDevelopment || !this.newDevelopment.emotionalDevelopment || !this.newDevelopment.intellectualDevelopment || !this.newDevelopment.socialDevelopment) {\n        this.errorMessage = 'Sva polja su obavezna.';\n        return;\n      }\n\n      // Ensure childId is present\n      if (!this.newDevelopment.childId) {\n        console.error('childId je undefined');\n        this.errorMessage = 'Greška, childId nije prosleđen.';\n        return;\n      }\n\n      // Ensure the user is logged in\n      const token = localStorage.getItem('token');\n      if (!token) {\n        this.errorMessage = 'Morate biti ulogovani da biste dodali dete.';\n        return;\n      }\n\n      // Wrap the form data in the periods array\n      const dataToSend = {\n        childId: this.newDevelopment.childId,\n        periods: [{\n          startAge: this.newDevelopment.startAge,\n          endAge: this.newDevelopment.endAge,\n          physicalDevelopment: this.newDevelopment.physicalDevelopment,\n          emotionalDevelopment: this.newDevelopment.emotionalDevelopment,\n          intellectualDevelopment: this.newDevelopment.intellectualDevelopment,\n          socialDevelopment: this.newDevelopment.socialDevelopment\n        }]\n      };\n      try {\n        const response = await axios.post(`http://localhost:5000/api/child-development/${this.newDevelopment.childId}`, dataToSend, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        console.log('Podaci su uspešno poslati:', response);\n        this.$router.push(`/child-development-page/${this.newDevelopment.childId}/${this.newDevelopment.startAge}`);\n      } catch (error) {\n        console.error('Greška prilikom slanja podataka:', error);\n        this.errorMessage = 'Greška prilikom slanja podataka. Molimo pokušajte ponovo.';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","props","data","newDevelopment","childId","startAge","endAge","parseInt","physicalDevelopment","emotionalDevelopment","intellectualDevelopment","socialDevelopment","errorMessage","methods","submitDevelopmentForm","console","error","token","localStorage","getItem","dataToSend","periods","response","post","headers","Authorization","log","$router","push"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"child-development-form\">\r\n    <h2 class=\"form-title\">Unos Razvoja Deteta</h2>\r\n    <form @submit.prevent=\"submitDevelopmentForm\" class=\"form-container\">\r\n      <div class=\"form-group\">\r\n        <label for=\"physicalDevelopment\">Fizički razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.physicalDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"emotionalDevelopment\">Emocionalni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.emotionalDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"intellectualDevelopment\">Intelektualni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.intellectualDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"socialDevelopment\">Socijalni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.socialDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <button type=\"submit\" class=\"submit-btn\">Pošaljite</button>\r\n    </form>\r\n\r\n    <div v-if=\"errorMessage\" class=\"error-message\">\r\n      {{ errorMessage }}\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: ['childId', 'startAge'], // make sure childId and startAge are passed correctly\r\n\r\n  data() {\r\n    return {\r\n      newDevelopment: {\r\n        childId: this.childId,  \r\n        startAge: this.startAge, // use the startAge to determine the period\r\n        endAge: parseInt(this.startAge) + 6,  // assuming each period is 6 months\r\n        physicalDevelopment: '',\r\n        emotionalDevelopment: '',\r\n        intellectualDevelopment: '',\r\n        socialDevelopment: ''\r\n      },\r\n      errorMessage: ''\r\n    };\r\n  },\r\n  methods: {\r\n    async submitDevelopmentForm() {\r\n      // Validate the required fields\r\n      if (!this.newDevelopment.physicalDevelopment || \r\n          !this.newDevelopment.emotionalDevelopment || \r\n          !this.newDevelopment.intellectualDevelopment || \r\n          !this.newDevelopment.socialDevelopment) {\r\n        this.errorMessage = 'Sva polja su obavezna.';\r\n        return;\r\n      }\r\n\r\n      // Ensure childId is present\r\n      if (!this.newDevelopment.childId) {\r\n        console.error('childId je undefined');\r\n        this.errorMessage = 'Greška, childId nije prosleđen.';\r\n        return;\r\n      }\r\n      \r\n      // Ensure the user is logged in\r\n      const token = localStorage.getItem('token');\r\n      if (!token) {\r\n        this.errorMessage = 'Morate biti ulogovani da biste dodali dete.';\r\n        return;\r\n      }\r\n\r\n      // Wrap the form data in the periods array\r\n      const dataToSend = {\r\n        childId: this.newDevelopment.childId,\r\n        periods: [{\r\n          startAge: this.newDevelopment.startAge,\r\n          endAge: this.newDevelopment.endAge,\r\n          physicalDevelopment: this.newDevelopment.physicalDevelopment,\r\n          emotionalDevelopment: this.newDevelopment.emotionalDevelopment,\r\n          intellectualDevelopment: this.newDevelopment.intellectualDevelopment,\r\n          socialDevelopment: this.newDevelopment.socialDevelopment\r\n        }]\r\n      };\r\n\r\n      try {\r\n        const response = await axios.post(`http://localhost:5000/api/child-development/${this.newDevelopment.childId}`, dataToSend, {\r\n          headers: {\r\n            Authorization: `Bearer ${token}`,\r\n          },\r\n        });\r\n        console.log('Podaci su uspešno poslati:', response);\r\n\r\n        this.$router.push(`/child-development-page/${this.newDevelopment.childId}/${this.newDevelopment.startAge}`);\r\n      } catch (error) {\r\n        console.error('Greška prilikom slanja podataka:', error);\r\n        this.errorMessage = 'Greška prilikom slanja podataka. Molimo pokušajte ponovo.';\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style lang=\"scss\">\r\n.child-development-form {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f9f9f9;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n  \r\n  .form-title {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: #333;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .form-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .form-group {\r\n    margin-bottom: 15px;\r\n    \r\n    label {\r\n      font-size: 16px;\r\n      font-weight: bold;\r\n      color: #333;\r\n      margin-bottom: 5px;\r\n    }\r\n\r\n    .form-input {\r\n      width: 100%;\r\n      height: 100px;\r\n      padding: 10px;\r\n      font-size: 14px;\r\n      border: 1px solid #ccc;\r\n      border-radius: 4px;\r\n      resize: none;\r\n      box-sizing: border-box;\r\n    }\r\n  }\r\n\r\n  .submit-btn {\r\n    padding: 12px;\r\n    font-size: 16px;\r\n    color: #fff;\r\n    background-color: #4CAF50;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease;\r\n    \r\n    &:hover {\r\n      background-color: #45a049;\r\n    }\r\n  }\r\n\r\n  .error-message {\r\n    margin-top: 20px;\r\n    padding: 10px;\r\n    background-color: #ffdddd;\r\n    border: 1px solid #f5c6cb;\r\n    color: #721c24;\r\n    border-radius: 4px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AA8BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE,CAAC,SAAS,EAAE,UAAU,CAAC;EAAE;;EAEhCC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE;QACdC,OAAO,EAAE,IAAI,CAACA,OAAO;QACrBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QAAE;QACzBC,MAAM,EAAEC,QAAQ,CAAC,IAAI,CAACF,QAAQ,IAAI,CAAC;QAAG;QACtCG,mBAAmB,EAAE,EAAE;QACvBC,oBAAoB,EAAE,EAAE;QACxBC,uBAAuB,EAAE,EAAE;QAC3BC,iBAAiB,EAAE;MACrB,CAAC;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,qBAAqBA,CAAA,EAAG;MAC5B;MACA,IAAI,CAAC,IAAI,CAACX,cAAc,CAACK,mBAAkB,IACvC,CAAC,IAAI,CAACL,cAAc,CAACM,oBAAmB,IACxC,CAAC,IAAI,CAACN,cAAc,CAACO,uBAAsB,IAC3C,CAAC,IAAI,CAACP,cAAc,CAACQ,iBAAiB,EAAE;QAC1C,IAAI,CAACC,YAAW,GAAI,wBAAwB;QAC5C;MACF;;MAEA;MACA,IAAI,CAAC,IAAI,CAACT,cAAc,CAACC,OAAO,EAAE;QAChCW,OAAO,CAACC,KAAK,CAAC,sBAAsB,CAAC;QACrC,IAAI,CAACJ,YAAW,GAAI,iCAAiC;QACrD;MACF;;MAEA;MACA,MAAMK,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAC3C,IAAI,CAACF,KAAK,EAAE;QACV,IAAI,CAACL,YAAW,GAAI,6CAA6C;QACjE;MACF;;MAEA;MACA,MAAMQ,UAAS,GAAI;QACjBhB,OAAO,EAAE,IAAI,CAACD,cAAc,CAACC,OAAO;QACpCiB,OAAO,EAAE,CAAC;UACRhB,QAAQ,EAAE,IAAI,CAACF,cAAc,CAACE,QAAQ;UACtCC,MAAM,EAAE,IAAI,CAACH,cAAc,CAACG,MAAM;UAClCE,mBAAmB,EAAE,IAAI,CAACL,cAAc,CAACK,mBAAmB;UAC5DC,oBAAoB,EAAE,IAAI,CAACN,cAAc,CAACM,oBAAoB;UAC9DC,uBAAuB,EAAE,IAAI,CAACP,