(period)\">\r\n          {{ formatPeriod(period.start, period.end) }}\r\n          <span v-if=\"isPeriodFilled(period)\" class=\"period-status\">(Ispunjeno ‚úÖ)</span>\r\n          <span v-else-if=\"isCurrentPeriod(period)\" class=\"period-status current\">(Trenutni üü¢)</span>\r\n          <span v-else-if=\"isPastPeriod(period)\" class=\"period-status past clickable\">(Kliknite üîÑ)</span>\r\n          <span v-else class=\"period-status future\">(Nije dostupno üö´)</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Selected Period Info -->\r\n    <div v-if=\"selectedPeriod\" class=\"selected-period\">\r\n      <h3>Izabrani period: {{ formatPeriod(selectedPeriod.start, selectedPeriod.end) }}</h3>\r\n      \r\n      <div class=\"action-buttons\">\r\n        <router-link :to=\"`/milestone-list/${$route.params.childId}`\" class=\"button\">\r\n          Pregled kljuƒçnih momenata\r\n        </router-link>\r\n        <router-link :to=\"`/child-development-list/${$route.params.childId}`\" class=\"button\">\r\n          Pregled razvoja deteta\r\n        </router-link>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return { \r\n      developmentData: [], \r\n      availablePeriods: [],\r\n      childName: null,\r\n      childAge: null, \r\n      selectedPeriod: null,\r\n      error: null,\r\n      loading: true\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    async fetchChildData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n\r\n        if (response.data) {\r\n          this.childName = response.data.name;\r\n          this.childAge = this.calculateAgeInMonths(new Date(response.data.birthDate));\r\n\r\n          console.log('üü¢ Dete:', this.childName);\r\n          console.log('üìå Starost deteta:', this.childAge, 'meseci');\r\n        } else {\r\n          console.warn(\"‚ö†Ô∏è Nema podataka o detetu.\");\r\n          this.childAge = 0;\r\n        }\r\n      } catch (error) {\r\n        console.error('‚ùå Gre≈°ka prilikom dobijanja podataka o detetu:', error);\r\n        this.childAge = 0;\r\n      }\r\n    },\r\n\r\n    calculateAgeInMonths(birthDate) {\r\n      const birth = new Date(birthDate);\r\n      const now = new Date();\r\n      return (now.getFullYear() - birth.getFullYear()) * 12 + (now.getMonth() - birth.getMonth());\r\n    },\r\n\r\n    generateAvailablePeriods() {\r\n      if (this.childAge === null) {\r\n        console.warn(\"‚ö†Ô∏è Starost deteta nije postavljena, preskaƒçem generisanje perioda.\");\r\n        return;\r\n      }\r\n\r\n      this.availablePeriods = [];\r\n      for (let i = 0; i <= 72; i += 6) {\r\n        this.availablePeriods.push({ start: i, end: i + 6 });\r\n      }\r\n    },\r\n\r\n    isCurrentPeriod(period) {\r\n      return this.childAge !== null && period.start <= this.childAge && period.end > this.childAge;\r\n    },\r\n\r\n    isPastPeriod(period) {\r\n      return this.childAge !== null && period.end <= this.childAge;\r\n    },\r\n\r\n    isFuturePeriod(period) {\r\n      return this.childAge !== null && period.start > this.childAge;\r\n    },\r\n\r\n    formatPeriod(start, end) {\r\n      return `${start}-${end} meseci`;\r\n    },\r\n\r\n    isPeriodFilled(period) {\r\n      if (!this.developmentData) return false;\r\n\r\n      return this.developmentData.some(dev =>\r\n        dev.startAge === period.start && dev.endAge === period.end\r\n      );\r\n    },\r\n\r\n    handlePeriodClick(period) {\r\n      if (this.isFuturePeriod(period)) {\r\n        return;\r\n      }\r\n\r\n      const childId = this.$route.params.childId;\r\n      \r\n      if (this.isPeriodFilled(period)) {\r\n        this.selectedPeriod = period;\r\n      } else if (this.isCurrentPeriod(pe