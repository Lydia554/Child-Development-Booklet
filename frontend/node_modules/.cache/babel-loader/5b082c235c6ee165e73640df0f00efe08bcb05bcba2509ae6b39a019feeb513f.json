pment-container\">\r\n    <div v-if=\"error\" class=\"error-message\">{{ error }}</div>\r\n\r\n    <h2>Pregled Razvoja Deteta</h2>\r\n\r\n    <!-- Debugging Logs -->\r\n    <div class=\"debug-info\">\r\n      <p><strong>Starost deteta:</strong> {{ childAge }} meseci</p>\r\n      <p><strong>Izabrani period:</strong> {{ selectedPeriod ? formatPeriod(selectedPeriod.start, selectedPeriod.end) : 'Nijedan' }}</p>\r\n    </div>\r\n\r\n    <!-- Period selector -->\r\n    <div class=\"available-periods\" v-if=\"availablePeriods.length > 0\">\r\n      <h3>Dostupni periodi:</h3>\r\n      <div class=\"periods-grid\">\r\n        <div v-for=\"(period, index) in availablePeriods\" \r\n             :key=\"index\" \r\n             :class=\"getPeriodClass(period)\"\r\n             @click=\"handlePeriodClick(period)\">\r\n          {{ formatPeriod(period.start, period.end) }}\r\n          <span v-if=\"isPeriodFilled(period)\" class=\"period-status\">(Ispunjeno ‚úÖ)</span>\r\n          <span v-else-if=\"isCurrentPeriod(period)\" class=\"period-status current\">(Trenutni üü¢)</span>\r\n          <span v-else-if=\"isPastPeriod(period)\" class=\"period-status past clickable\">(Kliknite üîÑ)</span>\r\n          <span v-else class=\"period-status future\">(Nije dostupno üö´)</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return { \r\n      developmentData: [], \r\n      availablePeriods: [],\r\n      childAge: null, \r\n      selectedPeriod: null,\r\n      error: null,\r\n      loading: true\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    async fetchChildAge(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n\r\n        if (response.data.birthDate) {\r\n          const birthDate = new Date(response.data.birthDate);\r\n          this.childAge = this.calculateAgeInMonths(birthDate);\r\n\r\n          console.log('üü¢ Datum roƒëenja deteta:', birthDate);\r\n          console.log('üìå Izraƒçunata starost deteta u mesecima:', this.childAge);\r\n        } else {\r\n          console.warn(\"‚ö†Ô∏è Datum roƒëenja nije pronaƒëen u API odgovoru.\");\r\n          this.childAge = 0;\r\n        }\r\n      } catch (error) {\r\n        console.error('‚ùå Gre≈°ka prilikom dobijanja starosti deteta:', error);\r\n        this.childAge = 0; \r\n      }\r\n    },\r\n\r\n    calculateAgeInMonths(birthDate) {\r\n      const birth = new Date(birthDate);\r\n      const now = new Date();\r\n      return (now.getFullYear() - birth.getFullYear()) * 12 + (now.getMonth() - birth.getMonth());\r\n    },\r\n\r\n    generateAvailablePeriods() {\r\n      if (this.childAge === null) {\r\n        console.warn(\"‚ö†Ô∏è Starost deteta nije postavljena, preskaƒçem generisanje perioda.\");\r\n        return;\r\n      }\r\n\r\n      this.availablePeriods = [];\r\n      for (let i = 0; i <= 72; i += 6) {\r\n        this.availablePeriods.push({ start: i, end: i + 6 });\r\n      }\r\n\r\n      console.log(\"‚úÖ Generisani dostupni periodi.\");\r\n    },\r\n\r\n    isCurrentPeriod(period) {\r\n      return this.childAge !== null && period.start <= this.childAge && period.end > this.childAge;\r\n    },\r\n\r\n    isPastPeriod(period) {\r\n      return this.childAge !== null && period.end <= this.childAge;\r\n    },\r\n\r\n    isFuturePeriod(period) {\r\n      return this.childAge !== null && period.start > this.childAge;\r\n    },\r\n\r\n    formatPeriod(start, end) {\r\n      return `${start}-${end} meseci`;\r\n    },\r\n\r\n    isPeriodFilled(period) {\r\n      if (!this.developmentData) return false;\r\n\r\n      return this.developmentData.some(dev =>\r\n        dev.startAge === period.start && dev.endAge === period.end\r\n      );\r\n    },\r\n\r\n    handlePeriodClick(period) {\r\n      if (this.isFuturePeriod(period)) {\r\n        return;\r\n      }\r\n\r\n      const childId = this.$route.params.childId;\r\n      \r\n      if (this.isPeriodFilled(period)) {\r\n        this.selectedPeriod = period;\r\n      } else if (this.isCurrentPeriod(period) || this.isPastPeriod(period)) {\r\n        this.$router.push({\r\n          path: `/child-development-form/${childId}`,\r\n          query: { startAge: period.start, endAge: period.end }\r\n        });\r\n      }\r\n    },\r\n\r\n    getPeriodClass(period) {\r\n      return {\r\n        'period-item': true,\r\n        'filled': this.isPeriodFilled(period),\r\n        'current': this.isCurrentPeriod(period),\r\n        'past': this.isPastPeriod(period),\r\n        'future': this.isFuturePeriod(period),\r\n        'clickable': !this.isFuturePeriod(period),\r\n        'selected': this.selectedPeriod && this.selectedPeriod.start === period.start\r\n      };\r\n    }\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      const childId = this.$route.params.childId;\r\n      if (childId) {\r\n        await this.fetchChildAge(childId);\r\n        this.generateAvailablePeriods();\r\n      }\r\n    } catch (error) {\r\n      console.error('‚ùå Gre≈°ka u created():', error);\r\n    } finally {\r\n      this.loading = false;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.development-container {\r\n  padding: 20px;\r\n}\r\n\r\n.error-message {\r\n  color: red;\r\n  background-color: #fee;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n}\r\n\r\n.debug-info {\r\n  background: #f8f9fa;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.available-periods h3 {\r\n  margin-bottom: 10px;\r\n  color: #1976d2;\r\n}\r\n\r\n/* üü¢ Grid Layout for Periods */\r\n.periods-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(120px, 1fr));\r\n  gap: 10px;\r\n}\r\n\r\n/* üîµ Period Item */\r\n.period-item {\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  text-align: center;\r\n  cursor: pointer;\r\n  transition: 0.3s;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n}\r\n\r\n/* üîµ Different Period Styles */\r\n.period-item.filled {\r\n  background: #81c784; /* Green */\r\n}\r\n\r\n.period-item.current {\r\n  background: #4caf50; /* Bright Green */\r\n  color: white;\r\n}\r\n\r\n.period-item.past {\r\n  background: #ffcc80; /* Orange */\r\n}\r\n\r\n.period-item.future {\r\n  background: #eeeeee; /* Gray */\r\n  cursor: not-allowed;\r\n}\r\n\r\n.period-status {\r\n  font-size: 12px;\r\n  margin-top: 3px;\r\n  display: block;\r\n}\r\n\r\n/* üü¢ Hover Effects */\r\n.period-item.clickable:hover {\r\n  background: #e3f2fd;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAuB;;EADpCC,GAAA;EAEsBD,KAAK,EAAC;;;EAKnBA,KAAK,EAAC;AAAY;;EAP3BC,GAAA;EAaSD,KAAK,EAAC;;;EAEJA,KAAK,EAAC;AAAc;mBAf/B;;EAAAC,GAAA;EAqB8CD,KAAK,EAAC;;;EArBpDC,GAAA;EAsBoDD,KAAK,EAAC;;;EAtB1DC,GAAA;EAuBiDD,KAAK,EAAC;;;EAvBvDC,GAAA;EAwBuBD,KAAK,EAAC;;;uBAvB3BE,mBAAA,CA2BM,OA3BNC,UA2BM,GA1BOC,KAAA,CAAAC,KAAK,I,cAAhBH,mBAAA,CAAyD,OAAzDI,UAAyD,EAAAC,gBAAA,CAAdH,KAAA,CAAAC,KAAK,oBAFpDG,mBAAA,gB,0BAIIC,mBAAA,CAA+B,YAA3B,wBAAsB,sBAE1BD,mBAAA,oBAAuB,EACvBC,mBAAA,CAGM,OAHNC,UAGM,GAFJD,mBAAA,CAA6D,Y,0BAA1DA,mBAAA,CAAgC,gBAAxB,iBAAe,sBARhCE,gBAAA,CAQyC,GAAC,GAAAJ,gBAAA,CAAGH,KAAA,CAAAQ,QAAQ,IAAG,SAAO,gB,GACzDH,mBAAA,CAAkI,Y,0BAA/HA,mBAAA,CAAiC,gBAAzB,kBAAgB,sBATjCE,gBAAA,CAS0C,GAAC,GAAAJ,gBAAA,CAAGH,KAAA,CAAAS,cAAc,GAAGC,QAAA,CAAAC,YAAY,CAACX,KAAA,CAAAS,cAAc,CAACG,KAAK,EAAEZ,KAAA,CAAAS,cAAc,CAACI,GAAG,8B,KAGhHT,mBAAA,qBAAwB,EACaJ,KAAA,CAAAc,gBAAgB,CAACC,MAAM,Q,cAA5DjB,mBAAA,CAcM,OAdNkB,UAcM,G,0BAbJX,mBAAA,CAA0B,YAAtB,mBAAiB,sBACrBA,mBAAA,CAWM,OAXNY,UAWM,I,kBAVJnB,mBAAA,CASMoB,SAAA,QAzBdC,WAAA,CAgBuCnB,KAAA,CAAAc,gBAAgB,EAhBvD,CAgBqBM,MAAM,EAAEC,KAAK;yBAA1BvB,mBAAA,CASM;MARAD,GAAG,EAAEwB,KAAK;MACVzB,KAAK,EAlBnB0B,eAAA,CAkBqBZ,QAAA,CAAAa,cAAc,CAACH,MAAM;MAC5BI,OAAK,EAAAC,MAAA,IAAEf,QAAA,CAAAgB,iBAAiB,CAACN,MAAM;QAnB7Cb,gBAAA,CAAAJ,gBAAA,CA