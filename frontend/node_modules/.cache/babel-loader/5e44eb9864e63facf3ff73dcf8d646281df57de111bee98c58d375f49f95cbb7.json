{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  props: ['childId'],\n  // We get childId as a prop\n\n  data() {\n    return {\n      newDevelopment: {\n        childId: this.childId,\n        // The childId is passed as a prop\n        startAge: '',\n        // Start age (months)\n        endAge: '',\n        // End age (months)\n        physicalDevelopment: '',\n        emotionalDevelopment: '',\n        intellectualDevelopment: '',\n        socialDevelopment: ''\n      },\n      errorMessage: ''\n    };\n  },\n  methods: {\n    async submitDevelopmentForm() {\n      if (!this.newDevelopment.startAge || !this.newDevelopment.endAge || !this.newDevelopment.physicalDevelopment || !this.newDevelopment.emotionalDevelopment || !this.newDevelopment.intellectualDevelopment || !this.newDevelopment.socialDevelopment) {\n        this.errorMessage = 'Sva polja su obavezna.';\n        return;\n      }\n      const token = localStorage.getItem('token');\n      if (!token) {\n        this.errorMessage = 'Morate biti ulogovani da biste dodali razvoj.';\n        return;\n      }\n      try {\n        // Send the data to the backend\n        const response = await axios.post(`http://localhost:5000/api/child-development/${this.newDevelopment.childId}`, this.newDevelopment, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        console.log('Podaci su uspešno poslati:', response);\n        this.$router.push('/child-development-list');\n      } catch (error) {\n        console.error('Greška prilikom slanja podataka:', error);\n        this.errorMessage = 'Došlo je do greške prilikom slanja podataka. Molimo pokušajte ponovo.';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","props","data","newDevelopment","childId","startAge","endAge","physicalDevelopment","emotionalDevelopment","intellectualDevelopment","socialDevelopment","errorMessage","methods","submitDevelopmentForm","token","localStorage","getItem","response","post","headers","Authorization","console","log","$router","push","error"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"child-development-form\">\r\n    <h2 class=\"form-title\">Unos Razvoja Deteta</h2>\r\n    <form @submit.prevent=\"submitDevelopmentForm\" class=\"form-container\">\r\n      <div class=\"form-group\">\r\n        <label for=\"physicalDevelopment\">Fizički razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.physicalDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"emotionalDevelopment\">Emocionalni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.emotionalDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"intellectualDevelopment\">Intelektualni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.intellectualDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"socialDevelopment\">Socijalni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.socialDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"startAge\">Početni uzrast:</label>\r\n        <input v-model=\"newDevelopment.startAge\" type=\"number\" required placeholder=\"Početni uzrast (meseci)\" />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"endAge\">Krajnji uzrast:</label>\r\n        <input v-model=\"newDevelopment.endAge\" type=\"number\" required placeholder=\"Krajnji uzrast (meseci)\" />\r\n      </div>\r\n      <button type=\"submit\" class=\"submit-btn\">Pošaljite</button>\r\n    </form>\r\n\r\n    <div v-if=\"errorMessage\" class=\"error-message\">\r\n      {{ errorMessage }}\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: ['childId'],  // We get childId as a prop\r\n\r\n  data() {\r\n    return {\r\n      newDevelopment: {\r\n        childId: this.childId,  // The childId is passed as a prop\r\n        startAge: '',  // Start age (months)\r\n        endAge: '',    // End age (months)\r\n        physicalDevelopment: '',\r\n        emotionalDevelopment: '',\r\n        intellectualDevelopment: '',\r\n        socialDevelopment: ''\r\n      },\r\n      errorMessage: ''\r\n    };\r\n  },\r\n  methods: {\r\n    async submitDevelopmentForm() {\r\n      if (!this.newDevelopment.startAge || !this.newDevelopment.endAge || !this.newDevelopment.physicalDevelopment || !this.newDevelopment.emotionalDevelopment || !this.newDevelopment.intellectualDevelopment || !this.newDevelopment.socialDevelopment) {\r\n        this.errorMessage = 'Sva polja su obavezna.';\r\n        return;\r\n      }\r\n\r\n      const token = localStorage.getItem('token');\r\n      if (!token) {\r\n        this.errorMessage = 'Morate biti ulogovani da biste dodali razvoj.';\r\n        return;\r\n      }\r\n\r\n      try {\r\n        // Send the data to the backend\r\n        const response = await axios.post(`http://localhost:5000/api/child-development/${this.newDevelopment.childId}`, this.newDevelopment, {\r\n          headers: {\r\n            Authorization: `Bearer ${token}`,\r\n          },\r\n        });\r\n\r\n        console.log('Podaci su uspešno poslati:', response);\r\n        this.$router.push('/child-development-list');\r\n      } catch (error) {\r\n        console.error('Greška prilikom slanja podataka:', error);\r\n        this.errorMessage = 'Došlo je do greške prilikom slanja podataka. Molimo pokušajte ponovo.';\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style lang=\"scss\">\r\n.child-development-form {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f9f9f9;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n  \r\n  .form-title {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: #333;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .form-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .form-group {\r\n    margin-bottom: 15px;\r\n    \r\n    label {\r\n      font-size: 16px;\r\n      font-weight: bold;\r\n      color: #333;\r\n      margin-bottom: 5px;\r\n    }\r\n\r\n    .form-input {\r\n      width: 100%;\r\n      height: 100px;\r\n      padding: 10px;\r\n      font-size: 14px;\r\n      border: 1px solid #ccc;\r\n      border-radius: 4px;\r\n      resize: none;\r\n      box-sizing: border-box;\r\n    }\r\n  }\r\n\r\n  .submit-btn {\r\n    padding: 12px;\r\n    font-size: 16px;\r\n    color: #fff;\r\n    background-color: #4CAF50;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease;\r\n    \r\n    &:hover {\r\n      background-color: #45a049;\r\n    }\r\n  }\r\n\r\n  .error-message {\r\n    margin-top: 20px;\r\n    padding: 10px;\r\n    background-color: #ffdddd;\r\n    border: 1px solid #f5c6cb;\r\n    color: #721c24;\r\n    border-radius: 4px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AAsCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE,CAAC,SAAS,CAAC;EAAG;;EAErBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE;QACdC,OAAO,EAAE,IAAI,CAACA,OAAO;QAAG;QACxBC,QAAQ,EAAE,EAAE;QAAG;QACfC,MAAM,EAAE,EAAE;QAAK;QACfC,mBAAmB,EAAE,EAAE;QACvBC,oBAAoB,EAAE,EAAE;QACxBC,uBAAuB,EAAE,EAAE;QAC3BC,iBAAiB,EAAE;MACrB,CAAC;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,qBAAqBA,CAAA,EAAG;MAC5B,IAAI,CAAC,IAAI,CAACV,cAAc,CAACE,QAAO,IAAK,CAAC,IAAI,CAACF,cAAc,CAACG,MAAK,IAAK,CAAC,IAAI,CAACH,cAAc,CAACI,mBAAkB,IAAK,CAAC,IAAI,CAACJ,cAAc,CAACK,oBAAmB,IAAK,CAAC,IAAI,CAACL,cAAc,CAACM,uBAAsB,IAAK,CAAC,IAAI,CAACN,cAAc,CAACO,iBAAiB,EAAE;QACnP,IA