       const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.developmentData = response.data;\n      } catch (error) {\n        console.error(\"❌ Greška pri učitavanju podataka o razvoju:\", error);\n      }\n    },\n    calculateAgeInMonths(birthDate) {\n      const birth = new Date(birthDate);\n      const now = new Date();\n      return (now.getFullYear() - birth.getFullYear()) * 12 + (now.getMonth() - birth.getMonth());\n    },\n    togglePeriod(period) {\n      this.selectedPeriod = this.selectedPeriod?.start === period.start ? null : period;\n    },\n    getPeriodClass(period) {\n      return {\n        'period-item': true,\n        'filled': this.isPeriodFilled(period),\n        'current': this.isCurrentPeriod(period),\n        'past': this.isPastPeriod(period),\n        'future': this.isFuturePeriod(period),\n        'clickable': !this.isFuturePeriod(period),\n        'selected': this.selectedPeriod && this.selectedPeriod.start === period.start\n      };\n    }\n  },\n  async created() {\n    try {\n      const childId = this.$route.params.childId;\n      if (childId) {\n        await this.fetchChildData(childId);\n        await this.fetchMilestones(childId);\n        await this.fetchDevelopmentData(childId);\n      }\n    } catch (error) {\n      console.error('❌ Greška u created():', error);\n    }\n  }\n};","map":{"version":3,"names":["axios","data","developmentData","milestoneData","availablePeriods","childName","childAge","selectedPeriod","error","computed","currentPeriodMilestones","filter","m","age","start","end","currentPeriodDevelopment","find","d","startAge","endAge","methods","fetchChildData","childId","token","sessionStorage","getItem","response","get","headers","Authorization","name","calculateAgeInMonths","Date","birthDate","console","fetchMiles