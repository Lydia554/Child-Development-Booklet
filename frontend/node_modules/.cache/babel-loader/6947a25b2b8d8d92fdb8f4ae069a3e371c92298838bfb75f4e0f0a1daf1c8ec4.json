{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport axios from \"axios\";\nimport jsPDF from \"jspdf\";\nimport \"jspdf-autotable\";\nexport default {\n  data() {\n    return {\n      children: [],\n      selectedChildId: \"\",\n      child: {},\n      developmentData: [],\n      loading: true,\n      errorMessage: \"\"\n    };\n  },\n  async created() {\n    await this.fetchChildren();\n  },\n  methods: {\n    async fetchChildren() {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(\"http://localhost:5000/api\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.children = response.data;\n        console.log(\"Lista dece učitana:\", this.children);\n        if (this.children.length > 0) {\n          this.selectedChildId = this.children[0]._id;\n          await this.fetchChildData(this.selectedChildId);\n          await this.fetchDevelopmentData(this.selectedChildId);\n        }\n      } catch (error) {\n        console.error(\"❌ Greška pri učitavanju dece:\", error);\n        this.errorMessage = \"Nije moguće učitati listu dece.\";\n      }\n    },\n    async fetchChildData(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (!response.data || Object.keys(response.data).length === 0) {\n          this.errorMessage = \"Podaci o detetu nisu pronađeni.\";\n          return;\n        }\n        this.child = response.data;\n        console.log(\"Podaci o detetu:\", this.child);\n      } catch (error) {\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\n        this.errorMessage = \"Nije moguće učitati podatke o detetu.\";\n      }\n    },\n    async fetchDevelopmentData(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.developmentData = response.data || [];\n      } catch (error) {\n        console.error(\"❌ Greška pri dobijanju podataka o razvoju:\", error);\n        this.errorMessage = \"Nije moguće učitati razvojne podatke.\";\n      } finally {\n        this.loading = false;\n      }\n    },\n    onChildSelect() {\n      console.log(\"Odabrano dete ID:\", this.selectedChildId);\n      this.fetchChildData(this.selectedChildId);\n      this.fetchDevelopmentData(this.selectedChildId);\n    },\n    downloadPDF() {\n      const doc = new jsPDF();\n      doc.text(`Razvoj deteta: ${this.child.name}`, 10, 10);\n      const tableColumn = [\"Period\", \"Fizički\", \"Emocionalni\", \"Intelektualni\", \"Socijalni\"];\n      const tableRows = [];\n      this.developmentData.forEach(period => {\n        const row = [`${period.startAge}-${period.endAge} meseci`, period.physicalDevelopment || \"-\", period.emotionalDevelopment || \"-\", period.intellectualDevelopment || \"-\", period.socialDevelopment || \"-\"];\n        tableRows.push(row);\n      });\n      doc.autoTable({\n        head: [tableColumn],\n        body: tableRows,\n        startY: 20\n      });\n      doc.save(`Razvoj_${this.child.name}.pdf`);\n    }\n  }\n};","map":{"version":3,"names":["axios","jsPDF","data","children","selectedChildId","child","developmentData","loading","errorMessage","created","fetchChildren","methods","token","sessionStorage","getItem","response","get","headers","Authorization","console","log","length","_id","fetchChildData","fetchDevelopmentData","error","childId","Object","keys","onChildSelect","downloadPDF","doc","text","name","tableColumn","tableRows","forEach","period","row","startAge","endAge","physicalDevelopment","emotionalDevelopment","intellectualDevelopment","socialDevelopment","push","autoTable","head","body","startY","save"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildOverview.vue"],"sourcesContent":["<template>\r\n  <div class=\"overview-container\">\r\n    <h2>Pregled razvoja deteta - {{ child.name }}</h2>\r\n\r\n    <div v-if=\"loading\">Učitavanje podataka...</div>\r\n    <div v-else-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</div>\r\n    <div v-else>\r\n      <!-- Dropdown za izbor deteta -->\r\n      <div class=\"select-container\">\r\n        <label for=\"child-select\">Izaberite dete:</label>\r\n        <select v-model=\"selectedChildId\" @change=\"onChildSelect\">\r\n          <option v-for=\"child in children\" :key=\"child._id\" :value=\"child._id\">\r\n            {{ child.name }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n\r\n      <!-- Tabela sa podacima o razvoju -->\r\n      <table class=\"development-table\">\r\n        <thead>\r\n          <tr>\r\n            <th>Period</th>\r\n            <th>Fizički razvoj</th>\r\n            <th>Emocionalni razvoj</th>\r\n            <th>Intelektualni razvoj</th>\r\n            <th>Socijalni razvoj</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"period in developmentData\" :key=\"period.startAge\">\r\n            <td>{{ period.startAge }} - {{ period.endAge }} meseci</td>\r\n            <td>{{ period.physicalDevelopment || '-' }}</td>\r\n            <td>{{ period.emotionalDevelopment || '-' }}</td>\r\n            <td>{{ period.intellectualDevelopment || '-' }}</td>\r\n            <td>{{ period.socialDevelopment || '-' }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n\r\n      <!-- Dugme za preuzimanje PDF-a -->\r\n      <button @click=\"downloadPDF\" class=\"pdf-button\">Preuzmi PDF</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport jsPDF from \"jspdf\";\r\nimport \"jspdf-autotable\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      children: [],\r\n      selectedChildId: \"\",\r\n      child: {},\r\n      developmentData: [],\r\n      loading: true,\r\n      errorMessage: \"\",\r\n    };\r\n  },\r\n  async created() {\r\n    await this.fetchChildren();\r\n  },\r\n  methods: {\r\n    async fetchChildren() {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\"http://localhost:5000/api\", {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n\r\n        this.children = response.data;\r\n        console.log(\"Lista dece učitana:\", this.children);\r\n\r\n        if (this.children.length > 0) {\r\n          this.selectedChildId = this.children[0]._id;\r\n          await this.fetchChildData(this.selectedChildId);\r\n          await this.fetchDevelopmentData(this.selectedChildId);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri učitavanju dece:\", error);\r\n        this.errorMessage = \"Nije moguće učitati listu dece.\";\r\n      }\r\n    },\r\n\r\n    async fetchChildData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n\r\n        if (!response.data || Object.keys(response.data).length === 0) {\r\n          this.errorMessage = \"Podaci o detetu nisu pronađeni.\";\r\n          return;\r\n        }\r\n\r\n        this.child = response.data;\r\n        console.log(\"Podaci o detetu:\", this.child);\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\r\n        this.errorMessage = \"Nije moguće učitati podatke o detetu.\";\r\n      }\r\n    },\r\n\r\n    async fetchDevelopmentData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        this.developmentData = response.data || [];\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri dobijanju podataka o razvoju:\", error);\r\n        this.errorMessage = \"Nije moguće učitati razvojne podatke.\";\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    onChildSelect() {\r\n      console.log(\"Odabrano dete ID:\", this.selectedChildId);\r\n      this.fetchChildData(this.selectedChildId);\r\n      this.fetchDevelopmentData(this.selectedChildId);\r\n    },\r\n\r\n    downloadPDF() {\r\n      const doc = new jsPDF();\r\n      doc.text(`Razvoj deteta: ${this.child.name}`, 10, 10);\r\n\r\n      const tableColumn = [\"Period\", \"Fizički\", \"Emocionalni\", \"Intelektualni\", \"Socijalni\"];\r\n      const tableRows = [];\r\n\r\n      this.developmentData.forEach(period => {\r\n        const row = [\r\n          `${period.startAge}-${period.endAge} meseci`,\r\n          period.physicalDevelopment || \"-\",\r\n          period.emotionalDevelopment || \"-\",\r\n          period.intellectualDevelopment || \"-\",\r\n          period.socialDevelopment || \"-\",\r\n        ];\r\n        tableRows.push(row);\r\n      });\r\n\r\n      doc.autoTable({ head: [tableColumn], body: tableRows, startY: 20 });\r\n      doc.save(`Razvoj_${this.child.name}.pdf`);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.overview-container {\r\n  max-width: 900px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f9f9f9;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\nh2 {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.development-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  background: white;\r\n}\r\n\r\n.development-table th,\r\n.development-table td {\r\n  border: 1px solid #ddd;\r\n  padding: 10px;\r\n  text-align: left;\r\n}\r\n\r\n.development-table th {\r\n  background: #007bff;\r\n  color: white;\r\n}\r\n\r\n.error-message {\r\n  color: red;\r\n  text-align: center;\r\n  font-weight: bold;\r\n}\r\n\r\n.pdf-button {\r\n  display: block;\r\n  margin: 20px auto;\r\n  padding: 10px 20px;\r\n  background-color: #28a745;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.pdf-button:hover {\r\n  background-color: #218838;\r\n}\r\n.select-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n\r\n  label {\r\n    font-size: 18px;\r\n    font-weight: bold;\r\n    color: #333;\r\n    margin-bottom: 8px;\r\n  }\r\n\r\n  select {\r\n    width: 250px;\r\n    padding: 12px;\r\n    font-size: 16px;\r\n    font-weight: bold;\r\n    color: #333;\r\n    background-color: #f0f0f0;\r\n    border: 2px solid #007bff;\r\n    border-radius: 8px;\r\n    cursor: pointer;\r\n    transition: all 0.3s ease-in-out;\r\n\r\n    &:hover {\r\n      background-color: #e0e0e0;\r\n      border-color: #0056b3;\r\n    }\r\n\r\n    &:focus {\r\n      outline: none;\r\n      border-color: #004085;\r\n      box-shadow: 0 0 8px rgba(0, 91, 187, 0.5);\r\n    }\r\n  }\r\n}\r\n\r\n\r\n\r\n</style>\r\n"],"mappings":";;;AA8CA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAO,iBAAiB;AAExB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MACnBC,KAAK,EAAE,CAAC,CAAC;MACTC,eAAe,EAAE,EAAE;MACnBC,OAAO,EAAE,IAAI;MACbC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,aAAa,CAAC,CAAC;EAC5B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAME,KAAI,GAAIC,cAAc,CAACC,OAAO,CAAC,OAA