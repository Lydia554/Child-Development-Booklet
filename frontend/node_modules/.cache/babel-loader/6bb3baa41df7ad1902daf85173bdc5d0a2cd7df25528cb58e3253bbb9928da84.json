{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport axios from \"axios\";\nimport jsPDF from \"jspdf\";\nimport \"jspdf-autotable\";\nexport default {\n  data() {\n    return {\n      children: [],\n      selectedChildId: \"\",\n      child: {},\n      developmentData: [],\n      milestoneData: [],\n      loading: true,\n      errorMessage: \"\"\n    };\n  },\n  async created() {\n    await this.fetchChildren();\n  },\n  methods: {\n    async fetchChildren() {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(\"http://localhost:5000/api\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.children = response.data;\n        console.log(\"Lista dece učitana:\", this.children);\n        if (this.children.length > 0) {\n          this.selectedChildId = this.children[0]._id;\n          await this.fetchChildData(this.selectedChildId);\n          await this.fetchDevelopmentData(this.selectedChildId);\n          await this.fetchMilestones(this.selectedChildId);\n        }\n      } catch (error) {\n        console.error(\"❌ Greška pri učitavanju dece:\", error);\n        this.errorMessage = \"Nije moguće učitati listu dece.\";\n      }\n    },\n    async fetchChildData(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (!response.data || Object.keys(response.data).length === 0) {\n          this.errorMessage = \"Podaci o detetu nisu pronađeni.\";\n          return;\n        }\n        this.child = response.data;\n        console.log(\"Podaci o detetu:\", this.child);\n      } catch (error) {\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\n        this.errorMessage = \"Nije moguće učitati podatke o detetu.\";\n      }\n    },\n    async fetchDevelopmentData(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.developmentData = response.data || [];\n      } catch (error) {\n        console.error(\"❌ Greška pri dobijanju podataka o razvoju:\", error);\n        this.errorMessage = \"Nije moguće učitati razvojne podatke.\";\n      }\n    },\n    async fetchMilestones(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/milestones/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.milestoneData = response.data || [];\n      } catch (error) {\n        console.error(\"❌ Greška pri dobijanju ključnih momenata:\", error);\n      }\n    },\n    onChildSelect() {\n      console.log(\"Odabrano dete ID:\", this.selectedChildId);\n      this.fetchChildData(this.selectedChildId);\n      this.fetchDevelopmentData(this.selectedChildId);\n      this.fetchMilestones(this.selectedChildId);\n    },\n    downloadPDF() {\n      const doc = new jsPDF();\n      doc.text(`Razvoj deteta: ${this.child.name}`, 10, 10);\n      const developmentTable = [[\"Period\", \"Fizički\", \"Emocionalni\", \"Intelektualni\", \"Socijalni\"], ...this.developmentData.map(period => [`${period.startAge}-${period.endAge} meseci`, period.physicalDevelopment || \"-\", period.emotionalDevelopment || \"-\", period.intellectualDevelopment || \"-\", period.socialDevelopment || \"-\"])];\n      const milestoneTable = [[\"Tip momenta\", \"Datum\", \"Napredak\", \"Napomene\"], ...this.milestoneData.map(milestone => [milestone.milestoneType, this.formatDate(milestone.milestoneDate), milestone.progress || \"-\", milestone.notes || \"-\"])];\n      doc.autoTable({\n        head: [developmentTable[0]],\n        body: developmentTable.slice(1),\n        startY: 20\n      });\n      doc.autoTable({\n        head: [milestoneTable[0]],\n        body: milestoneTable.slice(1),\n        startY: doc.lastAutoTable.finalY + 10\n      });\n      doc.save(`Razvoj_${this.child.name}.pdf`);\n    },\n    formatDate(date) {\n      return new Date(date).toLocaleDateString(\"sr-RS\");\n    }\n  }\n};","map":{"version":3,"names":["axios","jsPDF","data","children","selectedChildId","child","developmentData","milestoneData","loading","errorMessage","created","fetchChildren","methods","token","sessionStorage","getItem","response","get","headers","Authorization","console","log","length","_id","fetchChildData","fetchDevelopmentData","fetchMilestones","error","childId","Object","keys","onChildSelect","downloadPDF","doc","text","name","developmentTable","map","period","startAge","endAge","physicalDevelopment","emotionalDevelopment","intellectualDevelopment","socialDevelopment","milestoneTable","milestone","milestoneType","formatDate","milestoneDate","progress","notes","autoTable","head","body","slice","startY","lastAutoTable","finalY","save","date","Date","toLocaleDateString"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildOverview.vue"],"sourcesContent":["<template>\r\n  <div class=\"overview-container\">\r\n    <h2>Pregled razvoja deteta - {{ child.name }}</h2>\r\n\r\n    <div v-if=\"loading\">Učitavanje podataka...</div>\r\n    <div v-else-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</div>\r\n    <div v-else>\r\n      <!-- Dropdown za izbor deteta -->\r\n      <div class=\"select-container\">\r\n        <label for=\"child-select\">Izaberite dete:</label>\r\n        <select v-model=\"selectedChildId\" @change=\"onChildSelect\">\r\n          <option v-for=\"child in children\" :key=\"child._id\" :value=\"child._id\">\r\n            {{ child.name }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n\r\n      <!-- Tabela sa podacima o razvoju -->\r\n      <h3>Razvoj deteta</h3>\r\n      <table class=\"development-table\">\r\n        <thead>\r\n          <tr>\r\n            <th>Period</th>\r\n            <th>Fizički razvoj</th>\r\n            <th>Emocionalni razvoj</th>\r\n            <th>Intelektualni razvoj</th>\r\n            <th>Socijalni razvoj</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"period in developmentData\" :key=\"period.startAge\">\r\n            <td>{{ period.startAge }} - {{ period.endAge }} meseci</td>\r\n            <td>{{ period.physicalDevelopment || '-' }}</td>\r\n            <td>{{ period.emotionalDevelopment || '-' }}</td>\r\n            <td>{{ period.intellectualDevelopment || '-' }}</td>\r\n            <td>{{ period.socialDevelopment || '-' }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n\r\n      <!-- Tabela sa milestone podacima -->\r\n      <h3>Ključni momenti</h3>\r\n      <table class=\"milestone-table\">\r\n        <thead>\r\n          <tr>\r\n            <th>Tip momenta</th>\r\n            <th>Datum</th>\r\n            <th>Napredak</th>\r\n            <th>Napomene</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"milestone in milestoneData\" :key=\"milestone._id\">\r\n            <td>{{ milestone.milestoneType }}</td>\r\n            <td>{{ formatDate(milestone.milestoneDate) }}</td>\r\n            <td>{{ milestone.progress || '-' }}</td>\r\n            <td>{{ milestone.notes || '-' }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n\r\n      <!-- Dugme za preuzimanje PDF-a -->\r\n      <button @click=\"downloadPDF\" class=\"pdf-button\">Preuzmi PDF</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport jsPDF from \"jspdf\";\r\nimport \"jspdf-autotable\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      children: [],\r\n      selectedChildId: \"\",\r\n      child: {},\r\n      developmentData: [],\r\n      milestoneData: [],\r\n      loading: true,\r\n      errorMessage: \"\",\r\n    };\r\n  },\r\n  async created() {\r\n    await this.fetchChildren();\r\n  },\r\n  methods: {\r\n    async fetchChildren() {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\"http://localhost:5000/api\", {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n\r\n        this.children = response.data;\r\n        console.log(\"Lista dece učitana:\", this.children);\r\n\r\n        if (this.children.length > 0) {\r\n          this.selectedChildId = this.children[0]._id;\r\n          await this.fetchChildData(this.selectedChildId);\r\n          await this.fetchDevelopmentData(this.selectedChildId);\r\n          await this.fetchMilestones(this.selectedChildId);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri učitavanju dece:\", error);\r\n        this.errorMessage = \"Nije moguće učitati listu dece.\";\r\n      }\r\n    },\r\n\r\n    async fetchChildData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n\r\n        if (!response.data || Object.keys(response.data).length === 0) {\r\n          this.errorMessage = \"Podaci o detetu nisu pronađeni.\";\r\n          return;\r\n        }\r\n\r\n        this.child = response.data;\r\n        console.log(\"Podaci o detetu:\", this.child);\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\r\n        this.errorMessage = \"Nije moguće učitati podatke o detetu.\";\r\n      }\r\n    },\r\n\r\n    async fetchDevelopmentData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        this.developmentData = response.data || [];\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri dobijanju podataka o razvoju:\", error);\r\n        this.errorMessage = \"Nije moguće učitati razvojne podatke.\";\r\n      }\r\n    },\r\n\r\n    async fetchMilestones(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/milestones/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        this.milestoneData = response.data || [];\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri dobijanju ključnih momenata:\", error);\r\n      }\r\n    },\r\n\r\n    onChildSelect() {\r\n      console.log(\"Odabrano dete ID:\", this.selectedChildId);\r\n      this.fetchChildData(this.selectedChildId);\r\n      this.fetchDevelopmentData(this.selectedChildId);\r\n      this.fetchMilestones(this.selectedChildId);\r\n    },\r\n\r\n    downloadPDF() {\r\n      const doc = new jsPDF();\r\n      doc.text(`Razvoj deteta: ${this.child.name}`, 10, 10);\r\n\r\n      const developmentTable = [\r\n        [\"Period\", \"Fizički\", \"Emocionalni\", \"Intelektualni\", \"Socijalni\"],\r\n        ...this.developmentData.map((period) => [\r\n          `${period.startAge}-${period.endAge} meseci`,\r\n          period.physicalDevelopment || \"-\",\r\n          period.emotionalDevelopment || \"-\",\r\n          period.intellectualDevelopment || \"-\",\r\n          period.socialDevelopment || \"-\",\r\n        ]),\r\n      ];\r\n\r\n      const milestoneTable = [\r\n        [\"Tip momenta\", \"Datum\", \"Napredak\", \"Napomene\"],\r\n        ...this.milestoneData.map((milestone) => [\r\n          milestone.milestoneType,\r\n          this.formatDate(milestone.milestoneDate),\r\n          milestone.progress || \"-\",\r\n          milestone.notes || \"-\",\r\n        ]),\r\n      ];\r\n\r\n      doc.autoTable({ head: [developmentTable[0]], body: developmentTable.slice(1), startY: 20 });\r\n      doc.autoTable({ head: [milestoneTable[0]], body: milestoneTable.slice(1), startY: doc.lastAutoTable.finalY + 10 });\r\n      doc.save(`Razvoj_${this.child.name}.pdf`);\r\n    },\r\n\r\n    formatDate(date) {\r\n      return new Date(date).toLocaleDateString(\"sr-RS\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n$primary-color: #007bff;\r\n$secondary-color: #28a745;\r\n$danger-color: red;\r\n$light-gray: #f9f9f9;\r\n$border-gray: #ddd;\r\n$dark-text: #333;\r\n$hover-primary: #0056b3;\r\n$hover-secondary: #218838;\r\n\r\n.overview-container {\r\n  max-width: 900px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: $light-gray;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\nh2 {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n  font-size: 1.8rem;\r\n  color: $dark-text;\r\n}\r\n\r\n.development-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  background: white;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n\r\n  th,\r\n  td {\r\n    border: 1px solid $border-gray;\r\n    padding: 12px;\r\n    text-align: left;\r\n  }\r\n\r\n  th {\r\n    background: $primary-color;\r\n    color: white;\r\n    font-size: 1.1rem;\r\n  }\r\n\r\n  tr:nth-child(even) {\r\n    background-color: #f2f2f2;\r\n  }\r\n\r\n  tr:hover {\r\n    background-color: rgba(0, 123, 255, 0.1);\r\n  }\r\n}\r\n\r\n.error-message {\r\n  color: $danger-color;\r\n  text-align: center;\r\n  font-weight: bold;\r\n  font-size: 1.1rem;\r\n  margin-top: 10px;\r\n}\r\n\r\n.pdf-button {\r\n  disp