{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  props: {\n    childId: {\n      type: String,\n      required: true\n    },\n    milestoneId: {\n      type: String,\n      default: null\n    }\n  },\n  data() {\n    return {\n      milestone: {\n        milestoneType: '',\n        milestoneDate: '',\n        progress: '',\n        notes: '',\n        childId: this.childId\n      },\n      childBirthDate: null,\n      isEditing: false,\n      periodInfo: null\n    };\n  },\n  computed: {\n    minDate() {\n      if (!this.childBirthDate || !this.periodInfo) return '';\n      const date = new Date(this.childBirthDate);\n      date.setMonth(date.getMonth() + this.periodInfo.start);\n      return date.toISOString().split('T')[0];\n    },\n    maxDate() {\n      if (!this.childBirthDate || !this.periodInfo) return '';\n      const date = new Date(this.childBirthDate);\n      date.setMonth(date.getMonth() + this.periodInfo.end);\n      return date.toISOString().split('T')[0];\n    }\n  },\n  async created() {\n    // Get period from query params\n    const startAge = parseInt(this.$route.query.startAge);\n    const endAge = parseInt(this.$route.query.endAge);\n    if (!isNaN(startAge) && !isNaN(endAge)) {\n      this.periodInfo = {\n        start: startAge,\n        end: endAge\n      };\n      console.log('Period info set:', this.periodInfo);\n    } else {\n      console.error('Invalid period information');\n    }\n    await this.fetchChildData();\n    const milestoneId = this.$route.params.milestoneId;\n    if (milestoneId && milestoneId !== 'undefined') {\n      this.isEditing = true;\n      await this.fetchMilestone(milestoneId);\n    }\n  },\n  methods: {\n    async fetchChildData() {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/children/${this.childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.childBirthDate = response.data.birthDate;\n        console.log('Child birth date:', this.childBirthDate);\n      } catch (error) {\n        console.error('Error fetching child data:', error);\n        if (error.response) {\n          console.error('Server error:', error.response.data);\n        }\n      }\n    },\n    async fetchMilestone(id) {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/milestones/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (response.data) {\n          const fetchedMilestone = response.data;\n          this.milestone = {\n            milestoneType: fetchedMilestone.milestoneType || '',\n            milestoneDate: fetchedMilestone.milestoneDate ? new Date(fetchedMilestone.milestoneDate).toISOString().split('T')[0] : '',\n            progress: fetchedMilestone.progress || '',\n            notes: fetchedMilestone.notes || '',\n            childId: this.childId\n          };\n        }\n      } catch (error) {\n        console.error('Error fetching milestone:', error);\n      }\n    },\n    async submitForm() {\n      try {\n        const token = sessionStorage.getItem('token');\n        if (!token) {\n          this.$router.push('/login');\n          return;\n        }\n\n        // Make sure we have period information\n        if (!this.periodInfo) {\n          console.error('No period information available');\n          return;\n        }\n\n        // Create milestone data with all required fields\n        const milestoneData = {\n          childId: this.childId,\n          milestoneType: this.milestone.milestoneType,\n          milestoneDate: this.milestone.milestoneDate,\n          progress: this.milestone.progress,\n          notes: this.milestone.notes || '',\n          periodStart: parseInt(this.periodInfo.start),\n          periodEnd: parseInt(this.periodInfo.end)\n        };\n        console.log('Submitting milestone data:', milestoneData);\n        const headers = {\n          Authorization: `Bearer ${token}`\n        };\n        let response;\n        if (this.isEditing) {\n          response = await axios.put(`http://localhost:5000/api/milestones/${this.$route.params.milestoneId}`, milestoneData, {\n            headers\n          });\n        } else {\n          response = await axios.post('http://localhost:5000/api/milestones', milestoneData, {\n            headers\n          });\n        }\n        console.log('Server response:', response.data);\n\n        // Navigate back to the development list with the period query parameters\n        this.$router.push({\n          path: `/child-development-list/${this.childId}`,\n          query: {\n            startAge: this.periodInfo.start,\n            endAge: this.periodInfo.end\n          }\n        });\n      } catch (error) {\n        console.error('Error saving milestone:', error);\n        if (error.response) {\n          console.error('Server error:', error.response.data);\n        }\n      }\n    },\n    goBack() {\n      this.$router.push(`/child-development-list/${this.childId}`);\n    },\n    addMilestone() {\n      if (!this.selectedPeriod) return;\n      this.$router.push({\n        path: `/milestone-form/${this.$route.params.childId}`,\n        query: {\n          startAge: this.selectedPeriod.start,\n          endAge: this.selectedPeriod.end\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","props","childId","type","String","required","milestoneId","default","data","milestone","milestoneType","milestoneDate","progress","notes","childBirthDate","isEditing","periodInfo","computed","minDate","date","Date","setMonth","getMonth","start","toISOString","split","maxDate","end","created","startAge","parseInt","$route","query","endAge","isNaN","console","log","error","fetchChildData","params","fetchMilestone","methods","token","sessionStorage","getItem","response","get","headers","Authorization","birthDate","id","fetchedMilestone","submitForm","$router","push","milestoneData","periodStart","periodEnd","put","post","path","goBack","addMilestone","selectedPeriod"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\MilestoneForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"milestone-form-container\">\r\n    <h2>{{ isEditing ? 'Izmeni Kljucni Momenat' : 'Dodaj Kljucni Momenat' }}</h2>\r\n    \r\n    <form @submit.prevent=\"submitForm\" class=\"milestone-form\">\r\n      <div v-if=\"periodInfo\" class=\"period-info\">\r\n        <h3>Period: {{ periodInfo.start }}-{{ periodInfo.end }} meseci</h3>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"milestoneType\">Tip Kljucnog Momenta:</label>\r\n        <input \r\n          v-model=\"milestone.milestoneType\" \r\n          type=\"text\" \r\n          id=\"milestoneType\" \r\n          required\r\n          placeholder=\"Npr. Prvi korak, Prva reč...\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"milestoneDate\">Datum:</label>\r\n        <input \r\n          v-model=\"milestone.milestoneDate\" \r\n          type=\"date\" \r\n          id=\"milestoneDate\" \r\n          required\r\n          :min=\"minDate\"\r\n          :max=\"maxDate\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"progress\">Napredak:</label>\r\n        <textarea \r\n          v-model=\"milestone.progress\" \r\n          id=\"progress\" \r\n          required\r\n          placeholder=\"Opišite napredak...\"\r\n        ></textarea>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"notes\">Napomene:</label>\r\n        <textarea \r\n          v-model=\"milestone.notes\" \r\n          id=\"notes\" \r\n          placeholder=\"Dodatne napomene...\"\r\n        ></textarea>\r\n      </div>\r\n\r\n      <div class=\"button-group\">\r\n        <button type=\"submit\" class=\"submit-btn\">\r\n          {{ isEditing ? 'Sačuvaj Izmene' : 'Dodaj Kljucni Momenat' }}\r\n        </button>\r\n        <button type=\"button\" @click=\"goBack\" class=\"cancel-btn\">Otkaži</button>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: {\r\n    childId: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    milestoneId: {\r\n      type: String,\r\n      default: null\r\n    }\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      milestone: {\r\n        milestoneType: '',\r\n        milestoneDate: '',\r\n        progress: '',\r\n        notes: '',\r\n        childId: this.childId\r\n      },\r\n      childBirthDate: null,\r\n      isEditing: false,\r\n      periodInfo: null\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    minDate() {\r\n      if (!this.childBirthDate || !this.periodInfo) return '';\r\n      const date = new Date(this.childBirthDate);\r\n      date.setMonth(date.getMonth() + this.periodInfo.start);\r\n      return date.toISOString().split('T')[0];\r\n    },\r\n\r\n    maxDate() {\r\n      if (!this.childBirthDate || !this.periodInfo) return '';\r\n      const date = new Date(this.childBirthDate);\r\n      date.setMonth(date.getMonth() + this.periodInfo.end);\r\n      return date.toISOString().split('T')[0];\r\n    }\r\n  },\r\n\r\n  async created() {\r\n    // Get period from query params\r\n    const startAge = parseInt(this.$route.query.startAge);\r\n    const endAge = parseInt(this.$route.query.endAge);\r\n    \r\n    if (!isNaN(startAge) && !isNaN(endAge)) {\r\n      this.periodInfo = { start: startAge, end: endAge };\r\n      console.log('Period info set:', this.periodInfo);\r\n    } else {\r\n      console.error('Invalid period information');\r\n    }\r\n\r\n    await this.fetchChildData();\r\n\r\n    const milestoneId = this.$route.params.milestoneId;\r\n    if (milestoneId && milestoneId !== 'undefined') {\r\n      this.isEditing = true;\r\n      await this.fetchMilestone(milestoneId);\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    async fetchChildData() {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/children/${this.childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        this.childBirthDate = response.data.birthDate;\r\n        console.log('Child birth date:', this.childBirthDate);\r\n      } catch (error) {\r\n        console.error('Error fetching child data:', error);\r\n        if (error.response) {\r\n          console.error('Server error:', error.response.data);\r\n        }\r\n      }\r\n    },\r\n\r\n    async fetchMilestone(id) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/milestones/${id}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        \r\n        if (response.data) {\r\n          const fetchedMilestone = response.data;\r\n          this.milestone = {\r\n            milestoneType: fetchedMilestone.milestoneType || '',\r\n            milestoneDate: fetchedMilestone.milestoneDate ? \r\n              new Date(fetchedMilestone.milestoneDate).toISOString().split('T')[0] : '',\r\n            progress: fetchedMilestone.progress || '',\r\n            notes: fetchedMilestone.notes || '',\r\n            childId: this.childId\r\n          };\r\n        }\r\n      } catch (error) {\r\n        console.error('Error fetching milestone:', error);\r\n      }\r\n    },\r\n\r\n    async submitForm() {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        if (!token) {\r\n          this.$router.push('/login');\r\n          return;\r\n        }\r\n\r\n        // Make sure we have period information\r\n        if (!this.periodInfo) {\r\n          console.error('No period information available');\r\n          return;\r\n        }\r\n\r\n        // Create milestone data with all required fields\r\n        const milestoneData = {\r\n          childId: this.childId,\r\n          milestoneType: this.milestone.milestoneType,\r\n          milestoneDate: this.milestone.milestoneDate,\r\n          progress: this.milestone.progress,\r\n          notes: this.milestone.notes || '',\r\n          periodStart: parseInt(this.periodInfo.start),\r\n          periodEnd: parseInt(this.periodInfo.end)\r\n        };\r\n\r\n        console.log('Submitting milestone data:', milestoneData);\r\n\r\n        const headers = { Authorization: `Bearer ${token}` };\r\n        \r\n        let response;\r\n        if (this.isEditing) {\r\n          response = await axios.put(\r\n            `http://localhost:5000/api/milestones/${this.$route.params.milestoneId}`,\r\n            milestoneData,\r\n            { headers }\r\n          );\r\n        } else {\r\n          response = await axios.post(\r\n            'http://localhost:5000/api/milestones',\r\n            milestoneData,\r\n            { headers }\r\n          );\r\n        }\r\n\r\n        console.log('Server response:', response.data);\r\n\r\n        // Navigate back to the development list with the period query parameters\r\n        this.$router.push({\r\n          path: `/child-development-list/${this.childId}`,\r\n          query: {\r\n            startAge: this.periodInfo.start,\r\n            endAge: this.periodInfo.end\r\n          }\r\n        });\r\n      } catch (error) {\r\n        console.error('Error saving milestone:', error);\r\n        if (error.response) {\r\n          console.error('Server error:', error.response.data);\r\n        }\r\n      }\r\n    },\r\n\r\n    goBack() {\r\n      this.$router.push(`/child-development-list/${this.childId}`);\r\n    },\r\n\r\n    addMilestone() {\r\n      if (!th