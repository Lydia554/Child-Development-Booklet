{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      childName: null,\n      childAge: null,\n      availablePeriods: []\n    };\n  },\n  methods: {\n    async fetchChildData(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.childName = response.data.name;\n        this.childAge = this.calculateAgeInMonths(new Date(response.data.birthDate));\n      } catch (error) {\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\n      }\n    },\n    calculateAgeInMonths(birthDate) {\n      const birth = new Date(birthDate);\n      const now = new Date();\n      return (now.getFullYear() - birth.getFullYear()) * 12 + (now.getMonth() - birth.getMonth());\n    },\n    generateAvailablePeriods() {\n      if (this.childAge === null) {\n        console.warn(\"⚠️ Starost deteta nije postavljena, preskačem generisanje perioda.\");\n        return;\n      }\n      this.availablePeriods = [];\n      for (let i = 0; i <= 72; i += 6) {\n        this.availablePeriods.push({\n          start: i,\n          end: i + 6\n        });\n      }\n    },\n    selectPeriod(period) {\n      this.$emit(\"period-selected\", period);\n    },\n    formatPeriod(start, end) {\n      return `${start}-${end} meseci`;\n    },\n    getPeriodClass(period) {\n      return {\n        \"period-button\": true\n      };\n    }\n  },\n  async created() {\n    const childId = this.$route.params.childId;\n    if (childId) {\n      await this.fetchChildData(childId);\n      this.generateAvailablePeriods();\n    }\n  }\n};","map":{"version":3,"names":["axios","data","childName","childAge","availablePeriods","methods","fetchChildData","childId","token","sessionStorage","getItem","response","get","headers","Authorization","name","calculateAgeInMonths","Date","birthDate","error","console","birth","now","getFullYear","getMonth","generateAvailablePeriods","warn","i","push","start","end","selectPeriod","period","$emit","formatPeriod","getPeriodClass","created","$route","params"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentPeriod.vue"],"sourcesContent":["<template>\r\n  <div class=\"development-container\">\r\n    <h2>Praćenje Razvoja za {{ childName || \"Nepoznato dete\" }}</h2>\r\n\r\n    <!-- Period Selector -->\r\n    <div class=\"period-selector\">\r\n      <h3>Izaberite period:</h3>\r\n      <div class=\"periods-list\">\r\n        <button v-for=\"period in availablePeriods\"\r\n                :key=\"`${period.start}-${period.end}`\"\r\n                :class=\"getPeriodClass(period)\"\r\n                @click=\"selectPeriod(period)\">\r\n          {{ formatPeriod(period.start, period.end) }}\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      childName: null,\r\n      childAge: null,\r\n      availablePeriods: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    async fetchChildData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/children/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        this.childName = response.data.name;\r\n        this.childAge = this.calculateAgeInMonths(new Date(response.data.birthDate));\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\r\n      }\r\n    },\r\n\r\n    calculateAgeInMonths(birthDate) {\r\n      const birth = new Date(birthDate);\r\n      const now = new Date();\r\n      return (now.getFullYear() - birth.getFullYear()) * 12 + (now.getMonth() - birth.getMonth());\r\n    },\r\n\r\n    generateAvailablePeriods() {\r\n      if (this.childAge === null) {\r\n        console.warn(\"⚠️ Starost deteta nije postavljena, preskačem generisanje perioda.\");\r\n        return;\r\n      }\r\n\r\n      this.availablePeriods = [];\r\n      for (let i = 0; i <= 72; i += 6) {\r\n        this.availablePeriods.push({ start: i, end: i + 6 });\r\n      }\r\n    },\r\n\r\n    selectPeriod(period) {\r\n      this.$emit(\"period-selected\", period);\r\n    },\r\n\r\n    formatPeriod(start, end) {\r\n      return `${start}-${end} meseci`;\r\n    },\r\n\r\n    getPeriodClass(period) {\r\n      return {\r\n        \"period-button\": true,\r\n      };\r\n    }\r\n  },\r\n\r\n  async created() {\r\n    const childId = this.$route.params.childId;\r\n    if (childId) {\r\n      await this.fetchChildData(childId);\r\n      this.generateAvailablePeriods();\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.development-container {\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.period-selector h3 {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.periods-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  gap: 10px;\r\n}\r\n\r\n.period-button {\r\n  padding: 12px 18px;\r\n  font-size: 18px;\r\n  border: none;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n  background-color: #ddd;\r\n  transition: 0.3s;\r\n}\r\n\r\n.period-button:hover {\r\n  transform: scale(1.05);\r\n}\r\n</style>\r\n"],"mappings":";AAoBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,QAAQ,EAAE,IAAI;MACdC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACP,MAAMC,cAAcA,CAACC,OAAO,EAAE;MAC5B,IAAI;QACF,MAAMC,KAAI,GAAIC,cAAc,CAACC,OAAO,CAAC,OAAO,CAAC;QAC7C,MAAMC,QAAO,GAAI,MAAMX,KAAK,CAACY,GAAG,CAC9B,sCAAsCL,OAAO,EAAE,EAC/C;UAAEM,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUN,KAAK;UAAG;QAAE,CAClD,CAAC;QACD,IAAI,CAACN,SAAQ,GAAIS,QAAQ,CAACV,IA