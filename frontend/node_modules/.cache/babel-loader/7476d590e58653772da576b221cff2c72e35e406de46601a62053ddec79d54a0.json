{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      periodRecords: [],\n      childName: \"\",\n      selectedPeriod: null,\n      availablePeriods: []\n    };\n  },\n  computed: {\n    sortedRecords() {\n      return [...this.periodRecords].sort((a, b) => a.startAge - b.startAge);\n    }\n  },\n  created() {\n    const childId = this.$route.params.childId;\n    if (childId) {\n      this.generateAvailablePeriods();\n      this.fetchDevelopmentData(childId);\n      this.fetchChildName(childId);\n    } else {\n      console.error('No child ID found');\n      this.$router.push('/child-registration');\n    }\n  },\n  methods: {\n    generateAvailablePeriods() {\n      // Generate periods from 0 to 6 years (72 months) in 6-month intervals\n      const periods = [];\n      for (let i = 0; i <= 72; i += 6) {\n        periods.push({\n          start: i,\n          end: i + 6\n        });\n      }\n      this.availablePeriods = periods;\n      this.selectedPeriod = periods[0]; // Select first period by default\n    },\n    formatPeriod(start, end) {\n      if (start === 0) {\n        return `0-6 meseci`;\n      }\n      return `${start}-${end} meseci`;\n    },\n    async fetchDevelopmentData(childId) {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.periodRecords = response.data.periods || [];\n      } catch (error) {\n        console.error('Error fetching development data:', error);\n      }\n    },\n    async fetchChildName(childId) {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.childName = response.data.name;\n      } catch (error) {\n        console.error('Error fetching child name:', error);\n      }\n    },\n    async handlePeriodClick(period) {\n      this.selectedPeriod = period;\n      await this.fetchMilestonesForPeriod(period);\n    },\n    async fetchMilestonesForPeriod(period) {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/milestones/${this.$route.params.childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.milestoneData = response.data.filter(milestone => milestone.milestoneDate >= new Date(this.calculateStartDate(period.start)) && milestone.milestoneDate < new Date(this.calculateEndDate(period.end)));\n      } catch (error) {\n        console.error('Error fetching milestones:', error);\n      }\n    },\n    calculateStartDate(months) {\n      const birthDate = new Date(this.childBirthDate);\n      birthDate.setMonth(birthDate.getMonth() + months);\n      return birthDate;\n    },\n    calculateEndDate(months) {\n      const birthDate = new Date(this.childBirthDate);\n      birthDate.setMonth(birthDate.getMonth() + months);\n      return birthDate;\n    },\n    isPeriodFilled(period) {\n      return this.periodRecords.some(record => record.startAge === period.start && record.endAge === period.end);\n    }\n  }\n};","map":{"version":3,"names":["axios","data","periodRecords","childName","selectedPeriod","availablePeriods","computed","sortedRecords","sort","a","b","startAge","created","childId","$route","params","generateAvailablePeriods","fetchDevelopmentData","fetchChildName","console","error","$router","push","methods","periods","i","start","end","formatPeriod","token","ses