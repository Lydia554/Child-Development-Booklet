{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport axios from \"axios\";\nexport default {\n  props: {\n    selectedPeriod: Object // ✅ Receives period from parent\n  },\n  data() {\n    return {\n      childName: null,\n      childAge: null,\n      developmentData: [],\n      milestoneData: []\n    };\n  },\n  computed: {\n    currentPeriodDevelopment() {\n      return this.selectedPeriod ? this.developmentData.find(d => d.startAge === this.selectedPeriod.start && d.endAge === this.selectedPeriod.end) : null;\n    },\n    currentPeriodMilestones() {\n      return this.selectedPeriod ? this.milestoneData.filter(m => m.age >= this.selectedPeriod.start && m.age < this.selectedPeriod.end) : [];\n    }\n  },\n  methods: {\n    async fetchChildData(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.childName = response.data.name;\n        this.childAge = this.calculateAgeInMonths(response.data.birthDate);\n      } catch (error) {\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\n      }\n    },\n    async fetchDevelopmentData(childId) {\n      const token = sessionStorage.getItem(\"token\");\n      const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\n        headers: {\n          Authorization: `Bearer ${token}`\n        }\n      });\n      this.developmentData = response.data;\n    },\n    async fetchMilestones(childId) {\n      const token = sessionStorage.getItem(\"token\");\n      const response = await axios.get(`http://localhost:5000/api/milestones/${childId}`, {\n        headers: {\n          Authorization: `Bearer ${token}`\n        }\n      });\n      this.milestoneData = response.data;\n    }\n  },\n  async created() {\n    const childId = this.$route.params.childId;\n    if (childId) {\n      await this.fetchChildData(childId);\n      await this.fetchDevelopmentData(childId);\n      await this.fetchMilestones(childId);\n    }\n  }\n};","map":{"version":3,"names":["axios","props","selectedPeriod","Object","data","childName","childAge","developmentData","milestoneData","computed","currentPeriodDevelopment","find","d","startAge","start","endAge","end","currentPeriodMilestones","filter","m","age","methods","fetchChildData","childId","token","sessionStorage","getItem","response","get","headers","Authorization","name","calculateAgeInMonths","birthDate","error","console","fetchDevelopmentData","fetchMilestones","created","$route","params"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentList.vue"],"sourcesContent":["<template>\r\n  <div class=\"development-container\">\r\n    <h2>Pregled Razvoja Deteta</h2>\r\n\r\n    <!-- Child Info -->\r\n    <div class=\"child-info\">\r\n      <h3>{{ childName ? childName : 'Nepoznato dete' }}</h3>\r\n      <p><strong>Starost:</strong> {{ childAge ? childAge + ' meseci' : 'Nepoznata starost' }}</p>\r\n    </div>\r\n\r\n    <!-- Display Selected Period Data -->\r\n    <div v-if=\"selectedPeriod\" class=\"period-records\">\r\n      <h3>Razvoj za period: {{ formatPeriod(selectedPeriod.start, selectedPeriod.end) }}</h3>\r\n\r\n      <!-- Development Data -->\r\n      <div v-if=\"currentPeriodDevelopment\" class=\"development-section\">\r\n        <h4>Razvoj deteta</h4>\r\n        <ul>\r\n          <li><strong>Fizički:</strong> {{ currentPeriodDevelopment.physicalDevelopment || \"Nema podataka\" }}</li>\r\n          <li><strong>Emocionalni:</strong> {{ currentPeriodDevelopment.emotionalDevelopment || \"Nema podataka\" }}</li>\r\n          <li><strong>Intelektualni:</strong> {{ currentPeriodDevelopment.intellectualDevelopment || \"Nema podataka\" }}</li>\r\n          <li><strong>Socijalni:</strong> {{ currentPeriodDevelopment.socialDevelopment || \"Nema podataka\" }}</li>\r\n        </ul>\r\n      </div>\r\n\r\n      <!-- Milestones Data -->\r\n      <div v-if=\"currentPeriodMilestones.length\" class=\"milestones-section\">\r\n        <h4>Prekretnice za ovaj period</h4>\r\n        <div class=\"milestone-list\">\r\n          <div v-for=\"milestone in currentPeriodMilestones\" :key=\"milestone._id\" class=\"milestone-item\">\r\n            <h3>{{ milestone.milestoneType }} - {{ formatDate(milestone.milestoneDate) }}</h3>\r\n            <p><strong>Napredak:</strong> {{ milestone.progress }}</p>\r\n            <p><strong>Napomene:</strong> {{ milestone.notes }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div v-else class=\"no-records\">\r\n        <p>Nema unetih podataka za ovaj period.</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  props: {\r\n    selectedPeriod: Object // ✅ Receives period from parent\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      childName: null,\r\n      childAge: null,\r\n      developmentData: [],\r\n      milestoneData: [],\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    currentPeriodDevelopment() {\r\n      return this.selectedPeriod\r\n        ? this.developmentData.find(\r\n            (d) =>\r\n              d.startAge === this.selectedPeriod.start &&\r\n              d.endAge === this.selectedPeriod.end\r\n          )\r\n        : null;\r\n    },\r\n\r\n    currentPeriodMilestones() {\r\n      return this.selectedPeriod\r\n        ? this.milestoneData.filter(\r\n            (m) =>\r\n              m.age >= this.selectedPeriod.start &&\r\n              m.age < this.selectedPeriod.end\r\n          )\r\n        : [];\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    async fetchChildData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/children/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        this.childName = response.data.name;\r\n        this.childAge = this.calculateAgeInMonths(response.data.birthDate);\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\r\n      }\r\n    },\r\n\r\n    async fetchDevelopmentData(childId) {\r\n      const token = sessionStorage.getItem(\"token\");\r\n      const response = await axios.get(\r\n        `http://localhost:5000/api/child-development/${childId}`,\r\n        { headers: { Authorization: `Bearer ${token}` } }\r\n      );\r\n      this.developmentData = response.data;\r\n    },\r\n\r\n    async fetchMilestones(childId) {\r\n      const token = sessionStorage.getItem(\"token\");\r\n      const response = await axios.get(\r\n        `http://localhost:5000/api/milestones/${childId}`,\r\n        { headers: { Authorization: `Bearer ${token}` } }\r\n      );\r\n      this.milestoneData = response.data;\r\n    }\r\n  },\r\n\r\n  async created() {\r\n    const childId = this.$route.params.childId;\r\n    if (childId) {\r\n      await this.fetchChildData(childId);\r\n      await this.fetchDevelopmentData(childId);\r\n      await this.fetchMilestones(childId);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.development-container {\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.child-info {\r\n  background: #f8f9fa;\r\n  padding: 15px;\r\n  border-radius: 10px;\r\n  margin-bottom: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.error-message {\r\n  color: red;\r\n  background-color: #fee;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n}\r\n\r\n.available-periods h3 {\r\n  margin-bottom: 10px;\r\n  color: #1976d2;\r\n}\r\n\r\n/* Centered Periods */\r\n.periods-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(140px, 1fr));\r\n  gap: 10px;\r\n  justify-content: center;\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.period-item {\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  text-align: center;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  transition: 0.3s;\r\n  cursor: pointer;\r\n}\r\n\r\n.period-item.current {\r\n  background: #4caf50;\r\n  color: white;\r\n}\r\n\r\n.period-item.past {\r\n  background: #ffcc80;\r\n}\r\n\r\n.period-item.future {\r\n  background: #eeeeee;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.period-status {\r\n  font-size: 12px;\r\n  display: block;\r\n}\r\n\r\n.action-buttons {\r\n  margin-top: 15px;\r\n}\r\n\r\n.button {\r\n  display: inline-block;\r\n  padding: 8px 12px;\r\n  background: #1976d2;\r\n  color: white;\r\n  border-radius: 5px;\r\n  text-decoration: none;\r\n  margin: 5px;\r\n}\r\n</style>\r\n"],"mappings":";;;AA6CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE;IACLC,cAAc,EAAEC,MAAK,CAAE;EACzB,CAAC;EAEDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,QAAQ,EAAE,IAAI;MACdC,eAAe,EAAE,EAAE;MACnBC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EAEDC,QAAQ,EAAE;IACRC,wBAAwBA,CAAA,EAAG;MACzB,OAAO,IAAI,CAACR,cAAa,GACrB,IAAI,CAACK,eAAe,CAACI,IAAI,CACtBC,CAAC,IACAA,CAAC,CAACC,QAAO,KAAM,IAAI,CAACX,cAAc,CAACY,KAAI,IACvCF,CAAC,CAACG,MAAK,KAAM,IAAI,CAACb,cAAc,CAACc,GACrC,IACA,IAAI;IACV,CAAC;IAEDC,uBAAuBA,CAAA,EAAG;MACxB,OAAO,IAAI,CAACf,cAAa,GACrB,IAAI,CAACM,aAAa,CAACU,MAAM,CACtBC,CAAC,IACAA,CAAC,CAACC,GAAE,IAAK,IAAI,CAAClB,cAAc,CAACY,KAAI,IACjCK,CAAC,CAACC,GAAE,GAAI,IAAI,CAAClB,cAAc,CAACc,GAChC,IACA,EAAE;IACR;EACF,CAAC;EAEDK,OAAO,EAAE;IACP,MAAMC,cAAcA,CAACC,OAAO,EAAE;MAC5B,IAAI;QACF,MAAMC,KAAI,GAAIC,cAAc,CAACC,OAAO,CAAC,OAAO,CAAC;QAC7C,MAAMC,QAAO,GAAI,MAAM3B,KAAK,CAAC4B,GAAG,CAC9B,sCAAsCL,OAAO,EAAE,EAC/C;UAAEM,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUN,KAAK;UAAG;QAAE,CAClD,CAAC;QACD,IAAI,CAACnB,SAAQ,GAAIsB,QAAQ,CAACvB,IAAI,CAAC2B,IAAI;QACnC,IAAI,CAACzB,QAAO,GAAI,IAAI,CAAC0B,oBAAoB,CAACL,QAAQ,CAACvB,IAAI,CAAC6B,SAAS,CAAC;MACpE,EAAE,OAAOC,KAA