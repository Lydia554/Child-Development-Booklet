eriod: {{ formData.startAge }} - {{ formData.endAge }} meseci</h3>\r\n\r\n    <div v-if=\"error\" class=\"error-message\">\r\n      {{ error }}\r\n    </div>\r\n\r\n    <form @submit.prevent=\"saveDevelopment\">\r\n      <div class=\"form-group\">\r\n        <label for=\"physicalDevelopment\">Fiziƒçki razvoj:</label>\r\n        <textarea id=\"physicalDevelopment\" v-model=\"formData.physicalDevelopment\" required @input=\"validateInput('physicalDevelopment')\"></textarea>\r\n        <div v-if=\"physicalError\" class=\"error-message\">{{ physicalError }}</div>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"emotionalDevelopment\">Emocionalni razvoj:</label>\r\n        <textarea id=\"emotionalDevelopment\" v-model=\"formData.emotionalDevelopment\" required @input=\"validateInput('emotionalDevelopment')\"></textarea>\r\n        <div v-if=\"emotionalError\" class=\"error-message\">{{ emotionalError }}</div>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"intellectualDevelopment\">Intelektualni razvoj:</label>\r\n        <textarea id=\"intellectualDevelopment\" v-model=\"formData.intellectualDevelopment\" required @input=\"validateInput('intellectualDevelopment')\"></textarea>\r\n        <div v-if=\"intellectualError\" class=\"error-message\">{{ intellectualError }}</div>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"socialDevelopment\">Socijalni razvoj:</label>\r\n        <textarea id=\"socialDevelopment\" v-model=\"formData.socialDevelopment\" required @input=\"validateInput('socialDevelopment')\"></textarea>\r\n        <div v-if=\"socialError\" class=\"error-message\">{{ socialError }}</div>\r\n      </div>\r\n\r\n      <button type=\"submit\" :disabled=\"loading || hasErrors\">\r\n        {{ loading ? \"ƒåuvanje...\" : \"Saƒçuvaj\" }}\r\n      </button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useAuthStore } from \"@/stores/authStore\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      formData: {\r\n        startAge: null,\r\n        endAge: null,\r\n        physicalDevelopment: \"\",\r\n        emotionalDevelopment: \"\",\r\n        intellectualDevelopment: \"\",\r\n        socialDevelopment: \"\",\r\n      },\r\n      childBirthDate: null,\r\n      error: null,\r\n      loading: false,\r\n\r\n      physicalError: null,\r\n      emotionalError: null,\r\n      intellectualError: null,\r\n      socialError: null,\r\n\r\n      recommendedDevelopment: {\r\n        physicalDevelopment: { min: 0, max: 100 },\r\n        emotionalDevelopment: { min: 0, max: 100 },\r\n        intellectualDevelopment: { min: 0, max: 100 },\r\n        socialDevelopment: { min: 0, max: 100 },\r\n      },\r\n\r\n      hasErrors: false,\r\n    };\r\n  },\r\n\r\n  async created() {\r\n    console.log(\"üîÑ Component Created: Fetching child and period data...\");\r\n    await this.initializeForm();\r\n  },\r\n\r\n  methods: {\r\n    async initializeForm() {\r\n      const authStore = useAuthStore();\r\n      const token = authStore.token;\r\n      const childId = authStore.childId;\r\n\r\n      const startAge = parseInt(this.$route.query.startAge);\r\n      const endAge = parseInt(this.$route.query.endAge);\r\n\r\n      console.log(\"üì¢ Setting period:\", { startAge, endAge });\r\n\r\n      this.formData = {\r\n        startAge,\r\n        endAge,\r\n        physicalDevelopment: \"\",\r\n        emotionalDevelopment: \"\",\r\n        intellectualDevelopment: \"\",\r\n        socialDevelopment: \"\",\r\n      };\r\n\r\n      try {\r\n        console.log(\"üîç Fetching existing development data...\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n\r\n        if (response.data.length > 0) {\r\n          const existingPeriod = response.data.find(\r\n            (p) => p.startAge === startAge && p.endAge === endAge\r\n          );\r\n\r\n          if (existingPeriod) {\r\n            console.log(\"‚úÖ Found existing data for period:\", existingPeriod);\r\n            this.formData = { ...existingPeriod };\r\n          }\r\n        }\r\n      } catch (error) {\r\n        console.warn(\"‚ö†Ô∏è No existing development data found.\");\r\n      }\r\n    },\r\n\r\n    validateInput(field) {\r\n      this.hasErrors = false;\r\n      const inputValue = this.formData[field].trim().length;\r\n\r\n      if (inputValue < 5) {\r\n        this[`${field}Error`] = `Polje ${field} mora imati najmanje 5 karaktera.`;\r\n        this.hasErrors = true;\r\n      } else {\r\n        this[`${field}Error`] = null;\r\n      }\r\n    },\r\n\r\n    async saveDevelopment() {\r\n      if (this.hasErrors) {\r\n        this.error = \"Molimo ispravite gre≈°ke pre nego ≈°to po≈°aljete podatke.\";\r\n        return;\r\n      }\r\n\r\n      try {\r\n        this.loading = true;\r\n        const authStore = useAuthStore();\r\n        const childId = authStore.childId;\r\n        const token = authStore.token;\r\n\r\n        console.log(\"üì§ Sending development data:\", this.formData);\r\n\r\n        const response = await axios.post(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          this.formData,\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${token}`,\r\n              \"Content-Type\": \"application/json\",\r\n            },\r\n          }\r\n        );\r\n\r\n        console.log(\"‚úÖ Development data saved:\", response.data);\r\n\r\n        this.$router.push(`/child-development-list/${childId}`);\r\n      } catch (error) {\r\n        console.error(\"‚ùå Error saving development:\", error);\r\n        this.error = \"Gre≈°ka pri ƒçuvanju podataka.\";\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.error-message {\r\n  color: red;\r\n  font-size: 14px;\r\n}\r\n</style>\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n$primary-color: #4CAF50;\r\n$primary-disabled: #cccccc;\r\n$error-bg: #ffebee;\r\n$error-text: red;\r\n$border-color: #ddd;\r\n$text-color: #333;\r\n$background-light: #ffffff;\r\n\r\n.development-form {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 20px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n  font-weight: bold;\r\n  color: $text-color;\r\n  font-size: 1rem;\r\n}\r\n\r\ntextarea {\r\n  width: 100%;\r\n  min-height: 120px;\r\n  padding: 12px;\r\n  border: 1px solid $border-color;\r\n  border-radius: 4px;\r\n  font-size: 1rem;\r\n  resize: vertical;\r\n}\r\n\r\nbutton {\r\n  padding: 12px 20px;\r\n  background-color: $primary-color;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  font-size: 1rem;\r\n  transition: background-color 0.3s ease;\r\n\r\n  &:hover {\r\n    background-color: darken($primary-color, 10%);\r\n  }\r\n\r\n  &:disabled {\r\n    background-color: $primary-disabled;\r\n    cursor: not-allowed;\r\n  }\r\n}\r\n\r\n.error-message {\r\n  color: $error-text;\r\n  margin-bottom: 20px;\r\n  padding: 10px;\r\n  background-color: $error-bg;\r\n  border-radius: 4px;\r\n  text-align: center;\r\n}\r\n\r\n/* üì± Mobile Styles: Optimized for Phones */\r\n@media (max-width: 768px) {\r\n  .development-form {\r\n    padding: 15px;\r\n  }\r\n\r\n  label {\r\n    font-size: 0.95rem;\r\n  }\r\n\r\n  textarea {\r\n    font-size: 0.95rem;\r\n    padding: 10px;\r\n  }\r\n\r\n  button {\r\n    font-size: 1rem;\r\n    padding: 14px;\r\n    width: 100%;\r\n  }\r\n}\r\n\r\n/* üì± Extra Small Mobile Screens (Max 480px) */\r\n@media (max-width: 480px) {\r\n  .development-form {\r\n    padding: 12px;\r\n  }\r\n\r\n  label {\r\n    font-size: 0.9rem;\r\n  }\r\n\r\n  textarea {\r\n    font-size: 0.9rem;\r\n    padding: 10px;\r\n  }\r\n\r\n  button {\r\n    font-size: 0.95rem;\r\n    padding: 12px;\r\n  }\r\n}\r\n\r\n/* üì± Ultra-Small Mobile Screens (Max 360px) */\r\n@media (max-width: 360px) {\r\n  .development-form {\r\n    padding: 10px;\r\n  }\r\n\r\n  label {\r\n    font-size: 0.85rem;\r\n  }\r\n\r\n  textarea {\r\n    font-size: 0.85rem;\r\n    padding: 8px;\r\n  }\r\n\r\n  button {\r\n    font-size: 0.9rem;\r\n    padding: 10px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAkB;;EAD/BC,GAAA;EAKsBD,KAAK,EAAC;;;EAKjBA,KAAK,EAAC;AAAY;;EAV7BC,GAAA;EAakCD,KAAK,EAAC;;;EAG7BA,KAAK,EAAC;AAAY;;EAhB7BC,GAAA;EAmBmCD,KAAK,EAAC;;;EAG9BA,KAAK,EAAC;AAAY;;EAtB7BC,GAAA;EAyBsCD,KAAK,EAAC;;;EAGjCA,KAAK,EAAC;AAAY;;EA5B7BC,GAAA;EA+BgCD,KAAK,EAAC;;oBA/BtC;;uBACEE,mBAAA,CAqCM,OArCNC,UAqCM,G,4BApCJC,mBAAA,CAA4B,YAAxB,qBAAmB,sBACvBA,mBAAA,CAAgF,YAA5E,mBAAiB,GAAAC,gBAAA,CAAGC,KAAA,CAAAC,QAAQ,CAACC,QAAQ,IAAG,KAAG,GAAAH,gBAAA,CAAGC,KAAA,CAAAC,QAAQ,CAACE,MAAM,IAAG,SAAO,iBAEhEH,KAAA,CAAAI,KAAK,I,cAAhBR,mBAAA,CAEM,OAFNS,UAEM,EAAAN,gBAAA,CADDC,KAAA,CAAAI,KAAK,oBANdE,mBAAA,gBASIR,mBAAA,CA4BO;IA5BAS,QAAM,EAAAC,MAAA,QAAAA,MAAA,MATjBC,cAAA,KAAAC,IAAA,KAS2BC,QAAA,CAAAC,eAAA,IAAAD,QAAA,CAAAC,eAAA,IAAAF,IAAA,CAAe;MACpCZ,mBAAA,CAIM,OAJNe,UAIM,G,0BAHJf,mBAAA,CAAwD;IAAjDgB,GAAG,EAAC;EAAqB,GAAC,iBAAe,sB,gBAChDhB,mBAAA,CAA4I;IAAlIiB,EAAE,EAAC,qBAAqB;IAZ1C,uBAAAP,MAAA,QAAAA,MAAA,MAAAQ,MAAA,IAYoDhB,KAAA,CAAAC,QAAQ,CAACgB,mBAAmB,GAAAD,MAAA;IAAEE,QAAQ,EAAR,EAAQ;IAAEC,OAAK,EAAAX,MAAA,QAAAA,MAAA,MAAAQ,MAAA,IAAEL,QAAA,CAAAS,aAAa;iEAA5DpB,KAAA,CAAAC,QAAQ,CAACgB,mBAAmB,E,GAC7DjB,KAAA,CAAAqB,aAAa,I,cAAxBzB,mBAAA,CAAyE,OAAzE0B,UAAyE,EAAAvB,gBAAA,CAAtBC,KAAA,CAAAqB,aAAa,oBAbxEf,mBAAA,e,GAgBMR,mBAAA,CAIM,OAJNyB,UAIM,G,4BAHJzB,mBAAA,CAA6D;IAAtDgB,GAAG,EAAC;EAAsB,GAAC,qBAAmB,sB,gBACrDhB,mBAAA,CAA+I;IAArIiB,EAAE,EAAC,sBAAsB;IAlB3C,uBAAAP,MAAA,QAAAA,MAAA,MAAAQ,MAAA,IAkBqDhB,KAAA,CAAAC,QAAQ,CAACuB,oBAAoB,GAAAR,MAAA;IAAEE,QAAQ,EAAR,EAAQ;IAAEC,OAAK,EAAAX,MAAA,QAAAA,MAAA,MAAAQ,MAAA,IAAEL,QAAA,CAAAS,aAAa;iEAA7DpB,KAAA