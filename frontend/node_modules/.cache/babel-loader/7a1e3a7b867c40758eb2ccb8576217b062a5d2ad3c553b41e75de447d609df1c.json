{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  props: ['childId'],\n  data() {\n    return {\n      milestones: []\n    };\n  },\n  created() {\n    if (this.childId) {\n      localStorage.setItem('childId', this.childId);\n    }\n    const storedId = localStorage.getItem('childId');\n    if (!this.childId && !storedId) {\n      alert('Molimo registrujte dete pre nastavka.');\n      this.$router.push('/child-registration');\n      return;\n    }\n    this.fetchMilestones();\n  },\n  methods: {\n    async fetchMilestones() {\n      const id = this.childId || localStorage.getItem('childId');\n      if (!id) {\n        console.error('Child ID is missing');\n        return;\n      }\n      try {\n        const token = localStorage.getItem('token');\n        if (!token) {\n          console.error('User not authenticated');\n          return;\n        }\n        const response = await axios.get(`http://localhost:5000/api/milestones/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (Array.isArray(response.data)) {\n          this.milestones = response.data;\n        } else {\n          console.error('Expected array but got:', response.data);\n        }\n      } catch (error) {\n        console.error('Error fetching milestones:', error);\n      }\n    },\n    formatDate(value) {\n      if (!value) return '';\n      const date = new Date(value);\n      return `${date.getDate()}/${date.getMonth() + 1}/${date.getFullYear()}`;\n    },\n    editMilestone(id) {\n      this.$router.push(`/milestone-form/${this.childId}/${id}`);\n    },\n    async deleteMilestone(id) {\n      const token = localStorage.getItem('token');\n      try {\n        const response = await axios.delete(`http://localhost:5000/api/milestones/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        console.log('Kljucni Moment obrisan:', response);\n        this.fetchMilestones();\n      } catch (error) {\n        console.error('Greška prilikom brisanja ključnog momenta:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","props","data","milestones","created","childId","localStorage","setItem","storedId","getItem","alert","$router","push","fetchMilestones","methods","id","console","error","token","response","get","headers","Authorization","Array","isArray","formatDate","value","date","Date","getDate","getMonth","getFullYear","editMilestone","deleteMilestone","delete","log"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\MilestoneList.vue"],"sourcesContent":["<template>\r\n  <div class=\"milestone-list-container\">\r\n    <h2>Pregled Kljucnih Momenta</h2>\r\n    <div v-if=\"milestones.length\" class=\"milestone-list\">\r\n      <div v-for=\"(milestone, index) in milestones\" :key=\"index\" class=\"milestone-item\">\r\n        <h3>{{ milestone.milestoneType }} - {{ formatDate(milestone.milestoneDate) }}</h3>\r\n        <p><strong>Napredak:</strong> {{ milestone.progress }}</p>\r\n        <p><strong>Napomene:</strong> {{ milestone.notes }}</p>\r\n\r\n      \r\n        <button v-if=\"milestone._id\" @click=\"editMilestone(milestone._id)\" class=\"edit-btn\">Izmeni</button>\r\n        <button v-if=\"milestone._id\" @click=\"deleteMilestone(milestone._id)\" class=\"delete-btn\">Obriši</button>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else class=\"no-data-message\">\r\n      <p>Nema podataka o Kljucnim Momentima za ovo dete.</p>\r\n    </div>\r\n\r\n    <router-link :to=\"`/milestone-form/${childId}`\" class=\"add-milestone-button\">Dodaj Kljucni Moment</router-link>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: ['childId'],\r\n  data() {\r\n    return {\r\n      milestones: [],\r\n    };\r\n  },\r\n  created() {\r\n    if (this.childId) {\r\n      localStorage.setItem('childId', this.childId);\r\n    }\r\n    const storedId = localStorage.getItem('childId');\r\n    if (!this.childId && !storedId) {\r\n      alert('Molimo registrujte dete pre nastavka.');\r\n      this.$router.push('/child-registration');\r\n      return;\r\n    }\r\n    this.fetchMilestones();\r\n  },\r\n  methods: {\r\n    async fetchMilestones() {\r\n      const id = this.childId || localStorage.getItem('childId');\r\n      if (!id) {\r\n        console.error('Child ID is missing');\r\n        return;\r\n      }\r\n      try {\r\n        const token = localStorage.getItem('token');\r\n        if (!token) {\r\n          console.error('User not authenticated');\r\n          return;\r\n        }\r\n\r\n     \r\n        const response = await axios.get(`http://localhost:5000/api/milestones/${id}`, {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n\r\n        if (Array.isArray(response.data)) {\r\n          this.milestones = response.data;\r\n        } else {\r\n          console.error('Expected array but got:', response.data);\r\n        }\r\n      } catch (error) {\r\n        console.error('Error fetching milestones:', error);\r\n      }\r\n    },\r\n\r\n    formatDate(value) {\r\n      if (!value) return '';\r\n      const date = new Date(value);\r\n      return `${date.getDate()}/${date.getMonth() + 1}/${date.getFullYear()}`;\r\n    },\r\n\r\n    editMilestone(id) {\r\n      this.$router.push(`/milestone-form/${this.childId}/${id}`);\r\n    },\r\n\r\n    async deleteMilestone(id) {\r\n      const token = localStorage.getItem('token');\r\n      try {\r\n        const response = await axios.delete(`http://localhost:5000/api/milestones/${id}`, {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n        console.log('Kljucni Moment obrisan:', response);\r\n        \r\n        this.fetchMilestones();\r\n      } catch (error) {\r\n        console.error('Greška prilikom brisanja ključnog momenta:', error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.milestone-list-container {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f7f7f7;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n\r\n  h2 {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: #333;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .milestone-list {\r\n    display: flex;\r\n    flex-direction: column;\r\n\r\n    .milestone-item {\r\n      background-color: #fff;\r\n      padding: 15px;\r\n      margin-bottom: 15px;\r\n      border: 1px solid #ddd;\r\n      border-radius: 8px;\r\n      box-shadow: 0 2px 6px rgba(0, 0, 0, 0.1);\r\n\r\n      h3 {\r\n        font-size: 20px;\r\n        color: #333;\r\n        margin-bottom: 10px;\r\n      }\r\n\r\n      p {\r\n        font-size: 16px;\r\n        color: #555;\r\n        margin: 5px 0;\r\n      }\r\n\r\n      .edit-btn,\r\n      .delete-btn {\r\n        padding: 8px 15px;\r\n        margin-top: 10px;\r\n        font-size: 14px;\r\n        border-radius: 5px;\r\n        cursor: pointer;\r\n        border: none;\r\n        transition: background-color 0.3s ease;\r\n\r\n        &:hover {\r\n          background-color: #0056b3;\r\n        }\r\n      }\r\n\r\n      .edit-btn {\r\n        background-color: #28a745;\r\n        color: #fff;\r\n\r\n        &:hover {\r\n          background-color: #218838;\r\n        }\r\n      }\r\n\r\n      .delete-btn {\r\n        background-color: #dc3545;\r\n        color: #fff;\r\n\r\n        &:hover {\r\n          background-color: #c82333;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  .no-data-message {\r\n    text-align: center;\r\n    color: #555;\r\n    font-size: 18px;\r\n    padding: 20px;\r\n    border: 1px solid #ddd;\r\n    border-radius: 4px;\r\n    background-color: #f2f2f2;\r\n  }\r\n\r\n  .add-milestone-button {\r\n    display: inline-block;\r\n    margin-top: 20px;\r\n    text-align: center;\r\n    padding: 12px 25px;\r\n    font-size: 16px;\r\n    color: #fff;\r\n    background-color: #007bff;\r\n    border-radius: 5px;\r\n    text-decoration: none;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease;\r\n\r\n    &:hover {\r\n      background-color: #0056b3;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AAwBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE,CAAC,SAAS,CAAC;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,IAAI,CAACC,OAAO,EAAE;MAChBC,YAAY,CAACC,OAAO,CAAC,SAAS,EAAE,IAAI,CAACF,OAAO,CAAC;IAC/C;IACA,MAAMG,QAAO,GAAIF,YAAY,CAACG,OAAO,CAAC,SAAS,CAAC;IAChD,IAAI,CAAC,IAAI,CAACJ,OAAM,IAAK,CAACG,QAAQ,EAAE;MAC9BE,KAAK,CAAC,uCAAuC,CAAC;MAC9C,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,qBAAqB,CAAC;MACxC;IACF;IACA,IAAI,CAACC,eAAe,CAAC,CAAC;EACxB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,eAAeA,CAAA,EAAG;MACtB,MAAME,EAAC,GAAI,IAAI,CAACV,OAAM,IAAKC,YAAY,CAACG,OAAO,CAAC,SAAS,CAAC;MAC1D,IAAI,CAACM,EAAE,EAAE;QACPC,OAAO,CAACC,KAAK,CAAC,qBAAqB,CAAC;QACpC;MACF;MACA,IAAI;QACF,MAAMC,KAAI,GAAIZ,YAAY,CAACG,OAAO,CAAC,OAAO,CAAC;QAC3C,IAAI,CAACS,KAAK,EAAE;UACVF,OAAO,CAACC,KAAK,CAAC,wBAAwB,CAAC;UACvC;QACF;QAGA,MAAME,QAAO,GAAI,MAAMnB,KAAK,CAACoB,GAAG,CAAC,wCAAwCL,EAAE,EAAE,EAAE;UAC7EM,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUJ,KAAK;UAAG;QAC9C,CAAC,CAAC;QAEF,IAAIK,KAAK,CAACC,OAAO,CAACL,QAAQ,CAACjB,IAAI,CAAC,EAAE;UAChC,IAAI,CAACC,UAAS,GAAIgB,QAAQ,CAACjB,IAAI;QACjC,OAAO;UACLc,OAAO,CAACC,KAAK,CAAC,yBAAyB,EAAEE,QAAQ,CAACjB,IAAI,CAAC;QACzD;MACF,EAAE,OAAOe,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACpD;IACF,CAAC;IAEDQ,UAAUA,CAACC,KAAK,EAAE;MAChB,IAAI,CAACA,KAAK,EAAE,OAAO,EAAE;MACrB,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,KAAK,CAAC;MAC5B,OAAO,GAAGC,IAAI,CAACE,OAAO,CAAC,CAAC,IAAIF,IAAI,CAACG,QAAQ,CAAC,IAAI,CAAC,IAAIH,IAAI,CAACI,WAAW,CAAC,CAAC,EAAE;IACzE,CAAC;IAEDC,aAAaA,CAACjB,EAAE,EAAE;MAChB,IAAI,CAACJ,OAAO,CAACC,IAAI,CAAC,mBAAmB,IAAI,CAACP,OAAO,IAAIU,EAAE,EAAE,CAAC;IAC5D,CAAC;IAED,MAAMkB,eAAeA,CAAClB,EAAE,EAAE;MACxB,MAAMG,KAAI,GAAIZ,YAAY,CA