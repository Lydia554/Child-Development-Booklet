{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport axios from \"axios\";\nexport default {\n  props: {\n    childId: {\n      type: String,\n      required: true\n    },\n    selectedPeriod: {\n      type: Object,\n      default: () => ({})\n    } // Prevents undefined errors\n  },\n  data() {\n    return {\n      milestones: []\n    };\n  },\n  computed: {\n    currentPeriodMilestones() {\n      if (!this.selectedPeriod || !this.selectedPeriod.start) return []; // Avoid undefined access\n      return this.milestones.filter(m => m.startAge === this.selectedPeriod.start && m.endAge === this.selectedPeriod.end);\n    }\n  },\n  async created() {\n    await this.fetchMilestones();\n  },\n  methods: {\n    async fetchMilestones() {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/milestones/${this.childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.milestones = response.data;\n      } catch (error) {\n        console.error(\"Error fetching milestones:\", error);\n      }\n    },\n    addMilestone() {\n      if (!this.selectedPeriod) return; // Prevent navigation error\n      this.$router.push({\n        path: `/milestone-form/${this.childId}`,\n        query: {\n          startAge: this.selectedPeriod.start,\n          endAge: this.selectedPeriod.end\n        }\n      });\n    },\n    editMilestone(id) {\n      this.$router.push(`/milestone-form/${this.childId}/${id}`);\n    },\n    async confirmDelete(id) {\n      if (window.confirm(\"Da li ste sigurni da želite da obrišete ovaj milestone?\")) {\n        await this.deleteMilestone(id);\n      }\n    },\n    async deleteMilestone(id) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        await axios.delete(`http://localhost:5000/api/milestones/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.milestones = this.milestones.filter(milestone => milestone._id !== id);\n      } catch (error) {\n        console.error(\"Error deleting milestone:\", error);\n      }\n    },\n    formatPeriod(start, end) {\n      return `${start}-${end} meseci`;\n    },\n    formatDate(value) {\n      if (!value) return \"\";\n      return new Date(value).toLocaleDateString(\"sr-RS\");\n    }\n  }\n};","map":{"version":3,"names":["axios","props","childId","type","String","required","selectedPeriod","Object","default","data","milestones","computed","currentPeriodMilestones","start","filter","m","startAge","endAge","end","created","fetchMilestones","methods","token","sessionStorage","getItem","response","get","headers","Authorization","error","console","addMilestone","$router","push","path","query","editMilestone","id","confirmDelete","window","confirm","deleteMilestone","delete","milestone","_id","formatPeriod","formatDate","value","Date","toLocaleDateString"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\MilestoneList.vue"],"sourcesContent":["<template>\r\n  <div class=\"milestone-list-container\">\r\n    <!-- Ensure selectedPeriod exists before accessing properties -->\r\n    <h4 v-if=\"selectedPeriod\">Prekretnice za period: {{ formatPeriod(selectedPeriod?.start, selectedPeriod?.end) }}</h4>\r\n\r\n    <!-- Display milestone list only if selectedPeriod is defined -->\r\n    <div v-if=\"selectedPeriod && currentPeriodMilestones.length\" class=\"milestone-list\">\r\n      <div v-for=\"milestone in currentPeriodMilestones\" :key=\"milestone._id\" class=\"milestone-item\">\r\n        <h3>{{ milestone.milestoneType }} - {{ formatDate(milestone.milestoneDate) }}</h3>\r\n        <p><strong>Napredak:</strong> {{ milestone.progress }}</p>\r\n        <p><strong>Napomene:</strong> {{ milestone.notes }}</p>\r\n        <div class=\"button-group\">\r\n          <button @click=\"editMilestone(milestone._id)\" class=\"edit-btn\">Izmeni</button>\r\n          <button @click=\"confirmDelete(milestone._id)\" class=\"delete-btn\">Obriši</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- No milestones message -->\r\n    <div v-else-if=\"selectedPeriod\" class=\"no-data-message\">\r\n      <p>Nema unetih podataka za ovaj period.</p>\r\n    </div>\r\n\r\n    <!-- Prevent button from showing if period is undefined -->\r\n    <button v-if=\"selectedPeriod\" @click=\"addMilestone\" class=\"add-milestone-button\">\r\n      Dodaj Kljucni Momenat\r\n    </button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  props: {\r\n    childId: { type: String, required: true },\r\n    selectedPeriod: { type: Object, default: () => ({}) } // Prevents undefined errors\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      milestones: []\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    currentPeriodMilestones() {\r\n      if (!this.selectedPeriod || !this.selectedPeriod.start) return []; // Avoid undefined access\r\n      return this.milestones.filter(\r\n        (m) => m.startAge === this.selectedPeriod.start && m.endAge === this.selectedPeriod.end\r\n      );\r\n    }\r\n  },\r\n\r\n  async created() {\r\n    await this.fetchMilestones();\r\n  },\r\n\r\n  methods: {\r\n    async fetchMilestones() {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(`http://localhost:5000/api/milestones/${this.childId}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n\r\n        this.milestones = response.data;\r\n      } catch (error) {\r\n        console.error(\"Error fetching milestones:\", error);\r\n      }\r\n    },\r\n\r\n    addMilestone() {\r\n      if (!this.selectedPeriod) return; // Prevent navigation error\r\n      this.$router.push({\r\n        path: `/milestone-form/${this.childId}`,\r\n        query: { startAge: this.selectedPeriod.start, endAge: this.selectedPeriod.end }\r\n      });\r\n    },\r\n\r\n    editMilestone(id) {\r\n      this.$router.push(`/milestone-form/${this.childId}/${id}`);\r\n    },\r\n\r\n    async confirmDelete(id) {\r\n      if (window.confirm(\"Da li ste sigurni da želite da obrišete ovaj milestone?\")) {\r\n        await this.deleteMilestone(id);\r\n      }\r\n    },\r\n\r\n    async deleteMilestone(id) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        await axios.delete(`http://localhost:5000/api/milestones/${id}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n\r\n        this.milestones = this.milestones.filter((milestone) => milestone._id !== id);\r\n      } catch (error) {\r\n        console.error(\"Error deleting milestone:\", error);\r\n      }\r\n    },\r\n\r\n    formatPeriod(start, end) {\r\n      return `${start}-${end} meseci`;\r\n    },\r\n\r\n    formatDate(value) {\r\n      if (!value) return \"\";\r\n      return new Date(value).toLocaleDateString(\"sr-RS\");\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n.milestone-list-container {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f7f7f7;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n\r\n  h2 {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: #333;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .milestone-list {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 15px;\r\n  }\r\n\r\n  .milestone-item {\r\n    background-color: #fff;\r\n    padding: 20px;\r\n    border-radius: 8px;\r\n    box-shadow: 0 2px 6px rgba(0, 0, 0, 0.1);\r\n\r\n    h3 {\r\n      font-size: 20px;\r\n      color: #333;\r\n      margin-bottom: 10px;\r\n    }\r\n\r\n    p {\r\n      margin: 8px 0;\r\n      color: #555;\r\n    }\r\n\r\n    .button-group {\r\n      display: flex;\r\n      gap: 10px;\r\n      margin-top: 15px;\r\n    }\r\n\r\n    .edit-btn,\r\n    .delete-btn {\r\n      padding: 8px 16px;\r\n      border: none;\r\n      border-radius: 4px;\r\n      cursor: pointer;\r\n      font-weight: 500;\r\n      transition: all 0.3s ease;\r\n    }\r\n\r\n    .edit-btn {\r\n      background-color: #28a745;\r\n      color: white;\r\n\r\n      &:hover {\r\n        background-color: #218838;\r\n      }\r\n    }\r\n\r\n    .delete-btn {\r\n      background-color: #dc3545;\r\n      color: white;\r\n\r\n      &:hover {\r\n        background-color: #c82333;\r\n      }\r\n    }\r\n  }\r\n\r\n  .no-data-message {\r\n    text-align: center;\r\n    padding: 20px;\r\n    background-color: #fff;\r\n    border-radius: 8px;\r\n    color: #666;\r\n  }\r\n\r\n  .add-milestone-button {\r\n    display: inline-block;\r\n    margin-top: 20px;\r\n    padding: 12px 24px;\r\n    background-color: #007bff;\r\n    color: white;\r\n    text-decoration: none;\r\n    border-radius: 4px;\r\n    transition: background-color 0.3s ease;\r\n\r\n    &:hover {\r\n      background-color: #0056b3;\r\n    }\r\n  }\r\n}\r\n</style>"],"mappings":";;;AA+BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE;IACLC,OAAO,EAAE;MAAEC,IAAI,EAAEC,MAAM;MAAEC,QAAQ,EAAE;IAAK,CAAC;IACzCC,cAAc,EAAE;MAAEH,IAAI,EAAEI,MAAM;MAAEC,OAAO,EAAEA,CAAA,MAAO,CAAC,CAAC;IAAE,EAAE;EACxD,CAAC;EAEDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EAEDC,QAAQ,EAAE;IACRC,uBAAuBA,CAAA,EAAG;MACxB,IAAI,CAAC,IAAI,CAACN,cAAa,IAAK,CAAC,IAAI,CAACA,cAAc,CAACO,KAAK,EAAE,OAAO,EAAE,EAAE;MACnE,OAAO,IAAI,CAACH,UAAU,CAACI,MAAM,CAC1BC,CAAC,IAAKA,CAAC,CAACC,QAAO,KAAM,IAAI,CAACV,cAAc,CAACO,KAAI,IAAKE,CAAC,CAACE,MAAK,KAAM,IAAI,CAACX,cAAc,CAACY,GACtF,CAAC;IACH;EACF,CAAC;EAED,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,eAAe,CAAC,CAAC;EAC9B,CAAC;EAEDC,OAAO,EAAE;IACP,MAAMD,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAME,KAAI,GAAIC,cAAc,CAACC,OAAO,CAAC,OAAO,CAAC;QAC7C,MAAMC,QAAO,GAAI,