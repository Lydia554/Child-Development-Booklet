ation: `Bearer ${sessionStorage.getItem('token')}` }\r\n        });\r\n\r\n        if (response.data && response.data._id) {\r\n          console.log(\"‚úÖ Dete registrovano:\", response.data);\r\n          \r\n          // Dodaj dete u authStore i sessionStorage\r\n          authStore.addChildId(response.data._id);\r\n          authStore.setCurrentChildId(response.data._id);\r\n          sessionStorage.setItem('childId', response.data._id);\r\n\r\n          // Preusmeri na dashboard\r\n          this.$router.push('/dashboard');\r\n        } else {\r\n          this.errorMessage = \"Gre≈°ka prilikom registracije deteta.\";\r\n        }\r\n      } catch (error) {\r\n        console.error(\"‚ùå Gre≈°ka pri registraciji deteta:\", error);\r\n        this.errorMessage = \"Neuspe≈°na registracija. Poku≈°ajte ponovo.\";\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n$primary-color: #007bff;\r\n$primary-hover: #0056b3;\r\n$error-bg: #f8d7da;\r\n$error-border: #f5c6cb;\r\n$error-text: #721c24;\r\n$text-color: #333;\r\n$background-light: #f7f7f7;\r\n$border-color: #ccc;\r\n\r\n.child-registration-container {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: $background-light;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n\r\n  h2 {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: $text-color;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .error-message {\r\n    margin-bottom: 20px;\r\n    padding: 10px;\r\n    background-color: $error-bg;\r\n    border: 1px solid $error-border;\r\n    color: $error-text;\r\n    border-radius: 5px;\r\n    text-align: center;\r\n  }\r\n\r\n  .child-registration-form {\r\n    display: flex;\r\n    flex-direction: column;\r\n\r\n    .form-group {\r\n      margin-bottom: 15px;\r\n\r\n      label {\r\n        font-size: 16px;\r\n        font-weight: bold;\r\n        color: $text-color;\r\n        margin-bottom: 5px;\r\n        display: block;\r\n      }\r\n\r\n      input {\r\n        width: 100%;\r\n        padding: 12px;\r\n        font-size: 16px;\r\n        border: 1px solid $border-color;\r\n        border-radius: 5px;\r\n        box-sizing: border-box;\r\n      }\r\n\r\n      input[type=\"date\"] {\r\n        padding: 10px;\r\n        font-size: 16px;\r\n      }\r\n    }\r\n\r\n    .submit-group {\r\n      display: flex;\r\n      justify-content: center;\r\n\r\n      .submit-btn {\r\n  padding: 12px 20px;\r\n  background-color: #4caf50;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.submit-btn:disabled {\r\n  background-color: #cccccc;\r\n  cursor: not-allowed;\r\n}\r\n    }\r\n\r\n        &:hover {\r\n          background-color: $primary-hover;\r\n        }\r\n      }\r\n}\r\n\r\n/* üì± Mobile Styles: Optimized for Phones */\r\n@media (max-width: 768px) {\r\n  .child-registration-container {\r\n    padding: 15px;\r\n  }\r\n\r\n  h2 {\r\n    font-size: 22px;\r\n  }\r\n\r\n  .form-group label {\r\n    font-size: 15px;\r\n  }\r\n\r\n  input {\r\n    font-size: 15px;\r\n    padding: 10px;\r\n  }\r\n\r\n  .submit-btn {\r\n    width: 100%;\r\n    font-size: 18px;\r\n    padding: 16px;\r\n  }\r\n}\r\n\r\n/* üì± Extra Small Mobile Screens (Max 480px) */\r\n@media (max-width: 480px) {\r\n  .child-registration-container {\r\n    padding: 12px;\r\n  }\r\n\r\n  h2 {\r\n    font-size: 20px;\r\n  }\r\n\r\n  .form-group label {\r\n    font-size: 14px;\r\n  }\r\n\r\n  input {\r\n    font-size: 14px;\r\n    padding: 10px;\r\n  }\r\n\r\n  .submit-btn {\r\n    font-size: 16px;\r\n    padding: 14px;\r\n  }\r\n}\r\n\r\n/* üì± Ultra-Small Mobile Screens (Max 360px) */\r\n@media (max-width: 360px) {\r\n  .child-registration-container {\r\n    padding: 10px;\r\n  }\r\n\r\n  h2 {\r\n    font-size: 18px;\r\n  }\r\n\r\n  .form-group label {\r\n    font-size: 14px;\r\n  }\r\n\r\n  input {\r\n    font-size: 14px