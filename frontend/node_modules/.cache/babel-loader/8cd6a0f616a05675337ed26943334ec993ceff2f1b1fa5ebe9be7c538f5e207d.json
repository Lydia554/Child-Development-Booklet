age"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"development-form\">\r\n    <h2>Unos Razvoja Deteta</h2>\r\n    <h3>Period: {{ formData.startAge }}-{{ formData.endAge }} meseci</h3>\r\n\r\n    <div v-if=\"error\" class=\"error-message\">\r\n      {{ error }}\r\n    </div>\r\n\r\n    <form @submit.prevent=\"saveDevelopment\">\r\n      <div class=\"form-group\">\r\n        <label for=\"physicalDevelopment\">Fizički razvoj:</label>\r\n        <textarea\r\n          id=\"physicalDevelopment\"\r\n          v-model=\"formData.physicalDevelopment\"\r\n          required\r\n        ></textarea>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"emotionalDevelopment\">Emocionalni razvoj:</label>\r\n        <textarea\r\n          id=\"emotionalDevelopment\"\r\n          v-model=\"formData.emotionalDevelopment\"\r\n          required\r\n        ></textarea>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"intellectualDevelopment\">Intelektualni razvoj:</label>\r\n        <textarea\r\n          id=\"intellectualDevelopment\"\r\n          v-model=\"formData.intellectualDevelopment\"\r\n          required\r\n        ></textarea>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"socialDevelopment\">Socijalni razvoj:</label>\r\n        <textarea\r\n          id=\"socialDevelopment\"\r\n          v-model=\"formData.socialDevelopment\"\r\n          required\r\n        ></textarea>\r\n      </div>\r\n\r\n      <button type=\"submit\" :disabled=\"loading\">\r\n        {{ loading ? 'Čuvanje...' : 'Sačuvaj' }}\r\n      </button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      formData: {\r\n        startAge: 0,\r\n        endAge: 6,\r\n        physicalDevelopment: '',\r\n        emotionalDevelopment: '',\r\n        intellectualDevelopment: '',\r\n        socialDevelopment: ''\r\n      },\r\n      error: null,\r\n      loading: false\r\n    };\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      const startAge = parseInt(this.$route.query.startAge) || 24;\r\n      const endAge = parseInt(this.$route.query.endAge) || (startAge + 6);\r\n      \r\n      this.formData.startAge = startAge;\r\n      this.formData.endAge = endAge;\r\n      \r\n      console.log('Form initialized with period:', {\r\n        startAge: this.formData.startAge,\r\n        endAge: this.formData.endAge\r\n      });\r\n\r\n      await this.loadDevelopmentData();\r\n    } catch (error) {\r\n      console.error('Error in created:', error);\r\n      this.error = 'Greška pri učitavanju podataka.';\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    async loadDevelopmentData() {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/child-development/${this.$route.params.childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` }\r\n          }\r\n        );\r\n\r\n        const periodData = response.data.find(data => \r\n          data.startAge === this.formData.startAge && \r\n          data.endAge === this.formData.endAge\r\n        );\r\n\r\n        if (periodData) {\r\n          this.formData = {\r\n            ...this.formData,\r\n            physicalDevelopment: periodData.physicalDevelopment || '',\r\n            emotionalDevelopment: periodData.emotionalDevelopment || '',\r\n            intellectualDevelopment: periodData.intellectualDevelopment || '',\r\n            socialDevelopment: periodData.socialDevelopment || ''\r\n          };\r\n        }\r\n      } catch (error) {\r\n        console.error('Error loading development data:', error);\r\n        this.error = 'Greška pri učitavanju postojećih podataka.';\r\n      }\r\n    },\r\n\r\n    async saveDevelopment() {\r\n      try {\r\n        this.loading = true;\r\n        const token = sessionStorage.getItem('token');\r\n        const childId = this.$route.params.childId;\r\n        \r\n        const developmentData = {\r\n          startAge: parseInt(this.formData.startAge),\r\n          endAge: parseInt(this.formData.endAge),\r\n          physicalDevelopment: this.formData.physicalDevelopment,\r\n          emotionalDevelopment: this.formData.emotionalDevelopment,\r\n          intellectualDevelopment: this.formData.intellectualDevelopment,\r\n          socialDevelopment: this.formData.socialDevelopment\r\n        };\r\n\r\n        console.log('Sending development data:', developmentData);\r\n\r\n        const response = await axios.post(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          developmentData,\r\n          {\r\n            headers: { \r\n              'Authorization': `Bearer ${token}`,\r\n              'Content-Type': 'application/json'\r\n            }\r\n          }\r\n        );\r\n\r\n        if (response.data) {\r\n          console.log('Development saved successfully:', response.data);\r\n          this.$router.push(`/child-development-list/${childId}`);\r\n        }\r\n      } catch (error) {\r\n        console.error('Error saving development:', error.response || error);\r\n        this.error = `Greška pri čuvanju podataka: ${error.response?.data?.message || error.message}`;\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.development-form {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 20px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n  font-weight: bold;\r\n}\r\n\r\ntextarea {\r\n  width: 100%;\r\n  min-height: 100px;\r\n  padding: 10px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n}\r\n\r\nbutton {\r\n  padding: 10px 20px;\r\n 