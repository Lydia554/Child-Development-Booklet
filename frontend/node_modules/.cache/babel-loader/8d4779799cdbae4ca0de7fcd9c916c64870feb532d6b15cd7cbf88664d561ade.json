tAge && data.endAge === this.formData.endAge);\n        if (periodData) {\n          // Pre-fill form with existing data\n          this.formData = {\n            ...this.formData,\n            physicalDevelopment: periodData.physicalDevelopment || '',\n            emotionalDevelopment: periodData.emotionalDevelopment || '',\n            intellectualDevelopment: periodData.intellectualDevelopment || '',\n            socialDevelopment: periodData.socialDevelopment || ''\n          };\n        }\n      } catch (error) {\n        console.error('Error loading development data:', error);\n        this.error = 'Greška pri učitavanju postojećih podataka.';\n      }\n    },\n    async saveDevelopment() {\n      try {\n        this.loading = true;\n        const token = sessionStorage.getItem('token');\n        const childId = this.$route.params.childId;\n        const response = await axios.post(`http://localhost:5000/api/children/${childId}/development`, this.formData, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (response.data) {\n          // Redirect back to development list\n          this.$router.push(`/child-development-list/${childId}`);\n        }\n      } catch (error) {\n        console.error('Error saving development:', error);\n        this.error = 'Greška pri čuvanju podataka.';\n      } finally {\n        this.loading = false;\n      }\n    },\n    formatPeriod(start, end) {\n      if (start === 0) {\n        return `0-6 meseci`;\n      }\n      return `${start}-${end} meseci`;\n    },\n    async submitDevelopmentForm() {\n      try {\n        const token = sessionStorage.getItem('token');\n        if (!token) {\n          this.errorMessage = 'Morate biti ulogovani.';\n          return;\n        }\n        const childId = this.$route.params.childId;\n        const developmentData = {\n          ...this.formData,\n          startAge: this.formData.startAge,\n          endAge: this.formData.endAge\n        };\n        await this.saveDevelopment();\n      } catch (error) {\n        console.error('Error submitting development data:', error);\n        this.errorMessage = 'Greška prilikom čuvanja podataka.';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","formData","startAge","endAge","physicalDevelopment","emotionalDevelopment","intellectualDevelopment","socialDevelopment","error","loading","computed","isPeriodAvailable","childAge","submitButtonText","created","parseInt","$route","query","console","log","loadDevelopmentData","methods","childId","params","token","sessionStorage","getItem","response","get","headers","Authorization","periodData","find","saveDevelopment","post","$router","push","formatPeriod","start","end","submitDevelopmentForm","errorMessage","developmentData"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"child-development-form\">\r\n    <h2>Unos Razvoja Deteta</h2>\r\n    <h3>Period: {{ formatPeriod(startAge, endAge) }}</h3>\r\n    \r\n    <form @submit.prevent=\"submitDevelopmentForm\" class=\"form-container\">\r\n      <div class=\"form-group\">\r\n        <label for=\"physicalDevelopment\">Fizički razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.physicalDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"emotionalDevelopment\">Emocionalni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.emotionalDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"intellectualDevelopment\">Intelektualni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.intellectualDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"socialDevelopment\">Socijalni razvoj:</