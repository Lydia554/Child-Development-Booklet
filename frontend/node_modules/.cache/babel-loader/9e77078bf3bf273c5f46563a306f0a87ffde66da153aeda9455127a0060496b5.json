     }\r\n    },\r\n    downloadPDF() {\r\n      const doc = new jsPDF();\r\n      doc.text(`Razvoj deteta: ${this.child.name}`, 10, 10);\r\n\r\n      const tableColumn = [\"Period\", \"Fizički\", \"Emocionalni\", \"Intelektualni\", \"Socijalni\"];\r\n      const tableRows = [];\r\n\r\n      this.developmentData.forEach(period => {\r\n        const row = [\r\n          `${period.startAge}-${period.endAge} meseci`,\r\n          period.physicalDevelopment || \"-\",\r\n          period.emotionalDevelopment || \"-\",\r\n          period.intellectualDevelopment || \"-\",\r\n          period.socialDevelopment || \"-\",\r\n        ];\r\n        tableRows.push(row);\r\n      });\r\n\r\n      doc.autoTable({ head: [tableColumn], body: tableRows, startY: 20 });\r\n      doc.save(`Razvoj_${this.child.name}.pdf`);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.overview-container {\r\n  max-width: 900px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f9f9f9;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\nh2 {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.development-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  background: white;\r\n}\r\n\r\n.development-table th,\r\n.development-table td {\r\n  border: 1px solid #ddd;\r\n  padding: 10px;\r\n  text-align: left;\r\n}\r\n\r\n.development-table th {\r\n  background: #007bff;\r\n  color: white;\r\n}\r\n\r\n.error-message {\r\n  color: red;\r\n  text-align: center;\r\n  font-weight: bold;\r\n}\r\n\r\n.pdf-button {\r\n  display: block;\r\n  margin: 20px auto;\r\n  padding: 10px 20px;\r\n  background-color: #28a745;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.pdf-button:hover {\r\n  background-color: #218838;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAoB;;EADjCC,GAAA;AAAA;;EAAAA,GAAA;EAKkCD,KAAK,EAAC;;;EALxCC,GAAA;AAAA;;EAOaD,KAAK,EAAC;AAAmB;;uBANpCE,mBAAA,CA8BM