alDevelopment }}</li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { useAuthStore } from '../stores/authStore';\r\n\r\nexport default {\r\n  data() {\r\n    return { \r\n      periodRecords: [], \r\n      childName: \"\" \r\n    };\r\n  },\r\n  created() {\r\n    const authStore = useAuthStore();\r\n\r\n\r\n    const childId = authStore.childId;\r\n\r\n    if (childId) {\r\n      this.fetchDevelopmentData(childId);\r\n      this.fetchChildName(childId);\r\n    } else {\r\n      console.error('No child ID found');\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchDevelopmentData(childId) {\r\n      try {\r\n        const response = await axios.get(`/api/child-development/${childId}`);\r\n        this.periodRecords = response.data; \r\n      } catch (error) {\r\n        console.error('Error fetching development data:', error);\r\n      }\r\n    },\r\n\r\n    async fetchChildName(childId) {\r\n      try {\r\n        const response = await axios.get(`/api/children/${childId}`);\r\n        this.childName = response.data.name; \r\n      } catch (error) {\r\n        console.error('Error fetching child name:', error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n  \r\n  <style scoped>\r\n \r\n  .error-message {\r\n    color: #ff4d4f;\r\n    font-size: 16px;\r\n    font-weight: bold;\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n\r\n  textarea {\r\n    width: 100%;\r\n    padding: 10px;\r\n    margin-top: 5px;\r\n  }\r\n  \r\n  button {\r\n    padding: 10px 20px;\r\n    background-color: #007bff;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  button:hover {\r\n    background-color: #0056b3;\r\n  }\r\n  \r\n  .nav-button {\r\n    margin-top: 20px;\r\n    display: inline-block;\r\n    text-decoration: none;\r\n    padding: 10px 20px;\r\n    background-color: #007bff;\r\n    color: white;\r\n    border-radius: 4px;\r\n  }\r\n  </style>\r\