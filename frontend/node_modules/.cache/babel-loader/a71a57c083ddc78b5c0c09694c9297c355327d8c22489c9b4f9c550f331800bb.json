idateDevelopmentStage\"\r\n        />\r\n        <div v-if=\"developmentStageError\" class=\"error-message\">{{ developmentStageError }}</div>\r\n      </div>\r\n\r\n      <div class=\"form-group submit-group\">\r\n        <button type=\"submit\" class=\"submit-btn\" >Registruj Dete</button>\r\n      </div>\r\n    </form>\r\n\r\n    <div v-if=\"errorMessage\" class=\"error-message\">\r\n      {{ errorMessage }}\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { useAuthStore } from '../stores/authStore';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      newChild: {\r\n        name: '',\r\n        birthDate: '',\r\n        developmentStage: ''\r\n      },\r\n      errorMessage: '',\r\n\r\n      // Validation errors\r\n      nameError: null,\r\n      birthDateError: null,\r\n      developmentStageError: null,\r\n\r\n      // To track whether the form has errors\r\n      hasErrors: true,\r\n    };\r\n  },\r\n  methods: {\r\n    async submitChildForm() {\r\n      // First, validate everything before submitting\r\n      this.validateName();\r\n      this.validateBirthDate();\r\n      this.validateDevelopmentStage();\r\n\r\n      if (this.hasErrors) {\r\n        return;\r\n      }\r\n\r\n      const token = sessionStorage.getItem('token');\r\n      if (!token) {\r\n        this.errorMessage = 'Morate biti ulogovani da biste dodali dete.';\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.post(\r\n          `http://localhost:5000/api/${childId}`,\r\n          this.newChild,\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${token}`,\r\n            },\r\n          }\r\n        );\r\n\r\n        const childId = response.data._id;\r\n        sessionStorage.setItem('childId', childId);\r\n        const authStore = useAuthStore();\r\n        authStore.addChildId(childId);\r\n\r\n        this.$router.push(`/child-overview/${childId}`);\r\n      } catch (error) {\r\n        console.error('GreÅ¡ka pri registraciji deteta:', error);\r\n        this.errorMessage = 'GreÅ¡ka prilikom registracije deteta. Molimo pokuÅ¡ajte ponovo.';\r\n      }\r\n    },\r\n\r\n    validateName() {\r\n      const nameRegex = /^[A-Za-z\\s]+$/;\r\n      if (!this.newChild.name.trim()) {\r\n        this.nameError = 'Ime je obavezno.';\r\n        this.hasErrors = true;\r\n      } else if (!nameRegex.test(this.newChild.name)) {\r\n        this.nameError = 'Ime moÅ¾e sadrÅ¾ati samo slova.';\r\n        this.hasErrors = true;\r\n      } else {\r\n        this.nameError = null;\r\n      }\r\n    },\r\n\r\n    validateBirthDate() {\r\n      const today = new Date();\r\n      const birthDate = new Date(this.newChild.birthDate);\r\n      if (!this.newChild.birthDate) {\r\n        this.birthDateError = 'Datum roÄ‘enja je obavezan.';\r\n        this.hasErrors = true;\r\n      } else if (birthDate > today) {\r\n        this.birthDateError = 'Datum roÄ‘enja ne moÅ¾e biti u buduÄ‡nosti.';\r\n        this.hasErrors = true;\r\n      } else {\r\n        this.birthDateError = null;\r\n      }\r\n    },\r\n\r\n    validateDevelopmentStage() {\r\n      if (!this.newChild.developmentStage.trim()) {\r\n        this.developmentStageError = 'Faza razvoja je obavezna.';\r\n        this.hasErrors = true;\r\n      } else {\r\n        this.developmentStageError = null;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n$primary-color: #007bff;\r\n$primary-hover: #0056b3;\r\n$error-bg: #f8d7da;\r\n$error-border: #f5c6cb;\r\n$error-text: #721c24;\r\n$text-color: #333;\r\n$background-light: #f7f7f7;\r\n$border-color: #ccc;\r\n\r\n.child-registration-container {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: $background-light;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n\r\n  h2 {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: $text-color;\r\n   