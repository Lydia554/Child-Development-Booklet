{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport axios from 'axios';\nexport default {\n  props: ['childId'],\n  data() {\n    return {\n      developmentData: [],\n      milestones: [],\n      children: []\n    };\n  },\n  created() {\n    const childrenIds = JSON.parse(localStorage.getItem('childrenIds')) || [];\n    this.children = childrenIds.map(id => {\n      return {\n        _id: id,\n        name: `Child ${id}`\n      }; // Simulate child data based on IDs\n    });\n  },\n  methods: {\n    async fetchDevelopmentData() {\n      const token = localStorage.getItem('token');\n      try {\n        const response = await axios.get(`http://localhost:5000/api/child-development/${this.childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.developmentData = response.data.periods;\n      } catch (error) {\n        console.error('Error fetching development data:', error);\n      }\n    },\n    async fetchMilestones() {\n      const token = localStorage.getItem('token');\n      try {\n        const response = await axios.get(`http://localhost:5000/api/milestones/${this.childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.milestones = response.data;\n      } catch (error) {\n        console.error('Error fetching milestones:', error);\n      }\n    },\n    formatDate(value) {\n      if (!value) return '';\n      const date = new Date(value);\n      return `${date.getDate()}/${date.getMonth() + 1}/${date.getFullYear()}`;\n    }\n  }\n};","map":{"version":3,"names":["axios","props","data","developmentData","milestones","children","created","childrenIds","JSON","parse","localStorage","getItem","map","id","_id","name","methods","fetchDevelopmentData","token","response","get","childId","headers","Authorization","periods","error","console","fetchMilestones","formatDate","value","date","Date","getDate","getMonth","getFullYear"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildOverview.vue"],"sourcesContent":["<template>\r\n    <div class=\"child-overview\">\r\n      <h2>Pregled Razvoja i Klju훾nih Momenta</h2>\r\n  \r\n      <div class=\"development-and-milestones\">\r\n        <div class=\"development-section\">\r\n          <h3>Razvoj Deteta</h3>\r\n          <div v-if=\"developmentData.length\">\r\n            <div v-for=\"(data, index) in developmentData\" :key=\"index\">\r\n              <h4>Period: {{ data.startAge }} - {{ data.endAge }} meseci</h4>\r\n              <p><strong>Fizi훾ki razvoj:</strong> {{ data.physicalDevelopment }}</p>\r\n              <p><strong>Emocionalni razvoj:</strong> {{ data.emotionalDevelopment }}</p>\r\n              <p><strong>Intelektualni razvoj:</strong> {{ data.intellectualDevelopment }}</p>\r\n              <p><strong>Socijalni razvoj:</strong> {{ data.socialDevelopment }}</p>\r\n            </div>\r\n          </div>\r\n          <div v-else>\r\n            <p>Nema podataka o razvoju deteta.</p>\r\n          </div>\r\n        </div>\r\n  \r\n        <div class=\"milestones-section\">\r\n          <h3>Klju훾ni Moment</h3>\r\n          <div v-if=\"milestones.length\">\r\n            <div v-for=\"(milestone, index) in milestones\" :key=\"index\">\r\n              <h4>{{ milestone.milestoneType }} - {{ formatDate(milestone.milestoneDate) }}</h4>\r\n              <p><strong>Napredak:</strong> {{ milestone.progress }}</p>\r\n              <p><strong>Napomene:</strong> {{ milestone.notes }}</p>\r\n            </div>\r\n          </div>\r\n          <div v-else>\r\n            <p>Nema podataka o klju훾nim momentima za ovo dete.</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    props: ['childId'],\r\n    data() {\r\n      return {\r\n        developmentData: [],\r\n        milestones: [],\r\n        children: [],\r\n      };\r\n    },\r\n    created() {\r\n  const childrenIds = JSON.parse(localStorage.getItem('childrenIds')) || [];\r\n  this.children = childrenIds.map(id => {\r\n    return { _id: id, name: `Child ${id}` }; // Simulate child data based on IDs\r\n  });\r\n\r\n    },\r\n    methods: {\r\n      async fetchDevelopmentData() {\r\n        const token = localStorage.getItem('token');\r\n        try {\r\n          const response = await axios.get(`http://localhost:5000/api/child-development/${this.childId}`, {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          });\r\n          this.developmentData = response.data.periods;\r\n        } catch (error) {\r\n          console.error('Error fetching development data:', error);\r\n        }\r\n      },\r\n  \r\n      async fetchMilestones() {\r\n        const token = localStorage.getItem('token');\r\n        try {\r\n          const response = await axios.get(`http://localhost:5000/api/milestones/${this.childId}`, {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          });\r\n          this.milestones = response.data;\r\n        } catch (error) {\r\n          console.error('Error fetching milestones:', error);\r\n        }\r\n      },\r\n  \r\n      formatDate(value) {\r\n        if (!value) return '';\r\n        const date = new Date(value);\r\n        return `${date.getDate()}/${date.getMonth() + 1}/${date.getFullYear()}`;\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped lang=\"scss\">\r\n  .child-overview {\r\n    max-width: 900px;\r\n    margin: 0 auto;\r\n    padding: 20px;\r\n    background-color: #f7f7f7;\r\n    border-radius: 8px;\r\n    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n  \r\n    h2 {\r\n      text-align: center;\r\n      font-size: 24px;\r\n      color: #333;\r\n      margin-bottom: 20px;\r\n    }\r\n  \r\n    .development-and-milestones {\r\n      display: flex;\r\n      flex-direction: column;\r\n      gap: 30px;\r\n    }\r\n  \r\n    .development-section, .milestones-section {\r\n      background-color: #fff;\r\n      padding: 20px;\r\n      border-radius: 8px;\r\n      box-shadow: 0 2px 6px rgba(0, 0, 0, 0.1);\r\n  \r\n      h3 {\r\n        font-size: 22px;\r\n        color: #333;\r\n        margin-bottom: 15px;\r\n      }\r\n  \r\n      h4 {\r\n        font-size: 18px;\r\n        color: #555;\r\n        margin-bottom: 10px;\r\n      }\r\n  \r\n      p {\r\n        font-size: 16px;\r\n        color: #555;\r\n        margin: 5px 0;\r\n      }\r\n  \r\n      .no-data-message {\r\n        text-align: center;\r\n        color: #555;\r\n        font-size: 18px;\r\n        padding: 20px;\r\n        border: 1px solid #ddd;\r\n        border-radius: 4px;\r\n        background-color: #f2f2f2;\r\n      }\r\n    }\r\n  }\r\n  </style>\r\n  "],"mappings":";;AAuCE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE,CAAC,SAAS,CAAC;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,EAAE;MACnBC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACZ,MAAMC,WAAU,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,aAAa,CAAC,KAAK,EAAE;IACzE,IAAI,CAACN,QAAO,GAAIE,WAAW,CAACK,GAAG,CAACC,EAAC,IAAK;MACpC,OAAO;QAAEC,GAAG,EAAED,EAAE;QAAEE,IAAI,EAAE,SAASF,EAAE;MAAG,CAAC,EAAE;IAC3C,CAAC,CAAC;EAEA,CAAC;EACDG,OAAO,EAAE;IACP,MAAMC,oBAAoBA,CAAA,EAAG;MAC3B,MAAMC,KAAI,GAAIR,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAC3C,IAAI;QACF,MAAMQ,QAAO,GAAI,MAAMnB,KAAK,CAACoB,GAAG,CAAC,+CAA+C,IAAI,CAACC,OAAO,EAAE,EAAE;UAC9FC,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUL,KAAK;UAAG;QAC9C,CAAC,CAAC;QACF,IAAI,CAACf,eAAc,GAAIgB,QAAQ,CAACjB,IAAI,CAACsB,OAAO;MAC9C,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;MAC1D;IACF,CAAC;IAED,MAAME,eAAeA,CAAA,EAAG;MACtB,MAAMT,KAAI,GAAIR,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAC3C,IAAI;QACF,MAAMQ,QAAO,GAAI,MAAMnB,KAAK,CAACoB,GAAG,CAAC,wCAAwC,IAAI,CAACC,OAAO,EAAE,EAAE;UACvFC,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUL,KAAK;UAAG;QAC9C,CAAC,