{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      newChild: {\n        name: '',\n        birthDate: '',\n        developmentStage: ''\n      },\n      errorMessage: ''\n    };\n  },\n  methods: {\n    async submitChildForm() {\n      const token = localStorage.getItem('token');\n      if (!token) {\n        this.errorMessage = 'Morate biti ulogovani da biste dodali dete.';\n        return;\n      }\n\n      // Provera da li su sva polja popunjena\n      if (!this.newChild.name || !this.newChild.birthDate || !this.newChild.developmentStage) {\n        this.errorMessage = 'Sva polja su obavezna.';\n        return;\n      }\n      try {\n        const response = await axios.post('http://localhost:5000/api/children', this.newChild, {\n          headers: {\n            Authorization: `Bearer ${token}` // Poslat ćemo token u header-u\n          }\n        });\n        console.log('Podaci su uspešno poslati:', response);\n\n        // Preusmerite korisnika na child-development\n        this.$router.push('/child-development');\n      } catch (error) {\n        console.error('Greška pri registraciji deteta:', error);\n        this.errorMessage = 'Greška prilikom registracije deteta. Molimo pokušajte ponovo.';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","newChild","name","birthDate","developmentStage","errorMessage","methods","submitChildForm","token","localStorage","getItem","response","post","headers","Authorization","console","log","$router","push","error"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentForm.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Registracija Deteta</h2>\r\n    <form @submit.prevent=\"submitChildForm\">\r\n      <div>\r\n        <label for=\"name\">Ime:</label>\r\n        <input v-model=\"newChild.name\" type=\"text\" required />\r\n      </div>\r\n      <div>\r\n        <label for=\"birthDate\">Datum rođenja:</label>\r\n        <input v-model=\"newChild.birthDate\" type=\"date\" required />\r\n      </div>\r\n      <div>\r\n        <label for=\"developmentStage\">Faza razvoja:</label>\r\n        <input v-model=\"newChild.developmentStage\" type=\"text\" required />\r\n      </div>\r\n      <button type=\"submit\">Registruj Dete</button>\r\n    </form>\r\n\r\n    <div v-if=\"errorMessage\" class=\"error-message\">\r\n      {{ errorMessage }}\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      newChild: {\r\n        name: '',\r\n        birthDate: '',\r\n        developmentStage: ''\r\n      },\r\n      errorMessage: '',\r\n    };\r\n  },\r\n  methods: {\r\n    async submitChildForm() {\r\n      const token = localStorage.getItem('token');\r\n      if (!token) {\r\n        this.errorMessage = 'Morate biti ulogovani da biste dodali dete.';\r\n        return;\r\n      }\r\n\r\n      // Provera da li su sva polja popunjena\r\n      if (!this.newChild.name || !this.newChild.birthDate || !this.newChild.developmentStage) {\r\n        this.errorMessage = 'Sva polja su obavezna.';\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.post(\r\n          'http://localhost:5000/api/children', \r\n          this.newChild,\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${token}`, // Poslat ćemo token u header-u\r\n            },\r\n          }\r\n        );\r\n\r\n        console.log('Podaci su uspešno poslati:', response);\r\n\r\n        // Preusmerite korisnika na child-development\r\n        this.$router.push('/child-development');\r\n      } catch (error) {\r\n        console.error('Greška pri registraciji deteta:', error);\r\n        this.errorMessage = 'Greška prilikom registracije deteta. Molimo pokušajte ponovo.';\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r