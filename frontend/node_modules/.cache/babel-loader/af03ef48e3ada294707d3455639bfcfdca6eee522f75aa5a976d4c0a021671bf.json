{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      developmentData: [],\n      milestoneData: [],\n      availablePeriods: [],\n      childName: null,\n      childAge: null,\n      selectedPeriod: null,\n      error: null,\n      loading: true\n    };\n  },\n  computed: {\n    currentPeriodMilestones() {\n      if (!this.selectedPeriod) return [];\n      return this.milestoneData.filter(milestone => milestone.age >= this.selectedPeriod.start && milestone.age < this.selectedPeriod.end);\n    },\n    currentPeriodDevelopment() {\n      if (!this.selectedPeriod) return null;\n      return this.developmentData.find(dev => dev.startAge === this.selectedPeriod.start && dev.endAge === this.selectedPeriod.end);\n    }\n  },\n  methods: {\n    async fetchChildData(childId) {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (response.data) {\n          this.childName = response.data.name;\n          this.childAge = this.calculateAgeInMonths(new Date(response.data.birthDate));\n        }\n      } catch (error) {\n        console.error('‚ùå Gre≈°ka pri dobijanju podataka o detetu:', error);\n      }\n    },\n    async fetchMilestones(childId) {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/milestones/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.milestoneData = response.data;\n      } catch (error) {\n        console.error(\"‚ùå Gre≈°ka pri uƒçitavanju kljuƒçnih momenata:\", error);\n      }\n    },\n    async fetchDevelopmentData(childId) {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.developmentData = response.data;\n      } catch (error) {\n        console.error(\"‚ùå Gre≈°ka pri uƒçitavanju podataka o razvoju:\", error);\n      }\n    },\n    calculateAgeInMonths(birthDate) {\n      const birth = new Date(birthDate);\n      const now = new Date();\n      return (now.getFullYear() - birth.getFullYear()) * 12 + (now.getMonth() - birth.getMonth());\n    },\n    togglePeriod(period) {\n      if (this.selectedPeriod && this.selectedPeriod.start === period.start) {\n        this.selectedPeriod = null; // Collapse\n      } else {\n        this.selectedPeriod = period; // Expand\n      }\n    },\n    getPeriodClass(period) {\n      return {\n        'period-item': true,\n        'filled': this.isPeriodFilled(period),\n        'current': this.isCurrentPeriod(period),\n        'past': this.isPastPeriod(period),\n        'future': this.isFuturePeriod(period),\n        'clickable': !this.isFuturePeriod(period),\n        'selected': this.selectedPeriod && this.selectedPeriod.start === period.start\n      };\n    }\n  },\n  async created() {\n    try {\n      const childId = this.$route.params.childId;\n      if (childId) {\n        await this.fetchChildData(childId);\n        await this.fetchMilestones(childId);\n        await this.fetchDevelopmentData(childId);\n      }\n    } catch (error) {\n      console.error('‚ùå Gre≈°ka u created():', error);\n    }\n  }\n};","map":{"version":3,"names":["axios","data","developmentData","milestoneData","availablePeriods","childName","childAge","selectedPeriod","error","loading","computed","currentPeriodMilestones","filter","milestone","age","start","end","currentPeriodDevelopment","find","dev","startAge","endAge","methods","fetchChildData","childId","token","sessionStorage","getItem","response","get","headers","Authorization","name","calculateAgeInMonths","Date","birthDate","console","fetchMilestones","fetchDevelopmentData","birth","now","getFullYear","getMonth","togglePeriod","period","getPeriodClass","isPeriodFilled","isCurrentPeriod","isPastPeriod","isFuturePeriod","created","$route","params"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentPeriod.vue"],"sourcesContent":["<template>\r\n  <div class=\"development-container\">\r\n    <div v-if=\"error\" class=\"error-message\">{{ error }}</div>\r\n\r\n    <h2>Pregled Razvoja Deteta</h2>\r\n\r\n    <!-- Child Info -->\r\n    <div class=\"child-info\">\r\n      <h3>{{ childName ? childName : 'Nepoznato dete' }}</h3>\r\n      <p><strong>Starost:</strong> {{ childAge ? childAge + ' meseci' : 'Nepoznata starost' }}</p>\r\n    </div>\r\n\r\n    <!-- Period selector -->\r\n    <div class=\"available-periods\" v-if=\"availablePeriods.length > 0\">\r\n      <h3>Dostupni periodi:</h3>\r\n      <div class=\"periods-grid\">\r\n        <button v-for=\"(period, index) in availablePeriods\" \r\n                :key=\"index\" \r\n                :class=\"getPeriodClass(period)\"\r\n                @click=\"togglePeriod(period)\">\r\n          {{ formatPeriod(period.start, period.end) }}\r\n          <span v-if=\"isPeriodFilled(period)\" class=\"period-status\">(Ispunjeno ‚úÖ)</span>\r\n          <span v-else-if=\"isCurrentPeriod(period)\" class=\"period-status current\">(Trenutni üü¢)</span>\r\n          <span v-else-if=\"isPastPeriod(period)\" class=\"period-status past clickable\">(Kliknite üîÑ)</span>\r\n          <span v-else class=\"period-status future\">(Nije dostupno üö´)</span>\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Selected Period Info - Expand Below -->\r\n    <div v-if=\"selectedPeriod\" class=\"selected-period\">\r\n      <h3>Izabrani period: {{ formatPeriod(selectedPeriod.start, selectedPeriod.end) }}</h3>\r\n\r\n      <!-- Display Milestones -->\r\n      <div class=\"milestone-section\">\r\n        <h3>Kljuƒçni momenti</h3>\r\n        <ul>\r\n          <li v-for=\"milestone in currentPeriodMilestones\" :key=\"milestone._id\">\r\n            <strong>{{ milestone.milestoneType }}</strong> - {{ milestone.progress }}\r\n          </li>\r\n        </ul>\r\n        <p v-if=\"currentPeriodMilestones.length === 0\">Nema kljuƒçnih momenata.</p>\r\n      </div>\r\n\r\n      <!-- Display Development Data -->\r\n      <div class=\"development-section\">\r\n        <h3>Razvoj deteta</h3>\r\n        <p><strong>Fiziƒçki razvoj:</strong> {{ currentPeriodDevelopment?.physicalDevelopment || 'Nema podataka' }}</p>\r\n        <p><strong>Emocionalni razvoj:</strong> {{ currentPeriodDevelopment?.emotionalDevelopment || 'Nema podataka' }}</p>\r\n        <p><strong>Intelektualni razvoj:</strong> {{ currentPeriodDevelopment?.intellectualDevelopment || 'Nema podataka' }}</p>\r\n        <p><strong>Socijalni razvoj:</strong> {{ currentPeriodDevelopment?.socialDevelopment || 'Nema podataka' }}</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return { \r\n      developmentData: [], \r\n      milestoneData: [],\r\n      availablePeriods: [],\r\n      childName: null,\r\n      childAge: null, \r\n      selectedPeriod: null,\r\n      error: null,\r\n      loading: true\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    currentPeriodMilestones() {\r\n      if (!this.selectedPeriod) return [];\r\n      return this.milestoneData.filter(milestone => \r\n        milestone.age >= this.selectedPeriod.start && \r\n        milestone.age < this.selectedPeriod.end\r\n      );\r\n    },\r\n\r\n    currentPeriodDevelopment() {\r\n      if (!this.selectedPeriod) return null;\r\n      return this.developmentData.find(dev => \r\n        dev.startAge === this.selectedPeriod.start && \r\n        dev.endAge === this.selectedPeriod.end\r\n      );\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    async fetchChildData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n\r\n        if (response.data) {\r\n          this.childName = response.data.name;\r\n          this.childAge = this.calculateAgeInMonths(new Date(response.data.birthDate));\r\n        }\r\n      } catch (error) {\r\n        console.error('‚ùå Gre≈°ka pri dobijanju podataka o detetu:', error);\r\n      }\r\n    },\r\n\r\n    async fetchMilestones(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(`http://localhost:5000/api/milestones/${childId}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n        this.milestoneData = response.data;\r\n      } catch (error) {\r\n        console.error(\"‚ùå Gre≈°ka pri uƒçitavanju kljuƒçnih momenata:\", error);\r\n      }\r\n    },\r\n\r\n    async fetchDevelopmentData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n        this.developmentData = response.data;\r\n      } catch (error) {\r\n        console.error(\"‚ùå Gre≈°ka pri uƒçitavanju podataka o razvoju:\", error);\r\n      }\r\n    },\r\n\r\n    calculateAgeInMonths(birthDate) {\r\n      const birth = new Date(birthDate);\r\n      const now = new Date();\r\n      return (now.getFullYear() - birth.getFullYear()) * 12 + (now.getMonth() - birth.getMonth());\r\n    },\r\n\r\n    togglePeriod(period) {\r\n      if (this.selectedPeriod && this.selectedPeriod.start === period.start) {\r\n        this.selectedPeriod = null; // Collapse\r\n      } else {\r\n        this.selectedPeriod = period; // Expand\r\n      }\r\n    },\r\n\r\n    getPeriodClass(period) {\r\n      return {\r\n        'period-item': true,\r\n        'filled': this.isPeriodFilled(period),\r\n        'current': this.isCurrentPeriod(period),\r\n        'past': this.isPastPeriod(period),\r\n        'future': this.isFuturePeriod(period),\r\n        'clickable': !this.isFuturePeriod(period),\r\n        'selected': this.selectedPeriod && this.selectedPeriod.start === period.start\r\n      };\r\n    }\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      const childId = this.$route.params.childId;\r\n      if (childId) {\r\n        await this.fetchChildData(childId);\r\n        await this.fetchMilestones(childId);\r\n        await this.fetchDevelopmentData(childId);\r\n      }\r\n    } catch (error) {\r\n      console.error('‚ùå Gre≈°ka u created():', error);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.development-container {\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.child-info {\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.periods-grid {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  gap: 15px;\r\n}\r\n\r\n.period-item {\r\n  padding: 15px;\r\n  font-size: 18px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.selected-period {\r\n  margin-top: 20px;\r\n  background: #f8f9fa;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n}\r\n\r\n.milestone-section, .development-section {\r\n  margin-top: 20px;\r\n  text-align: left;\r\n  max-width: 600px;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n</style>\r\n"],"mappings":";;;AAyDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,EAAE;MACnBC,aAAa,EAAE,EAAE;MACjBC,gBAAgB,EAAE,EAAE;MACpBC,SAAS,EAAE,IAAI;MACfC,QAAQ,EAAE,IAAI;MACdC,cAAc,EAAE,IAAI;