{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  props: ['childId', 'milestoneId'],\n  data() {\n    return {\n      newMilestone: {\n        milestoneType: '',\n        milestoneDate: '',\n        progress: '',\n        notes: ''\n      },\n      errorMessage: ''\n    };\n  },\n  created() {\n    // If editing, fetch the existing milestone data\n    if (this.milestoneId) {\n      this.fetchMilestoneData();\n    }\n  },\n  methods: {\n    async fetchMilestoneData() {\n      try {\n        const token = localStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/milestones/${this.milestoneId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        // Pre-fill the form with existing milestone data\n        this.newMilestone = response.data;\n      } catch (error) {\n        console.error('Error fetching milestone data:', error);\n        this.errorMessage = 'Failed to fetch milestone data';\n      }\n    },\n    async submitMilestone() {\n      const token = localStorage.getItem('token');\n      if (!token) {\n        this.errorMessage = 'You must be logged in to submit a milestone.';\n        return;\n      }\n      if (!this.newMilestone.milestoneType || !this.newMilestone.milestoneDate || !this.newMilestone.progress) {\n        this.errorMessage = 'All fields are required.';\n        return;\n      }\n      try {\n        let response;\n        if (this.milestoneId) {\n          // If editing, send a PUT request to update the milestone\n          response = await axios.put(`http://localhost:5000/api/milestones/${this.milestoneId}`, this.newMilestone, {\n            headers: {\n              Authorization: `Bearer ${token}`\n            }\n          });\n        } else {\n          // If adding, send a POST request to add the milestone\n          response = await axios.post(`http://localhost:5000/api/milestones`, {\n            ...this.newMilestone,\n            childId: this.childId\n          }, {\n            headers: {\n              Authorization: `Bearer ${token}`\n            }\n          });\n        }\n        console.log('Milestone saved:', response);\n        // Redirect to milestone list after saving\n        this.$router.push(`/milestone-list/${this.childId}`);\n      } catch (error) {\n        console.error('Error saving milestone:', error);\n        this.errorMessage = 'Failed to save milestone data.';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","props","data","newMilestone","milestoneType","milestoneDate","progress","notes","errorMessage","created","milestoneId","fetchMilestoneData","methods","token","localStorage","getItem","response","get","headers","Authorization","error","console","submitMilestone","put","post","childId","log","$router","push"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\MilestoneForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"milestone-form\">\r\n    <h2>{{ milestoneId ? 'Edit Milestone' : 'Add Milestone' }}</h2>\r\n    <form @submit.prevent=\"submitMilestone\" class=\"form-container\">\r\n      <div class=\"form-group\">\r\n        <label for=\"milestoneType\">Milestone Tip:</label>\r\n        <input\r\n          v-model=\"newMilestone.milestoneType\"\r\n          required\r\n          type=\"text\"\r\n          class=\"form-input\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"milestoneDate\">Milestone Podaci:</label>\r\n        <input\r\n          v-model=\"newMilestone.milestoneDate\"\r\n          required\r\n          type=\"date\"\r\n          class=\"form-input\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"progress\">Napredak:</label>\r\n        <input\r\n          v-model=\"newMilestone.progress\"\r\n          required\r\n          type=\"text\"\r\n          class=\"form-input\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"notes\">Beleske:</label>\r\n        <textarea\r\n          v-model=\"newMilestone.notes\"\r\n          class=\"form-input\"\r\n          rows=\"4\"\r\n        ></textarea>\r\n      </div>\r\n      <button type=\"submit\" class=\"submit-btn\">\r\n        {{ milestoneId ? 'Azuriraj Milestone' : 'Dodaj Milestone' }}\r\n      </button>\r\n    </form>\r\n    <div v-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: ['childId', 'milestoneId'],\r\n  data() {\r\n    return {\r\n      newMilestone: {\r\n        milestoneType: '',\r\n        milestoneDate: '',\r\n        progress: '',\r\n        notes: '',\r\n      },\r\n      errorMessage: '',\r\n    };\r\n  },\r\n  created() {\r\n    // If editing, fetch the existing milestone data\r\n    if (this.milestoneId) {\r\n      this.fetchMilestoneData();\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchMilestoneData() {\r\n      try {\r\n        const token = localStorage.getItem('token');\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/milestones/${this.milestoneId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          }\r\n        );\r\n        // Pre-fill the form with existing milestone data\r\n        this.newMilestone = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching milestone data:', error);\r\n        this.errorMessage = 'Failed to fetch milestone data';\r\n      }\r\n    },\r\n    async submitMilestone() {\r\n      const token = localStorage.getItem('token');\r\n      if (!token) {\r\n        this.errorMessage = 'You must be logged in to submit a milestone.';\r\n        return;\r\n      }\r\n\r\n      if (!this.newMilestone.milestoneType || !this.newMilestone.milestoneDate || !this.newMilestone.progress) {\r\n        this.errorMessage = 'All fields are required.';\r\n        return;\r\n      }\r\n\r\n      try {\r\n        let response;\r\n        if (this.milestoneId) {\r\n          // If editing, send a PUT request to update the milestone\r\n          response = await axios.put(\r\n            `http://localhost:5000/api/milestones/${this.milestoneId}`,\r\n            this.newMilestone,\r\n            { headers: { Authorization: `Bearer ${token}` } }\r\n          );\r\n        } else {\r\n          // If adding, send a POST request to add the milestone\r\n          response = await axios.post(\r\n            `http://localhost:5000/api/milestones`,\r\n            { ...this.newMilestone, childId: this.childId },\r\n            { headers: { Authorization: `Bearer ${token}` } }\r\n          );\r\n        }\r\n\r\n        console.log('Milestone saved:', response);\r\n        // Redirect to milestone list after saving\r\n        this.$router.push(`/milestone-list/${this.childId}`);\r\n      } catch (error) {\r\n        console.error('Error saving milestone:', error);\r\n        this.errorMessage = 'Failed to save milestone data.';\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.milestone-form {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f9f9f9; /* Soft light background */\r\n  border-radius: 8px;\r\n  box-shadow: 0 6px 15px rgba(0, 0, 0, 0.1);\r\n  \r\n  h2 {\r\n    text-align: center;\r\n    font-size: 26px;\r\n    color: #333;\r\n    margin-bottom: 30px;\r\n    font-weight: 600; /* Make the title bold */\r\n    text-transform: uppercase; /* Modern style */\r\n    letter-spacing: 1px;\r\n  }\r\n\r\n  .error-message {\r\n    margin-bottom: 20px;\r\n    padding: 12px;\r\n    background-color: #f8d7da;\r\n    border: 1px solid #f5c6cb;\r\n    color: #721c24;\r\n    border-radius: 6px;\r\n    font-size: 14px;\r\n  }\r\n\r\n  .form-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n\r\n    .form-group {\r\n      margin-bottom: 18px;\r\n\r\n      label {\r\n        font-size: 18px;\r\n        font-weight: 500;\r\n        color: #333;\r\n        margin-bottom: 8px;\r\n      }\r\n\r\n      input,\r\n      textarea {\r\n        width: 100%;\r\n        padding: 12px;\r\n        font-size: 16px;\r\n        border: 1px solid #ddd;\r\n        border-radius: 8px;\r\n        box-sizing: border-box;\r\n        transition: border-color 0.3s ease;\r\n\r\n        &:focus {\r\n          border-color: #007bff;\r\n          outline: none;\r\n        }\r\n      }\r\n\r\n      textarea {\r\n        height: 120px;\r\n        resize: vertical;\r\n      }\r\n    }\r\n\r\n    .submit-group {\r\n      display: flex;\r\n      justify-content: center;\r\n      \r\n      .submit-btn {\r\n        padding: 14px 25px;\r\n        font-size: 18px;\r\n        color: #fff;\r\n        border: none;\r\n        border-radius: 6px;\r\n        cursor: pointer;\r\n        transition: background-color 0.3s ease, transform 0.2s ease;\r\n\r\n        &.add-btn {\r\n          background-color: #4CAF50; /* Green color for Add button */\r\n        }\r\n\r\n        &.update-btn {\r\n          background-color: #f1c40f; /* Yellow for Update button */\r\n        }\r\n\r\n        &:hover {\r\n          transform: scale(1.05); /* Slight scale effect for hover */\r\n        }\r\n\r\n        &:active {\r\n          background-color: #388e3c; /* Darker shade when clicked */\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AAgDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE,CAAC,SAAS,EAAE,aAAa,CAAC;EACjCC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE;QACZC,aAAa,EAAE,EAAE;QACjBC,aAAa,EAAE,EAAE;QACjBC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE;MACT,CAAC;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,IAAI,CAACC,WAAW,EAAE;MACpB,IAAI,CAACC,kBAAkB,CAAC,CAAC;IAC3B;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,kBAAkBA,CAAA,EAAG;MACzB,IAAI;QACF,MAAME,KAAI,GAAIC