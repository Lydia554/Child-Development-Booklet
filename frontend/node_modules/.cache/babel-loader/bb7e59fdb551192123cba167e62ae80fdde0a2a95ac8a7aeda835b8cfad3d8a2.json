{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport axios from 'axios';\nimport { useAuthStore } from '../stores/authStore';\nexport default {\n  data() {\n    return {\n      periodRecords: [],\n      childName: \"\",\n      selectedPeriod: null,\n      availablePeriods: []\n    };\n  },\n  computed: {\n    sortedRecords() {\n      return [...this.periodRecords].sort((a, b) => a.startAge - b.startAge);\n    }\n  },\n  created() {\n    const childId = this.$route.params.childId;\n    if (childId) {\n      this.generateAvailablePeriods();\n      this.fetchDevelopmentData(childId);\n      this.fetchChildName(childId);\n    } else {\n      console.error('No child ID found');\n      this.$router.push('/child-registration');\n    }\n  },\n  methods: {\n    generateAvailablePeriods() {\n      // Generate periods from 0 to 6 years (72 months) in 6-month intervals\n      const periods = [];\n      for (let i = 0; i <= 72; i += 6) {\n        periods.push({\n          start: i,\n          end: i + 6\n        });\n      }\n      this.availablePeriods = periods;\n      this.selectedPeriod = periods[0]; // Select first period by default\n    },\n    formatPeriod(start, end) {\n      if (start === 0) {\n        return `0-6 meseci`;\n      }\n      return `${start}-${end} meseci`;\n    },\n    async fetchDevelopmentData(childId) {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.periodRecords = response.data.periods || [];\n      } catch (error) {\n        console.error('Error fetching development data:', error);\n      }\n    },\n    async fetchChildName(childId) {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.childName = response.data.name;\n      } catch (error) {\n        console.error('Error fetching child name:', error);\n      }\n    },\n    handlePeriodChange() {\n      // Filter records for selected period\n      const filteredRecords = this.periodRecords.filter(record => record.startAge === this.selectedPeriod.start && record.endAge === this.selectedPeriod.end);\n      this.periodRecords = filteredRecords;\n    }\n  }\n};","map":{"version":3,"names":["axios","useAuthStore","data","periodRecords","childName","selectedPeriod","availablePeriods","computed","sortedRecords","sort","a","b","startAge","created","childId","$route","params","generateAvailablePeriods","fetchDevelopmentData","fetchChildName","console","error","$router","push","methods","periods","i","start","end","formatPeriod","token","sessionStorage","getItem","response","get","headers","Authorization","name","handlePeriodChange","filteredRecords","filter","record","endAge"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentPeriod.vue"],"sourcesContent":["<template>\r\n  <div class=\"development-tracker\">\r\n    <h2>Praćenje Razvoja za {{ childName }}</h2>\r\n    \r\n    <!-- Period selector -->\r\n    <div class=\"period-selector\">\r\n      <h3>Izaberite period:</h3>\r\n      <select v-model=\"selectedPeriod\" @change=\"handlePeriodChange\">\r\n        <option v-for=\"period in availablePeriods\" \r\n                :key=\"`${period.start}-${period.end}`\"\r\n                :value=\"period\">\r\n          {{ formatPeriod(period.start, period.end) }}\r\n        </option>\r\n      </select>\r\n    </div>\r\n\r\n    <!-- Display records -->\r\n    <div v-if=\"periodRecords.length\" class=\"period-records\">\r\n      <div v-for=\"record in sortedRecords\" :key=\"record._id\" class=\"period-section\">\r\n        <h3>{{ formatPeriod(record.startAge, record.endAge) }}</h3>\r\n        <ul>\r\n          <li><strong>Fizički:</strong> {{ record.physicalDevelopment }}</li>\r\n          <li><strong>Emocionalni:</strong> {{ record.emotionalDevelopment }}</li>\r\n          <li><strong>Intelektualni:</strong> {{ record.intellectualDevelopment }}</li>\r\n          <li><strong>Socijalni:</strong> {{ record.socialDevelopment }}</li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n    <div v-else class=\"no-records\">\r\n      <p>Nema unetih podataka za ovaj period.</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { useAuthStore } from '../stores/authStore';\r\n\r\nexport default {\r\n  data() {\r\n    return { \r\n      periodRecords: [],\r\n      childName: \"\",\r\n      selectedPeriod: null,\r\n      availablePeriods: []\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    sortedRecords() {\r\n      return [...this.periodRecords].sort((a, b) => a.startAge - b.startAge);\r\n    }\r\n  },\r\n\r\n  created() {\r\n    const childId = this.$route.params.childId;\r\n    \r\n    if (childId) {\r\n      this.generateAvailablePeriods();\r\n      this.fetchDevelopmentData(childId);\r\n      this.fetchChildName(childId);\r\n    } else {\r\n      console.error('No child ID found');\r\n      this.$router.push('/child-registration');\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    generateAvailablePeriods() {\r\n      // Generate periods from 0 to 6 years (72 months) in 6-month intervals\r\n      const periods = [];\r\n      for (let i = 0; i <= 72; i += 6) {\r\n        periods.push({\r\n          start: i,\r\n          end: i + 6\r\n        });\r\n      }\r\n      this.availablePeriods = periods;\r\n      this.selectedPeriod = periods[0]; // Select first period by default\r\n    },\r\n\r\n    formatPeriod(start, end) {\r\n      if (start === 0) {\r\n        return `0-6 meseci`;\r\n      }\r\n      return `${start}-${end} meseci`;\r\n    },\r\n\r\n    async fetchDevelopmentData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` }\r\n          }\r\n        );\r\n        this.periodRecords = response.data.periods || [];\r\n      } catch (error) {\r\n        console.error('Error fetching development data:', error);\r\n      }\r\n    },\r\n\r\n    async fetchChildName(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/children/${childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` }\r\n          }\r\n        );\r\n        this.childName = response.data.name;\r\n      } catch (error) {\r\n        console.error('Error fetching child name:', error);\r\n      }\r\n    },\r\n\r\n    handlePeriodChange() {\r\n      // Filter records for selected period\r\n      const filteredRecords = this.periodRecords.filter(record => \r\n        record.startAge === this.selectedPeriod.start && \r\n        record.endAge === this.selectedPeriod.end\r\n      );\r\n      this.periodRecords = filteredRecords;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.development-tracker {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n\r\n  .period-selector {\r\n    margin: 20px 0;\r\n    \r\n    select {\r\n      padding: 8px;\r\n      font-size: 16px;\r\n      border-radius: 4px;\r\n      width: 200px;\r\n    }\r\n  }\r\n\r\n  .period-section {\r\n    background-color: #fff;\r\n    padding: 20px;\r\n    margin: 15px 0;\r\n    border-radius: 8px;\r\n    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\r\n\r\n    ul {\r\n      list-style: none;\r\n      padding: 0;\r\n\r\n      li {\r\n        margin: 10px 0;\r\n        \r\n        strong {\r\n          color: #333;\r\n          margin-right: 10px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  .no-records {\r\n    text-align: center;\r\n    color: #666;\r\n    margin: 20px 0;\r\n  }\r\n}\r\n</style>\r\n  "],"mappings":";;;AAmCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,YAAW,QAAS,qBAAqB;AAElD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,EAAE;MACjBC,SAAS,EAAE,EAAE;MACbC,cAAc,EAAE,IAAI;MACpBC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EAEDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,OAAO,CAAC,GAAG,IAAI,CAACL,aAAa,CAAC,CAACM,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKD,CAAC,CAACE,QAAO,GAAID,CAAC,CAACC,QAAQ,CAAC;IACxE;EACF,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACR,MAAMC,OAAM,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,OAAO;IAE1C,IAAIA,OAAO,EAAE;MACX,IAAI,CAACG,wBAAwB,CAAC,CAAC;MAC/B,IAAI,CAACC,oBAAoB,CAACJ,OAAO,CAAC;MAClC,IAAI,CAACK,cAAc,CAACL,OAAO,CAAC;IAC9B,OAAO;MACLM,OAAO,CAACC,KAAK,CAAC,mBAAmB,CAAC;MAClC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,qBAAqB,CAAC;IAC1C;EACF,CAAC;EAEDC,OAAO,EAAE;IACPP,wBAAwBA,CAAA,EAAG;MACzB;MACA,MAAMQ,OAAM,GAAI,EAAE;MAClB,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,IAAK,EAAE,EAAEA,CAAA,IAAK,CAAC,EAAE;QAC/BD,OAAO,CAACF,IAAI,CAAC;UACXI,KAAK,EAAED,CAAC;UACRE,GAAG,EAAEF,CAAA,GAAI;QACX,CAAC,CAAC;MACJ;MACA,IAAI,CAACpB,gBAAe,GAAImB,OAAO;MAC/B,IAAI,CAACpB,cAAa,GAAIoB,OAAO,CAAC,CAAC,CAAC,EAAE;IACpC,CAAC;IAEDI,YAAYA,CAACF,KAAK,EAAEC,GAAG,EAAE;MACvB,IAAID,KAAI,KAAM,CAAC,EAAE;QACf,OAAO,YAAY;MACrB;MACA,OAAO,GAAGA,KAAK,IAAIC,GAAG,SAAS;IACjC,CAAC;IAED,MAAMV,oBAAoBA,CAACJ,OAAO,EAAE;MAClC,IAAI;QACF,MAAMgB,KAAI,GAAIC,cAAc,CAACC,OAAO,CAAC,OAAO,CAAC;QAC7C,MAAMC,QAAO,GAAI,MAAMjC,KAAK,CAACkC,GAAG,CAC9B,+CAA+CpB,OAAO,EAAE,EACxD;UACEqB,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUN,KAAK;UAAG;QAC9C,CACF,CAAC;QACD,IAAI,CAAC3B,aAAY,GAAI8B,QAAQ,CAAC/B,IAAI,CAACuB,OAAM,IAAK,EAAE;MAClD,EAAE,OAAOJ,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;MAC1D;IACF,CAAC;IAED,MAAMF,cAAcA,CAACL,OAAO,EAAE;MAC5B,IAAI;QACF,MAAMgB,KAAI,GAAIC,cAAc,CAACC,OAAO,CAAC,OAAO,CAAC;QAC7C