n        <div class=\"button-group\">\r\n          <button @click=\"editMilestone(milestone._id)\" class=\"edit-btn\">Izmeni</button>\r\n          <button @click=\"confirmDelete(milestone._id)\" class=\"delete-btn\">Obriši</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else class=\"no-data-message\">\r\n      <p>Nema podataka o Kljucnim Momentima za ovo dete.</p>\r\n    </div>\r\n\r\n    <router-link :to=\"`/milestone-form/${childId}`\" class=\"add-milestone-button\">\r\n      Dodaj Kljucni Momenat\r\n    </router-link>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useAuthStore } from '@/stores/authStore';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      milestones: [],\r\n      childId: null,\r\n    };\r\n  },\r\n  \r\n  created() {\r\n    const authStore = useAuthStore();\r\n    this.childId = authStore.childId || sessionStorage.getItem('childId');\r\n\r\n    if (!this.childId) {\r\n      this.$router.push('/child-registration');\r\n      return;\r\n    }\r\n\r\n    this.fetchMilestones();\r\n  },\r\n\r\n  methods: {\r\n    async fetchMilestones() {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        if (!token) {\r\n          console.error('User not authenticated');\r\n          return;\r\n        }\r\n\r\n        const response = await axios.get(`http://localhost:5000/api/milestones/child/${this.childId}`, {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n\r\n        if (Array.isArray(response.data)) {\r\n          this.milestones = response.data;\r\n        } else {\r\n          console.error('Expected array but got:', response.data);\r\n        }\r\n      } catch (error) {\r\n        console.error('Error fetching milestones:', error);\r\n      }\r\n    },\r\n\r\n    formatDate(value) {\r\n      if (!value) return '';\r\n      const date = new Date(value);\r\n      return `${date.getDate()}/${date.getMonth() + 1}/${date.getFullYear()}`;\r\n    },\r\n\r\n    editMilestone(id) {\r\n      this.$router.push(`/milestone-form/${this.childId}/${id}`);\r\n    },\r\n\r\n    async confirmDelete(id) {\r\n      if (confirm('Da li ste sigurni da želite da obrišete ovaj milestone?')) {\r\n        await this.deleteMilestone(id);\r\n      }\r\n    },\r\n\r\n    async deleteMilestone(id) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        await axios.delete(`http://localhost:5000/api/milestones/${id}`, {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n        \r\n        // Refresh the list after deletion\r\n        await this.fetchMilestones();\r\n      } catch (error) {\r\n        console.error('Greška prilikom brisanja ključnog momenta:', error);\r\n        alert('Došlo je do greške prilikom brisanja. Molimo pokušajte ponovo.');\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.milestone-list-container {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f7f7f7;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n\r\n  h2 {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: #333;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .milestone-list {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 15px;\r\n  }\r\n\r\n  .milestone-item {\r\n    background-color: #fff;\r\n    padding: 20px;\r\n    border-radius: 8px;\r\n    box-shadow: 0 2px 6px rgba(0, 0, 0, 0.1);\r\n\r\n    h3 {\r\n      font-size: 20px;\r\n      color: #333;\r\n      margin-bottom: 10px;\r\n    }\r\n\r\n    p {\r\n      margin: 8px 0;\r\n      color: #555;\r\n    }\r\n\r\n    .button-group {\r\n      display: flex;\r\n      gap: 10px;\r\n      margin-top: 15px;\r\n    }\r\n\r\n    .edit-btn,\r\n    .delete-btn {\r\n      padding: 8px 16px;\r\n      border: none;\r\n      border-radius: 4px;\r\n      cursor: pointer;\r\n      font-weight: 500;\r\n      transition: all 0.3s ease;\r\n    }\r\n\r\n    .edit-btn {\r\n      background-color: #28a745;\r\n      color: white;\r\n\r\n      &:hover {\r\n        background-color: #218838;\r\n      }\r\n    }\r\n\r\n    .delete-btn {\r\n      background-color: #dc3545;\r\n      color: white;\r\n\r\n      &:hover {\r\n        background-color: #c82333;\r\n      }\r\n    }\r\n  }\r\n\r\n  .no-data-message {\r\n    text-align: center;\r\n    padding: 20px;\r\n    background-color: #fff;\r\n    border-radius: 8px;\r\n    color: #666;\r\n  }\r\n\r\n  .add-milestone-button {\r\n    display: inline-block;\r\n    margin-top: 20px;\r\n    padding: 12px 24px;\r\n    background-color: #007bff;\r\n    color: white;\r\n    text-decoration: none;\r\n    border-radius: 4px;\r\n    transition: background-color 0.3s ease;\r\n\r\n    &:hover {\r\n      background-color: #0056b3;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAA0B;;EADvCC,GAAA;EAGkCD,KAAK,EAAC;;;EAM3BA,KAAK,EAAC;AAAc;mBATjC;mBAAA;;EAAAC,GAAA;EAgBgBD,KAAK,EAAC;;;;uBAfpBE,mBAAA,CAsBM,OAtBNC,UAsBM,G,0BArBJC,mBAAA,CAAkC,YAA9B,2BAAyB,sBAClBC,KAAA,CAAAC,UAAU,CAACC,MAAM,I,cAA5BL,mBAAA,CAWM,OAXNM,UAWM,I,kBAVJN,mBAAA,CASMO,SAAA,QAbZC,WAAA,CAIwCL,KAAA,CAAAC,UAAU,EAJlD,CAImBK,SAAS,EAAEC,KAAK;yBAA7BV,mBAAA,CASM;MATyCD,GAAG,EAAEW,KAAK;MAAEZ,KAAK,EAAC;QAC/DI,mBAAA,CAAkF,YAAAS,gBAAA,CAA3EF,SAAS,CAACG,aAAa,IAAG,KAAG,GAAAD,gBAAA,CAAGE,QAAA,CAAAC,UAAU,CAACL,SAAS,CAACM,aAAa,mBACzEb,mBAAA,CAA0D,Y,0BAAvDA,mBAAA,CAA0B,gBAAlB,WAAS,sBAN5Bc,gBAAA,CAMqC,GAAC,GAAAL,gBAAA,CAAGF,SAAS,CAACQ,QAAQ,iB,GACnDf,mBAAA,CAAuD,Y,0BAApDA,mBAAA,CAA0B,gBAAlB,WAAS,sBAP5Bc,gBAAA,CAOqC,GAAC,GAAAL,gBAAA,CAAGF,SAAS,CAACS,KAAK,iB,GAEhDhB,mBAAA,CAGM,OAHNiB,UAGM,GAFJjB,mBAAA,CAA8E;MAArEkB,OAAK,EAAAC,MAAA,IAAER,QAAA,CAAAS,aAAa,CAACb,SAAS,CAACc,GAAG;MAAGzB,KAAK,EAAC;OAAW,QAAM,iBAV/E0B,UAAA,GAWUtB,mBAAA,CAAgF;MAAvEkB,OAAK,EAAAC,MAAA,IAAER,QAAA,CAAAY,aAAa,CAAChB,SAAS,CAACc,GAAG;MAAGzB,KAAK,EAAC;OAAa,QAAM,iBAXjF4B,UAAA,E;qDAgBI1B,mBAAA,CAEM,OAFN2B,UAEM,EA