{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useAuthStore } from '@/stores/authStore';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      loading: true,\n      currentChild: null,\n      errorMessage: '',\n      developmentPeriods: []\n    };\n  },\n  async created() {\n    await this.fetchCurrentChild();\n  },\n  methods: {\n    async fetchCurrentChild() {\n      try {\n        const authStore = useAuthStore();\n        let childId = authStore.childId || localStorage.getItem('childId');\n        console.log(\"üîç Fetching child data for ID:\", childId);\n        if (!childId || childId.length !== 24) {\n          console.warn(\"‚ö†Ô∏è Invalid or missing childId. Redirecting to registration.\");\n          this.errorMessage = \"Nema registrovanog deteta. Molimo registrujte dete.\";\n          return;\n        }\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        if (response.data) {\n          this.currentChild = response.data;\n          this.calculateDevelopmentPeriods();\n        } else {\n          this.errorMessage = \"Podaci o detetu nisu pronaƒëeni.\";\n        }\n      } catch (error) {\n        console.error(\"‚ùå Error fetching child data:\", error);\n        this.errorMessage = \"Gre≈°ka prilikom uƒçitavanja podataka. Poku≈°ajte ponovo.\";\n      } finally {\n        this.loading = false;\n      }\n    },\n    calculateDevelopmentPeriods() {\n      if (!this.currentChild || !this.currentChild.birthDate) return;\n      const birthDate = new Date(this.currentChild.birthDate);\n      const today = new Date();\n      const ageInMonths = (today.getFullYear() - birthDate.getFullYear()) * 12 + (today.getMonth() - birthDate.getMonth());\n      console.log(\"Child's age in months:\", ageInMonths);\n      let periods = [];\n      for (let start = 0; start <= ageInMonths; start += 6) {\n        periods.push({\n          startAge: start,\n          endAge: start + 6\n        });\n      }\n      this.developmentPeriods = periods;\n    }\n  }\n};","map":{"version":3,"names":["useAuthStore","axios","data","loading","currentChild","errorMessage","developmentPeriods","created","fetchCurrentChild","methods","authStore","childId","localStorage","getItem","console","log","length","warn","response","get","headers","Authorization","calculateDevelopmentPeriods","error","birthDate","Date","today","ageInMonths","getFullYear","getMonth","periods","start","push","startAge","endAge"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\views\\DashboardPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard-container\">\r\n    <h2>Dobrodo≈°li na Dashboard</h2>\r\n\r\n    <!-- Loading State -->\r\n    <div v-if=\"loading\">Loading...</div>\r\n\r\n    <!-- Error Handling -->\r\n    <div v-else-if=\"errorMessage\" class=\"error-message\">\r\n      {{ errorMessage }}\r\n    </div>\r\n\r\n    <!-- Main Content (Only if Child Exists) -->\r\n    <div v-else-if=\"currentChild\">\r\n      <h3>Opcije za dete: {{ currentChild.name }}</h3>\r\n\r\n      <div class=\"dashboard-links\">\r\n        <div class=\"links-grid\">\r\n          <router-link :to=\"`/milestone-form/${currentChild._id}`\" class=\"dashboard-link\">\r\n            Dodaj Kljucni Momenat\r\n          </router-link>\r\n          <router-link :to=\"`/milestone-list/${currentChild._id}`\" class=\"dashboard-link\">\r\n            Pregled Kljucnog Momenta\r\n          </router-link>\r\n          <router-link :to=\"`/child-development-form/${currentChild._id}`\" class=\"dashboard-link\">\r\n            Unos razvoja deteta\r\n          </router-link>\r\n          <router-link :to=\"`/child-development-list/${currentChild._id}`\" class=\"dashboard-link\">\r\n            Pregled razvoja\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Development Period Links -->\r\n      <div class=\"development-periods\">\r\n        <h4>Periodi razvoja:</h4>\r\n        <div class=\"period-links\">\r\n          <router-link\r\n            v-for=\"(period, index) in developmentPeriods\"\r\n            :key=\"index\"\r\n            :to=\"`/child-development/${currentChild._id}/${period.startAge}`\"\r\n            class=\"period-link\"\r\n          >\r\n            {{ period.startAge }}-{{ period.endAge }} meseci\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Overview Link -->\r\n      <div class=\"overview-link\">\r\n        <router-link :to=\"`/child-overview/${currentChild._id}`\" class=\"dashboard-link\">\r\n          Pregled sve Djece\r\n        </router-link>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- No Child Found -->\r\n    <div v-else class=\"error-message\">\r\n      <p>Nema podataka o detetu. Molimo proverite unos ili registrujte dete.</p>\r\n      <router-link to=\"/child-registration\" class=\"dashboard-link\">\r\n        Registruj Dete\r\n      </router-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useAuthStore } from '@/stores/authStore';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      currentChild: null,\r\n      errorMessage: '',\r\n      developmentPeriods: []\r\n    };\r\n  },\r\n  async created() {\r\n    await this.fetchCurrentChild();\r\n  },\r\n  methods: {\r\n    async fetchCurrentChild() {\r\n      try {\r\n        const authStore = useAuthStore();\r\n        let childId = authStore.childId || localStorage.getItem('childId');\r\n\r\n        console.log(\"üîç Fetching child data for ID:\", childId);\r\n\r\n        if (!childId || childId.length !== 24) {\r\n          console.warn(\"‚ö†Ô∏è Invalid or missing childId. Redirecting to registration.\");\r\n          this.errorMessage = \"Nema registrovanog deteta. Molimo registrujte dete.\";\r\n          return;\r\n        }\r\n\r\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\r\n          headers: {\r\n            Authorization: `Bearer ${localStorage.getItem('token')}`\r\n          }\r\n        });\r\n\r\n        if (response.data) {\r\n          this.currentChild = response.data;\r\n          this.calculateDevelopmentPeriods();\r\n        } else {\r\n          this.errorMessage = \"Podaci o detetu nisu pronaƒëeni.\";\r\n        }\r\n      } catch (error) {\r\n        console.error(\"‚ùå Error fetching child data:\", error);\r\n        this.errorMessage = \"Gre≈°ka prilikom uƒçitavanja podataka. Poku≈°ajte ponovo.\";\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    calculateDevelopmentPeriods() {\r\n      if (!this.currentChild || !this.currentChild.birthDate) return;\r\n\r\n      const birthDate = new Date(this.currentChild.birthDate);\r\n      const today = new Date();\r\n      const ageInMonths = (today.getFullYear() - birthDate.getFullYear()) * 12 + (today.getMonth() - birthDate.getMonth());\r\n\r\n      console.log(\"Child's age in months:\", ageInMonths);\r\n\r\n      let periods = [];\r\n      for (let start = 0; start <= ageInMonths; start += 6) {\r\n        periods.push({ startAge: start, endAge: start + 6 });\r\n      }\r\n\r\n      this.developmentPeriods = periods;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.dashboard-container {\r\n  max-width: 1000px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.error-message {\r\n  background: #f8d7da;\r\n  color: #721c24;\r\n  padding: 15px;\r\n  border: 1px solid #f5c6cb;\r\n  border-radius: 5px;\r\n  text-align: center;\r\n}\r\n\r\n.dashboard-links {\r\n  margin-top: 20px;\r\n}\r\n\r\n.links-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\r\n  gap: 20px;\r\n  margin-top: 20px;\r\n}\r\n\r\n.dashboard-link {\r\n  display: block;\r\n  padding: 15px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  text-decoration: none;\r\n  border-radius: 8px;\r\n  text-align: center;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.dashboard-link:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.development-periods {\r\n  margin-top: 30px;\r\n}\r\n\r\n.period-links {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 15px;\r\n  margin-top: 10px;\r\n}\r\n\r\n.period-link {\r\n  padding: 10px 15px;\r\n  background-color: #28a745;\r\n  color: white;\r\n  text-decoration: none;\r\n  border-radius: 6px;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.period-link:hover {\r\n  background-color: #218838;\r\n}\r\n\r\n.overview-link {\r\n  margin-top: 30px;\r\n}\r\n</style>"],"mappings":";AAmEA,SAASA,YAAW,QAAS,oBAAoB;AACjD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE,EAAE;MAChBC,kBAAkB,EAAE;IACtB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAChC,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,iBAAiBA,CAAA,EAAG;MACxB,IAAI;QACF,MAAME,SAAQ,GAAIV,YAAY,CAAC,CAAC;QAChC,IAAIW,OAAM,GAAID,SAAS,CAACC,OAAM,IAAKC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC;QAElEC,OAAO,CAACC,GAAG,CAAC,gCAAgC,EAAEJ,OAAO,CAAC;QAEtD,IAAI,CAACA,OAAM,IAAKA,OAAO,CAACK,MAAK,KAAM,EAAE,EAAE;UACrCF,OAAO,CAACG,IAAI,CAAC,6DAA6D,CAAC;UAC3E,IAAI,CAACZ,YAAW,GAAI,qDAAqD;UACzE;QACF;QAEA,MAAMa,QAAO,GAAI,MAAMjB,KAAK,CAACkB,GAAG,CAAC,sCAAsCR,OAAO,EAAE,EAAE;UAChFS,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUT,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;UACxD;QACF,CAAC,CAAC;QAEF,IAAIK,QAAQ,CAAChB,IAAI,EAAE;UACjB,IAAI,CAACE,YAAW,GAAIc,QAAQ,CAAChB,IAAI;UACjC,IAAI,CAACoB,2BAA2B,CAAC,CAAC;QACpC,OAAO;UACL,IAAI,CAACjB,YAAW,GAAI,iCAAiC;QACvD;MACF,EAAE,OAAOkB,KAAK,EAAE;QACdT,OAAO,CAACS,KAAK,CAAC,8BAA8B,EAAEA,KAAK,CAAC;QACpD,IAAI,CAAClB,YAAW,GAAI,wDAAwD;MAC9E,UAAU;QACR,IAAI,CAACF,OAAM,GAAI,KAAK;MACtB;IACF,CAAC;IAEDmB,2BAA2BA,CAAA,EAAG;MAC5B,IAAI,CAAC,IAAI,CAAClB,YAAW,IAAK,CAAC,IAAI,CAACA,YAAY,CAACoB,SAAS,EAAE;MAExD,MAA