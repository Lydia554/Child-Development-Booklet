{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  props: ['childId'],\n  // Make sure `childId` is passed as a prop\n  data() {\n    return {\n      newMilestone: {\n        milestoneType: '',\n        milestoneDate: '',\n        progress: '',\n        notes: '',\n        childId: this.childId || localStorage.getItem('childId') // Set the childId here\n      },\n      errorMessage: ''\n    };\n  },\n  created() {\n    if (!this.newMilestone.childId) {\n      alert('Molimo registrujte dete pre nastavka.');\n      this.$router.push('/child-registration');\n    }\n  },\n  methods: {\n    async submitMilestone() {\n      // Validate that all required fields are filled\n      if (!this.newMilestone.milestoneType || !this.newMilestone.milestoneDate || !this.newMilestone.progress) {\n        this.errorMessage = 'Molimo popunite sva obavezna polja.';\n        return;\n      }\n      const token = localStorage.getItem('token');\n      if (!token) {\n        this.errorMessage = 'Morate biti ulogovani da biste poslali podatke.';\n        return;\n      }\n      try {\n        let response;\n        if (this.milestoneId) {\n          // Update existing milestone\n          response = await axios.put(`/api/milestones/${this.milestoneId}`, this.newMilestone, {\n            headers: {\n              Authorization: `Bearer ${token}`\n            }\n          });\n        } else {\n          // Add new milestone\n          response = await axios.post('/api/milestones', this.newMilestone, {\n            headers: {\n              Authorization: `Bearer ${token}`\n            }\n          });\n        }\n        console.log('Milestone saved:', response);\n        this.$router.push(`/milestone-list/${this.newMilestone.childId}`);\n      } catch (error) {\n        console.error('Error saving milestone:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","props","data","newMilestone","milestoneType","milestoneDate","progress","notes","childId","localStorage","getItem","errorMessage","created","alert","$router","push","methods","submitMilestone","token","response","milestoneId","put","headers","Authorization","post","console","log","error"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\MilestoneForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"milestone-form\">\r\n    <h2>{{ milestoneId ? 'Azuriraj Kljucni Momenat' : 'Dodaj Kljucni Momenat' }}</h2>\r\n    <form @submit.prevent=\"submitMilestone\" class=\"form-container\">\r\n      <div class=\"form-group\">\r\n        <label for=\"milestoneType\">Milestone Tip:</label>\r\n        <input\r\n          v-model=\"newMilestone.milestoneType\"\r\n          required\r\n          type=\"text\"\r\n          class=\"form-input\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"milestoneDate\">Milestone Podaci:</label>\r\n        <input\r\n          v-model=\"newMilestone.milestoneDate\"\r\n          required\r\n          type=\"date\"\r\n          class=\"form-input\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"progress\">Napredak:</label>\r\n        <input\r\n          v-model=\"newMilestone.progress\"\r\n          required\r\n          type=\"text\"\r\n          class=\"form-input\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"notes\">Beleske:</label>\r\n        <textarea\r\n          v-model=\"newMilestone.notes\"\r\n          class=\"form-input\"\r\n          rows=\"4\"\r\n        ></textarea>\r\n      </div>\r\n      <button\r\n        type=\"submit\"\r\n        class=\"submit-btn\"\r\n        :class=\"{'add-btn': !milestoneId, 'update-btn': milestoneId}\">\r\n        {{ milestoneId ? 'Azuriraj Kljucni Momenat' : 'Dodaj Kljucni Momenat' }}\r\n      </button>\r\n    </form>\r\n    <div v-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: ['childId'],  // Make sure `childId` is passed as a prop\r\n  data() {\r\n    return {\r\n      newMilestone: {\r\n        milestoneType: '',\r\n        milestoneDate: '',\r\n        progress: '',\r\n        notes: '',\r\n        childId: this.childId || localStorage.getItem('childId'), // Set the childId here\r\n      },\r\n      errorMessage: ''\r\n    };\r\n  },\r\n  created() {\r\n    if (!this.newMilestone.childId) {\r\n      alert('Molimo registrujte dete pre nastavka.');\r\n      this.$router.push('/child-registration');\r\n    }\r\n  },\r\n  methods: {\r\n    async submitMilestone() {\r\n      // Validate that all required fields are filled\r\n      if (!this.newMilestone.milestoneType || !this.newMilestone.milestoneDate || !this.newMilestone.progress) {\r\n        this.errorMessage = 'Molimo popunite sva obavezna polja.';\r\n        return;\r\n      }\r\n\r\n      const token = localStorage.getItem('token');\r\n      if (!token) {\r\n        this.errorMessage = 'Morate biti ulogovani da biste poslali podatke.';\r\n        return;\r\n      }\r\n\r\n      try {\r\n        let response;\r\n        if (this.milestoneId) {\r\n          // Update existing milestone\r\n          response = await axios.put(`/api/milestones/${this.milestoneId}`, this.newMilestone, {\r\n            headers: { Authorization: `Bearer ${token}` }\r\n          });\r\n        } else {\r\n          // Add new milestone\r\n          response = await axios.post('/api/milestones', this.newMilestone, {\r\n            headers: { Authorization: `Bearer ${token}` }\r\n          });\r\n        }\r\n\r\n        console.log('Milestone saved:', response);\r\n        this.$router.push(`/milestone-list/${this.newMilestone.childId}`);\r\n      } catch (error) {\r\n        console.error('Error saving milestone:', error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style lang=\"scss\">\r\n.milestone-form {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f7f7f7;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n\r\n  h2 {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: #333;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .form-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n\r\n    .form-group {\r\n      margin-bottom: 15px;\r\n\r\n      label {\r\n        font-size: 16px;\r\n        font-weight: bold;\r\n        color: #333;\r\n        margin-bottom: 5px;\r\n      }\r\n\r\n      input,\r\n      textarea {\r\n        width: 100%;\r\n        padding: 10px;\r\n        font-size: 14px;\r\n        border: 1px solid #ccc;\r\n        border-radius: 5px;\r\n        box-sizing: border-box;\r\n      }\r\n\r\n      textarea {\r\n        height: 100px;\r\n        resize: vertical;\r\n      }\r\n    }\r\n\r\n    .submit-btn {\r\n      padding: 12px 20px;\r\n      font-size: 16px;\r\n      color: #fff;\r\n      border: none;\r\n      border-radius: 5px;\r\n      cursor: pointer;\r\n      transition: background-color 0.3s ease;\r\n\r\n      &.add-btn {\r\n        background-color: #4CAF50; /* Green color for Add button */\r\n      }\r\n\r\n      &.update-btn {\r\n        background-color: #f1c40f; /* Yellow for Update button */\r\n      }\r\n\r\n      &:hover {\r\n        background-color: #45a049;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AAmDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE,CAAC,SAAS,CAAC;EAAG;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE;QACZC,aAAa,EAAE,EAAE;QACjBC,aAAa,EAAE,EAAE;QACjBC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,IAAI,CAACA,OAAM,IAAKC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC,CAAE;MAC5D,CAAC;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAAC,IAAI,CAACT,YAAY,CAACK,OAAO,EAAE;MAC9BK,KAAK,CAAC,uCAAuC,CAAC;MAC9C,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,qBAAqB,CAAC;IAC1C;EACF,CAAC;EACDC,