{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  props: ['childId'],\n  data() {\n    return {\n      newDevelopment: {\n        childId: this.childId || localStorage.getItem('childId'),\n        // Use the childId from props or localStorage\n        physicalDevelopment: '',\n        emotionalDevelopment: '',\n        intellectualDevelopment: '',\n        socialDevelopment: ''\n      },\n      errorMessage: ''\n    };\n  },\n  created() {\n    // In case childId is passed through the route, you can use it, else it will be taken from localStorage.\n    if (!this.newDevelopment.childId) {\n      console.error('Child ID is missing');\n    }\n  },\n  methods: {\n    async submitDevelopmentForm() {\n      console.log(this.newDevelopment);\n      if (!this.newDevelopment.physicalDevelopment || !this.newDevelopment.emotionalDevelopment || !this.newDevelopment.intellectualDevelopment || !this.newDevelopment.socialDevelopment) {\n        this.errorMessage = 'Molimo vas da popunite sva obavezna polja.';\n        return;\n      }\n      const token = localStorage.getItem('token');\n      if (!token) {\n        this.errorMessage = 'Morate biti ulogovani da biste dodali podatke.';\n        return;\n      }\n      try {\n        const response = await axios.post(`http://localhost:5000/api/child-development/${this.newDevelopment.childId}`, this.newDevelopment, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        console.log('Podaci su uspešno poslati:', response);\n        this.$router.push(`/child-development-list/${this.newDevelopment.childId}`);\n      } catch (error) {\n        console.error('Greška prilikom slanja podataka:', error);\n        this.errorMessage = 'Greška prilikom slanja podataka. Molimo pokušajte ponovo.';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","props","data","newDevelopment","childId","localStorage","getItem","physicalDevelopment","emotionalDevelopment","intellectualDevelopment","socialDevelopment","errorMessage","created","console","error","methods","submitDevelopmentForm","log","token","response","post","headers","Authorization","$router","push"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"child-development-form\">\r\n    <h2 class=\"form-title\">Unos Razvoja Deteta</h2>\r\n    <form @submit.prevent=\"submitDevelopmentForm\" class=\"form-container\">\r\n      <div class=\"form-group\">\r\n        <label for=\"physicalDevelopment\">Fizički razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.physicalDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"emotionalDevelopment\">Emocionalni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.emotionalDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"intellectualDevelopment\">Intelektualni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.intellectualDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"socialDevelopment\">Socijalni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.socialDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <button type=\"submit\" class=\"submit-btn\">Pošaljite</button>\r\n    </form>\r\n\r\n    <div v-if=\"errorMessage\" class=\"error-message\">\r\n      {{ errorMessage }}\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: ['childId'],  \r\n\r\n  data() {\r\n    return {\r\n      newDevelopment: {\r\n        childId: this.childId || localStorage.getItem('childId'),  // Use the childId from props or localStorage\r\n        physicalDevelopment: '',\r\n        emotionalDevelopment: '',\r\n        intellectualDevelopment: '',\r\n        socialDevelopment: ''\r\n      },\r\n      errorMessage: ''\r\n    };\r\n  },\r\n\r\n  created() {\r\n    // In case childId is passed through the route, you can use it, else it will be taken from localStorage.\r\n    if (!this.newDevelopment.childId) {\r\n      console.error('Child ID is missing');\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    async submitDevelopmentForm() {\r\n      console.log(this.newDevelopment); \r\n\r\n     \r\n      if (!this.newDevelopment.physicalDevelopment || \r\n          !this.newDevelopment.emotionalDevelopment || \r\n          !this.newDevelopment.intellectualDevelopment || \r\n          !this.newDevelopment.socialDevelopment) {\r\n        this.errorMessage = 'Molimo vas da popunite sva obavezna polja.';\r\n        return;\r\n      }\r\n\r\n      \r\n      const token = localStorage.getItem('token');\r\n      if (!token) {\r\n        this.errorMessage = 'Morate biti ulogovani da biste dodali podatke.';\r\n        return;\r\n      }\r\n\r\n      try {\r\n      \r\n        const response = await axios.post(`http://localhost:5000/api/child-development/${this.newDevelopment.childId}`, this.newDevelopment, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n        console.log('Podaci su uspešno poslati:', response);\r\n\r\n        \r\n        this.$router.push(`/child-development-list/${this.newDevelopment.childId}`);\r\n      } catch (error) {\r\n        console.error('Greška prilikom slanja podataka:', error);\r\n        this.errorMessage = 'Greška prilikom slanja podataka. Molimo pokušajte ponovo.';\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n\r\n\r\n<style lang=\"scss\">\r\n.child-development-form {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f9f9f9;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n  \r\n  .form-title {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: #333;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .form-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .form-group {\r\n    margin-bottom: 15px;\r\n    \r\n    label {\r\n      font-size: 16px;\r\n      font-weight: bold;\r\n      color: #333;\r\n      margin-bottom: 5px;\r\n    }\r\n\r\n    .form-input {\r\n      width: 100%;\r\n      height: 100px;\r\n      padding: 10px;\r\n      font-size: 14px;\r\n      border: 1px solid #ccc;\r\n      border-radius: 4px;\r\n      resize: none;\r\n      box-sizing: border-box;\r\n    }\r\n  }\r\n\r\n  .submit-btn {\r\n    padding: 12px;\r\n    font-size: 16px;\r\n    color: #fff;\r\n    background-color: #4CAF50;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease;\r\n    \r\n    &:hover {\r\n      background-color: #45a049;\r\n    }\r\n  }\r\n\r\n  .error-message {\r\n    margin-top: 20px;\r\n    padding: 10px;\r\n    background-color: #ffdddd;\r\n    border: 1px solid #f5c6cb;\r\n    color: #721c24;\r\n    border-radius: 4px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AA8BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE,CAAC,SAAS,CAAC;EAElBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE;QACdC,OAAO,EAAE,IAAI,CAACA,OAAM,IAAKC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC;QAAG;QAC3DC,mBAAmB,EAAE,EAAE;QACvBC,oBAAoB,EAAE,EAAE;QACxBC,uBAAuB,EAAE,EAAE;QAC3BC,iBAAiB,EAAE;MACrB,CAAC;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAAC,IAAI,CAACT,cAAc,CAACC,OAAO,EAAE;MAChCS,OAAO,CAACC,KAAK,CAAC,qBAAqB,CAAC;IACtC;EACF,CAAC;EAEDC,OAAO,EAAE;IACP,MAAMC,qBAAqBA,CAAA,EAAG;MAC5BH,OAAO,CAACI,GAAG,CAAC,IAAI,CAACd,cAAc,CAAC;MAGhC,IAAI,CAAC,IAAI,CAACA,cAAc,CAACI,mBAAkB,IACvC,CAAC,IAAI,CAACJ,cAAc,CAACK,oBAAmB,IACxC,CAAC,IAAI,CAACL,cAAc,CAACM,uBAAsB,IAC3C,CAAC,IAAI,CAACN,cAAc,CAACO,iBAAiB,EAAE;QAC1C,IAAI,CAACC,YAAW,GAAI,4CAA4C;QAChE;MACF;MAGA,MAAMO,KAAI,GAAIb,YAAY,C