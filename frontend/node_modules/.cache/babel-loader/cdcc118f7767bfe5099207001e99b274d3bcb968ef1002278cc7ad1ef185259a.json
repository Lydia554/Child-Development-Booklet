{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useAuthStore } from './stores/authStore';\nimport { useRouter } from 'vue-router';\nexport default {\n  setup() {\n    const authStore = useAuthStore();\n    const router = useRouter();\n\n    // Proverava status prijave prilikom učitavanja\n    authStore.checkAuth();\n\n    // Logout funkcija koja briše token i preusmerava na login stranicu\n    const logout = () => {\n      authStore.logout();\n      router.push('/login');\n    };\n    return {\n      authStore,\n      logout\n    };\n  }\n};","map":{"version":3,"names":["useAuthStore","useRouter","setup","authStore","router","checkAuth","logout","push"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\App.vue"],"sourcesContent":["<template>\n  <div>\n    <nav>\n      <!-- Linkovi dostupni nakon što je korisnik prijavljen -->\n      <router-link v-if=\"authStore.isAuthenticated\" to=\"/milestone-form\">Dodaj Milestone</router-link> \n      <router-link v-if=\"authStore.isAuthenticated\" to=\"/milestone-list\">Pregled Milestone-a</router-link> \n      \n      <!-- Linkovi vezani za Child Development -->\n      <router-link v-if=\"authStore.isAuthenticated\" to=\"/child-development-list\">Pregled razvoja</router-link>\n      <router-link v-if=\"authStore.isAuthenticated\" to=\"/child-development-page\">Periodi</router-link> \n\n      <!-- Linkovi za Login i Signup ako korisnik nije prijavljen -->\n      <router-link v-if=\"!authStore.isAuthenticated\" to=\"/login\">Prijavite se</router-link> \n      <router-link v-if=\"!authStore.isAuthenticated\" to=\"/\">Registrujte se</router-link>\n\n      <!-- Dugme za odjavu -->\n      <button v-if=\"authStore.isAuthenticated\" @click=\"logout\" class=\"logout-btn\">Odjavi se</button>\n    </nav>\n\n    <!-- Prikazivanje odgovarajuće komponente na osnovu rute -->\n    <router-view />\n  </div>\n</template>\n\n<script>\nimport { useAuthStore } from './stores/authStore';  \nimport { useRouter } from 'vue-router';  \n\nexport default {\n  setup() {\n    const authStore = useAuthStore();\n    const router = useRouter();  \n\n    // Proverava status prijave prilikom učitavanja\n    authStore.checkAuth();  \n\n    // Logout funkcija koja briše token i preusmerava na login stranicu\n    const logout = () => {\n      authStore.logout();  \n      router.push('/login');  \n    };\n\n    return { authStore, logout };\n  }\n};\n</script>\n\n<style scoped>\n/* Stilizacija za navigaciju */\nnav {\n  margin-bottom: 20px;\n}\n\nnav a {\n  margin-right: 10px;\n  text-decoration: none;\n  color: #007bff;\n}\n\nnav a:hover {\n  text-decoration: underline;\n}\n\n.logout-btn {\n  padding: 8px 16px;\n  background-color: #690aa8;  \n  color: white;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  font-size: 16px;\n  transition: background-color 0.3s, transform 0.3s;\n  margin-left: 20px;\n}\n\n.logout-btn:hover {\n  background-color: #250891;  \n  transform: scale(1.05);  \n}\n\n.logout-btn:focus {\n  outline: none;\n}\n</style>\n"],"mappings":";AAyBA,SAASA,YAAW,QAAS,oBAAoB;AACjD,SAASC,SAAQ,QAAS,YAAY;AAEtC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIH,YAAY,CAAC,CAAC;IAChC,MAAMI,MAAK,GAAIH,SAAS,CAAC,CAAC;;IAE1B;IACAE,SAAS,CAACE,SAAS,CAAC,CAAC;;IAErB;IACA,MAAMC,MAAK,GAAIA,CAAA,KAAM;MACnBH,SAAS,CAACG,MAAM,CAAC,CAAC;MAClBF,MAAM,CAACG,IAAI,CAAC,QAAQ,CAAC;IACvB,CAAC;IAED,OAAO;MAAEJ,SAAS;MAAEG;IAAO,CAAC;EAC9B;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}