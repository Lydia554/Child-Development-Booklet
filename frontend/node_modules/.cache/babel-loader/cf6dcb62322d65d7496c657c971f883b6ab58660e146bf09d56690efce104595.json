 <h4>Razvoj deteta</h4>\r\n        <ul>\r\n          <li><strong>Fiziƒçki:</strong> {{ currentPeriodDevelopment.physicalDevelopment || \"Nema podataka\" }}</li>\r\n          <li><strong>Emocionalni:</strong> {{ currentPeriodDevelopment.emotionalDevelopment || \"Nema podataka\" }}</li>\r\n          <li><strong>Intelektualni:</strong> {{ currentPeriodDevelopment.intellectualDevelopment || \"Nema podataka\" }}</li>\r\n          <li><strong>Socijalni:</strong> {{ currentPeriodDevelopment.socialDevelopment || \"Nema podataka\" }}</li>\r\n        </ul>\r\n      </div>\r\n\r\n      <!-- Milestones Data -->\r\n      <div v-if=\"currentPeriodMilestones.length\" class=\"milestones-section\">\r\n        <h4>Prekretnice za ovaj period</h4>\r\n        <div class=\"milestone-list\">\r\n          <div v-for=\"milestone in currentPeriodMilestones\" :key=\"milestone._id\" class=\"milestone-item\">\r\n            <h3>{{ milestone.milestoneType }} - {{ formatDate(milestone.milestoneDate) }}</h3>\r\n            <p><strong>Napredak:</strong> {{ milestone.progress }}</p>\r\n            <p><strong>Napomene:</strong> {{ milestone.notes }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div v-else class=\"no-records\">\r\n        <p>Nema unetih podataka za ovaj period.</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  props: {\r\n    selectedPeriod: Object // ‚úÖ Receives selected period from parent\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      childName: null,\r\n      childAge: null,\r\n      developmentData: [],\r\n      milestoneData: [],\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    currentPeriodDevelopment() {\r\n      return this.selectedPeriod\r\n        ? this.developmentData.find(\r\n            (d) =>\r\n              d.startAge === this.selectedPeriod.start &&\r\n              d.endAge === this.selectedPeriod.end\r\n          )\r\n        : null;\r\n    },\r\n\r\n    currentPeriodMilestones() {\r\n      return this.selectedPeriod\r\n        ? this.milestoneData.filter(\r\n            (m) =>\r\n              m.age >= this.selectedPeriod.start &&\r\n              m.age < this.selectedPeriod.end\r\n          )\r\n        : [];\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    async fetchChildData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        if (!token) {\r\n          console.error(\"üö® No token found in sessionStorage!\");\r\n          return;\r\n        }\r\n\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/children/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n\r\n        if (response.data && response.data.birthDate) {\r\n          this.childName = response.data.name;\r\n          this.childAge = this.calculateAgeInMonths(response.data.birthDate);\r\n          console.log(\"‚úÖ Child data fetched successfully:\", response.data);\r\n        } else {\r\n          console.warn(\"‚ö†Ô∏è Missing birthDate in response data.\");\r\n        }\r\n      } catch (error) {\r\n        console.error(\"‚ùå Gre≈°ka pri dobijanju podataka o detetu:\", error);\r\n      }\r\n    },\r\n\r\n    async fetchDevelopmentData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        this.developmentData = response.data || [];\r\n      } catch (error) {\r\n        console.error(\"‚ùå Gre≈°ka pri dobijanju podataka o razvoju:\", error);\r\n      }\r\n    },\r\n\r\n    async fetchMilestones(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/milestones/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        this.milestoneData = response.data || [];\r\n      } catch (error) {\r\n        console.error(\"‚ùå Gre≈°ka pri dobijanju prekretnica:\", error);\r\n      }\r\n    },\r\n\r\n    calculateAgeInMonths(birthDate) {\r\n      if (!birthDate) return 0;\r\n      const birth = new Date(birthDate);\r\n      const now = new Date();\r\n      return (now.getFullYear() - birth.getFullYear()) * 12 + (now.getMonth() - birth.getMonth());\r\n    },\r\n\r\n    formatPeriod(start, end) {\r\n      return `${start}-${end} meseci`;\r\n    },\r\n\r\n    formatDate(date) {\r\n      return new Date(date).toLocaleDateString(\"sr-RS\");\r\n    }\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      const childId = this.$route.params.childId;\r\n      if (childId) {\r\n        await this.fetchChildData(childId);\r\n        await this.fetchDevelopmentData(childId);\r\n        await this.fetchMilestones(childId);\r\n      }\r\n    } catch (error) {\r\n      console.error(\"‚ùå Gre≈°ka u created():\", error);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.development-container {\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.child-info {\r\n  background: #f8f9fa;\r\n  padding: 15px;\r\n  border-radius: 10px;\r\n  margin-bottom: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.error-message {\r\n  color: red;\r\n  background-color: #fee;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n}\r\n\r\n.available-periods h3 {\r\n  margin-bottom: 10px;\r\n  color: #1976d2;\r\n}\r\n\r\n/* Centered Periods */\r\n.periods-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(140px, 1fr));\r\n  gap: 10px;\r\n  justify-content: center;\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.period-item {\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  text-align: center;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  transition: 0.3s;\r\n  cursor: pointer;\r\n}\r\n\r\n.period-item.current {\r\n  background: #4caf50;\r\n  color: white;\r\n}\r\n\r\n.period-item.past {\r\n  background: #ffcc80;\r\n}\r\n\r\n.period-item.future {\r\n  background: #eeeeee;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.period-status {\r\n  font-size: 12px;\r\n  display: block;\r\n}\r\n\r\n.action-buttons {\r\n  margin-top: 15px;\r\n}\r\n\r\n.button {\r\n  display: inline-block;\r\n  padding: 8px 12px;\r\n  background: #1976d2;\r\n  color: white;\r\n  border-radius: 5px;\r\n  text-decoration: none;\r\n  margin: 5px;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAuB;;EAI3BA,KAAK,EAAC;AAAY;;EAL3BC,GAAA;EAW+BD,KAAK,EAAC;;;EAXrCC,GAAA;EAe2CD,KAAK,EAAC;;;EAfjDC,GAAA;EA0BiDD,KAAK,EAAC;;;EAE1CA,KAAK,EAAC;AAAgB;;EA5BnCC,GAAA;EAqCkBD,KAAK,EAAC;;;uBApCtBE,mBAAA,CAwCM,OAxCNC,UAwCM,G,4BAvCJC,mBAAA,CAA+B,YAA3B,wBAAsB,sBAE1BC,mBAAA,gBAAmB,EACnBD,mBAAA,CAGM,OAHNE,UAGM,GAFJF,mBAAA,CAAuD,YAAAG,gBAAA,CAAhDC,KAAA,CAAAC,SAAS,GAAGD,KAAA,CAAAC,SAAS,qCAC5BL,mBAAA,CAA4F,Y,0BAAzFA,mBAAA,CAAyB,gBAAjB,UAAQ,sBAPzBM,gBAAA,CAOkC,GAAC,GAAAH,gBAAA,CAAGC,KAAA,CAAAG,QAAQ,GAAGH,KAAA,CAAAG,QAAQ,mD,KAGrDN,mBAAA,kCAAqC,EAC1BO,MAAA,CAAAC,cAAc,I,cAAzBX,mBAAA,CA6BM,OA7BNY,UA6BM,GA5BJV,mBAAA,CAAuF,YAAnF,oBAAkB,GAAAG,gBAAA,CAAGQ,QAAA,CAAAC,YAAY,CAACJ,MAAA,CAAAC,cAAc,CAACI,KAAK,EAAEL,MAAA,CAAAC,cAAc,CAACK,GAAG,mBAE9Eb,mBAAA,sBAAyB,EACdU,QAAA,CAAAI,wBAAwB,I,cAAnCjB,mBAAA,CAQM,OARNkB,UAQM,G,0BAPJhB,mBAAA,CAAsB,YAAlB,eAAa,sBACjBA,mBAAA,CAKK,aAJHA,mBAAA,CAAwG,a,0BAApGA,mBAAA,CAAyB,gBAAjB,UAAQ,sBAlB9BM,gBAAA,CAkBuC,GAAC,GAAAH,gBAAA,CAAGQ,QAAA,CAAAI,wBAAwB,CAACE,mBAAmB,oC,GAC7EjB,mBAAA,CAA6G,a,0BAAzGA,mBAAA,CAA6B,gBAArB,cAAY,sBAnBlCM,gBAAA,CAmB2C,GAAC,GAAAH,gBAAA,CAAGQ,QAAA,CAAAI,wBAAwB,CAACG,oBAAoB,oC,GAClFlB,mBAAA,CAAkH,a,0BAA9GA,mBAAA,CAA+B,gBAAvB,gBAAc,sBApBpCM,gBAAA,CAoB6C,GAAC,GAAAH,gBAAA,CAAGQ,QAAA,CAAAI,wBAAwB,CAACI,uBAAuB,oC,GACvFnB,mBAAA,CAAwG,a,0BAApGA,mBAAA,CAA2B,gBAAnB,YAAU,sBArBhCM,gBAAA,CAqByC,GAAC,GAAAH,gBAAA,CAAGQ,QAAA,CAAAI,wBAAwB,CAACK,iBAAiB,oC,SArBvFnB,mBAAA,gBAyBMA,mBAAA,qBAAwB,EACbU,QAAA,CAA