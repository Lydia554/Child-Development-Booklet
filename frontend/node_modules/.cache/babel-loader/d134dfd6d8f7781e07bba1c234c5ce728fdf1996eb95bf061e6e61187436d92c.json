{"ast":null,"code":"import { useAuthStore } from '@/stores/authStore';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      loading: true,\n      currentChild: null,\n      errorMessage: ''\n    };\n  },\n  async created() {\n    await this.fetchCurrentChild();\n  },\n  methods: {\n    async fetchCurrentChild() {\n      try {\n        const authStore = useAuthStore();\n        let childId = authStore.childId || localStorage.getItem('childId');\n        console.log(\"üîç Fetching child data for ID:\", childId);\n        if (!childId || childId.length !== 24) {\n          console.warn(\"‚ö†Ô∏è Invalid or missing childId. Redirecting to registration.\");\n          this.errorMessage = \"Nema registrovanog deteta. Molimo registrujte dete.\";\n          return;\n        }\n        const response = await axios.get(`http://localhost:5000/api/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        if (response.data && response.data._id) {\n          this.currentChild = response.data;\n        } else {\n          this.errorMessage = \"Podaci o detetu nisu pronaƒëeni.\";\n        }\n      } catch (error) {\n        console.error(\"‚ùå Error fetching child data:\", error);\n        this.errorMessage = \"Gre≈°ka prilikom uƒçitavanja podataka. Poku≈°ajte ponovo.\";\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["useAuthStore","axios","data","loading","currentChild","errorMessage","created","fetchCurrentChild","methods","authStore","childId","localStorage","getItem","console","log","length","warn","response","get","headers","Authorization","_id","error"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\views\\DashboardPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard-container\">\r\n    <h2>Dobrodo≈°li na Dashboard</h2>\r\n\r\n    \r\n    <div v-if=\"loading\">Loading...</div>\r\n\r\n\r\n    <div v-else-if=\"errorMessage\" class=\"error-message\">\r\n      {{ errorMessage }}\r\n    </div>\r\n\r\n    <!-- Main Content (Only if Child Exists) -->\r\n    <div v-else-if=\"currentChild && currentChild._id\">\r\n      <h3>Opcije za dete: {{ currentChild.name }}</h3>\r\n\r\n      <div class=\"dashboard-links\">\r\n        <div class=\"links-grid\">\r\n          <router-link :to=\"`/milestone-form/${currentChild._id}`\" class=\"dashboard-link\">\r\n            Dodaj Kljucni Momenat\r\n          </router-link>\r\n          <router-link :to=\"`/milestone-list/${currentChild._id}`\" class=\"dashboard-link\">\r\n            Pregled Kljucnog Momenta\r\n          </router-link>\r\n          <router-link :to=\"`/child-development-form/${currentChild._id}`\" class=\"dashboard-link\">\r\n            Unos razvoja deteta\r\n          </router-link>\r\n          <router-link :to=\"`/child-development-list/${currentChild._id}`\" class=\"dashboard-link\">\r\n            Pregled razvoja\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Overview Link -->\r\n      <div class=\"overview-link\">\r\n        <router-link :to=\"`/child-overview/${currentChild._id}`\" class=\"dashboard-link\">\r\n          Pregled sve Djece\r\n        </router-link>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- No Child Found -->\r\n    <div v-else class=\"error-message\">\r\n      <p>Nema podataka o detetu. Molimo proverite unos ili registrujte dete.</p>\r\n      <router-link to=\"/child-registration\" class=\"dashboard-link\">\r\n        Registruj Dete\r\n      </router-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useAuthStore } from '@/stores/authStore';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      currentChild: null,\r\n      errorMessage: '',\r\n    };\r\n  },\r\n  async created() {\r\n    await this.fetchCurrentChild();\r\n  },\r\n  methods: {\r\n    async fetchCurrentChild() {\r\n      try {\r\n        const authStore = useAuthStore();\r\n        let childId = authStore.childId || localStorage.getItem('childId');\r\n\r\n        console.log(\"üîç Fetching child data for ID:\", childId);\r\n\r\n        if (!childId || childId.length !== 24) {\r\n          console.warn(\"‚ö†Ô∏è Invalid or missing childId. Redirecting to registration.\");\r\n          this.errorMessage = \"Nema registrovanog deteta. Molimo registrujte dete.\";\r\n          return;\r\n        }\r\n\r\n        const response = await axios.get(`http://localhost:5000/api/${childId}`, {\r\n          headers: {\r\n            Authorization: `Bearer ${localStorage.getItem('token')}`\r\n          }\r\n        });\r\n\r\n        if (response.data && response.data._id) {\r\n          this.currentChild = response.data;\r\n        } else {\r\n          this.errorMessage = \"Podaci o detetu nisu pronaƒëeni.\";\r\n        }\r\n      } catch (error) {\r\n        console.error(\"‚ùå Error fetching child data:\", error);\r\n        this.errorMessage = \"Gre≈°ka prilikom uƒçitavanja podataka. Poku≈°ajte ponovo.\";\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.dashboard-container {\r\n  max-width: 1000px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.error-message {\r\n  background: #f8d7da;\r\n  color: #721c24;\r\n  padding: 15px;\r\n  border: 1px solid #f5c6cb;\r\n  border-radius: 5px;\r\n  text-align: center;\r\n}\r\n\r\n.dashboard-links {\r\n  margin-top: 20px;\r\n}\r\n\r\n.links-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\r\n  gap: 20px;\r\n  margin-top: 20px;\r\n}\r\n\r\n.dashboard-link {\r\n  display: block;\r\n  padding: 15px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  text-decoration: none;\r\n  border-radius: 8px;\r\n  text-align: center;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.dashboard-link:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.development-periods {\r\n  margin-top: 30px;\r\n}\r\n\r\n.period-links {\r\n  display: flex;\r\n  gap: 15px;\r\n  margin-top: 10px;\r\n}\r\n\r\n.period-link {\r\n  padding: 10px 15px;\r\n  background-color: #28a745;\r\n  color: white;\r\n  text-decoration: none;\r\n  border-radius: 6px;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.period-link:hover {\r\n  background-color: #218838;\r\n}\r\n\r\n.overview-link {\r\n  margin-top: 30px;\r\n}\r\n</style>\r\n\r\n"],"mappings":"AAoDA,SAASA,YAAW,QAAS,oBAAoB;AACjD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAChC,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,iBAAiBA,CAAA,EAAG;MACxB,IAAI;QACF,MAAME,SAAQ,GAAIT,YAAY,CAAC,CAAC;QAChC,IAAIU,OAAM,GAAID,SAAS,CAACC,OAAM,IAAKC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC;QAElEC,OAAO,CAACC,GAAG,CAAC,gCAAgC,EAAEJ,OAAO,CAAC;QAEtD,IAAI,CAACA,OAAM,IAAKA,OAAO,CAACK,MAAK,KAAM,EAAE,EAAE;UACrCF,OAAO,CAACG,IAAI,CAAC,6DAA6D,CAAC;UAC3E,IAAI,CAACX,YAAW,GAAI,qDAAqD;UACzE;QACF;QAEA,MAAMY,QAAO,GAAI,MAAMhB,KAAK,CAACiB,GAAG,CAAC,6BAA6BR,OAAO,EAAE,EAAE;UACvES,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUT,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;UACxD;QACF,CAAC,CAAC;QAEF,IAAIK,QAAQ,CAACf,IAAG,IAAKe,QAAQ,CAACf,IAAI,CAACmB,GAAG,EAAE;UACtC,IAAI,CAACjB,YAAW,GAAIa,QAAQ,CAACf,IAAI;QACnC,OAAO;UACL,IAAI,CAACG,YAAW,GAAI,iCAAiC;QACvD;MACF,EAAE,OAAOiB,KAAK,EAAE;QACdT,OAAO,CAACS,KAAK,CAAC,8BAA8B,EAAEA,KAAK,CAAC;QACpD,IAAI,CAACjB,YAAW,GAAI,wDAAwD;MAC9E,UAAU;QACR,IAAI,CAACF,OAAM,GAAI,KAAK;MACtB;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}