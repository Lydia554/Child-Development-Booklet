{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      periodRecords: [],\n      childName: \"\",\n      selectedPeriod: null,\n      availablePeriods: [],\n      milestoneData: []\n    };\n  },\n  computed: {\n    sortedRecords() {\n      return [...this.periodRecords].sort((a, b) => a.startAge - b.startAge);\n    }\n  },\n  created() {\n    const childId = this.$route.params.childId;\n    if (childId) {\n      this.generateAvailablePeriods();\n      this.fetchDevelopmentData(childId);\n      this.fetchChildName(childId);\n    } else {\n      console.error('No child ID found');\n      this.$router.push('/child-registration');\n    }\n  },\n  methods: {\n    generateAvailablePeriods() {\n      // Generate periods from 0 to 6 years (72 months) in 6-month intervals\n      const periods = [];\n      for (let i = 0; i <= 72; i += 6) {\n        periods.push({\n          start: i,\n          end: i + 6\n        });\n      }\n      this.availablePeriods = periods;\n      this.selectedPeriod = periods[0]; // Select first period by default\n    },\n    formatPeriod(start, end) {\n      if (start === 0) {\n        return `0-6 meseci`;\n      }\n      return `${start}-${end} meseci`;\n    },\n    async fetchDevelopmentData(childId) {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.periodRecords = response.data.periods || [];\n      } catch (error) {\n        console.error('Error fetching development data:', error);\n      }\n    },\n    async fetchChildName(childId) {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/children/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.childName = response.data.name;\n      } catch (error) {\n        console.error('Error fetching child name:', error);\n      }\n    },\n    async handlePeriodClick(period) {\n      this.selectedPeriod = period;\n      await this.fetchMilestonesForPeriod(period);\n    },\n    async fetchMilestonesForPeriod(period) {\n      try {\n        const token = sessionStorage.getItem('token');\n        const response = await axios.get(`http://localhost:5000/api/milestones/${this.$route.params.childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.milestoneData = response.data.filter(milestone => new Date(milestone.milestoneDate) >= this.calculateStartDate(period.start) && new Date(milestone.milestoneDate) < this.calculateEndDate(period.end));\n      } catch (error) {\n        console.error('Error fetching milestones:', error);\n      }\n    },\n    calculateStartDate(months) {\n      const birthDate = new Date(this.childBirthDate);\n      birthDate.setMonth(birthDate.getMonth() + months);\n      return birthDate;\n    },\n    calculateEndDate(months) {\n      const birthDate = new Date(this.childBirthDate);\n      birthDate.setMonth(birthDate.getMonth() + months);\n      return birthDate;\n    },\n    isPeriodFilled(period) {\n      return this.periodRecords.some(record => record.startAge === period.start && record.endAge === period.end);\n    },\n    formatDate(date) {\n      const options = {\n        year: 'numeric',\n        month: 'long',\n        day: 'numeric'\n      };\n      return new Date(date).toLocaleDateString(undefined, options);\n    },\n    editMilestone(milestoneId) {\n      this.$router.push(`/milestone-form/${this.$route.params.childId}/${milestoneId}`);\n    },\n    async deleteMilestone(milestoneId) {\n      try {\n        const token = sessionStorage.getItem('token');\n        await axios.delete(`http://localhost:5000/api/milestones/${milestoneId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.fetchMilestonesForPeriod(this.selectedPeriod);\n      } catch (error) {\n        console.error('Error deleting milestone:', error);\n      }\n    },\n    async confirmDelete(milestoneId) {\n      if (window.confirm('Da li ste sigurni da želite da obrišete ovaj milestone?')) {\n        await this.deleteMilestone(milestoneId);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","periodRecords","childName","selectedPeriod","availablePeriods","milestoneData","computed","sortedRecords","sort","a","b","startAge","created","childId","$route","params","generateAvailablePeriods","fetchDevelopmentData","fetchChildName","console","error","$router","push","methods","periods","i","start","end","formatPeriod","token","sessionStorage","getItem","response","get","headers","Authorization","name","handlePeriodClick","period","fetchMilestonesForPeriod","filter","milestone","Date","milestoneDate","calculateStartDate","calculateEndDate","months","birthDate","childBirthDate","setMonth","getMonth","isPeriodFilled","some","record","endAge","formatDate","date","options","year","month","day","toLocaleDateString","undefined","editMilestone","milestoneId","deleteMilestone","delete","confirmDelete","window","confirm"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentPeriod.vue"],"sourcesContent":["<template>\r\n  <div class=\"development-tracker\">\r\n    <h2>Praćenje Razvoja za {{ childName }}</h2>\r\n    \r\n    <!-- Period selector -->\r\n    <div class=\"period-selector\">\r\n      <h3>Izaberite period:</h3>\r\n      <div class=\"periods-list\">\r\n        <div v-for=\"period in availablePeriods\" \r\n             :key=\"`${period.start}-${period.end}`\"\r\n             class=\"period-item\"\r\n             @click=\"handlePeriodClick(period)\">\r\n          {{ formatPeriod(period.start, period.end) }}\r\n          <span v-if=\"isPeriodFilled(period)\" class=\"status\">(Ispunjeno)</span>\r\n          <span v-else class=\"status empty\">(Nije ispunjeno)</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Display records -->\r\n    <div v-if=\"periodRecords.length\" class=\"period-records\">\r\n      <div v-for=\"record in sortedRecords\" :key=\"record._id\" class=\"period-section\">\r\n        <h3>{{ formatPeriod(record.startAge, record.endAge) }}</h3>\r\n        <ul>\r\n          <li><strong>Fizički:</strong> {{ record.physicalDevelopment }}</li>\r\n          <li><strong>Emocionalni:</strong> {{ record.emotionalDevelopment }}</li>\r\n          <li><strong>Intelektualni:</strong> {{ record.intellectualDevelopment }}</li>\r\n          <li><strong>Socijalni:</strong> {{ record.socialDevelopment }}</li>\r\n        </ul>\r\n        <div class=\"milestones-section\">\r\n          <h4>Prekretnice za ovaj period</h4>\r\n          <div class=\"milestone-list\">\r\n            <div v-for=\"milestone in milestoneData\" :key=\"milestone._id\" class=\"milestone-item\">\r\n              <h3>{{ milestone.milestoneType }} - {{ formatDate(milestone.milestoneDate) }}</h3>\r\n              <p><strong>Napredak:</strong> {{ milestone.progress }}</p>\r\n              <p><strong>Napomene:</strong> {{ milestone.notes }}</p>\r\n              <div class=\"button-group\">\r\n                <button @click=\"editMilestone(milestone._id)\" class=\"edit-btn\">Izmeni</button>\r\n                <button @click=\"confirmDelete(milestone._id)\" class=\"delete-btn\">Obriši</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div v-else class=\"no-records\">\r\n      <p>Nema unetih podataka za ovaj period.</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return { \r\n      periodRecords: [],\r\n      childName: \"\",\r\n      selectedPeriod: null,\r\n      availablePeriods: [],\r\n      milestoneData: []\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    sortedRecords() {\r\n      return [...this.periodRecords].sort((a, b) => a.startAge - b.startAge);\r\n    }\r\n  },\r\n\r\n  created() {\r\n    const childId = this.$route.params.childId;\r\n    \r\n    if (childId) {\r\n      this.generateAvailablePeriods();\r\n      this.fetchDevelopmentData(childId);\r\n      this.fetchChildName(childId);\r\n    } else {\r\n      console.error('No child ID found');\r\n      this.$router.push('/child-registration');\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    generateAvailablePeriods() {\r\n      // Generate periods from 0 to 6 years (72 months) in 6-month intervals\r\n      const periods = [];\r\n      for (let i = 0; i <= 72; i += 6) {\r\n        periods.push({\r\n          start: i,\r\n          end: i + 6\r\n        });\r\n      }\r\n      this.availablePeriods = periods;\r\n      this.selectedPeriod = periods[0]; // Select first period by default\r\n    },\r\n\r\n    formatPeriod(start, end) {\r\n      if (start === 0) {\r\n        return `0-6 meseci`;\r\n      }\r\n      return `${start}-${end} meseci`;\r\n    },\r\n\r\n    async fetchDevelopmentData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` }\r\n          }\r\n        );\r\n        this.periodRecords = response.data.periods || [];\r\n      } catch (error) {\r\n        console.error('Error fetching development data:', error);\r\n      }\r\n    },\r\n\r\n    async fetchChildName(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/children/${childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` }\r\n          }\r\n        );\r\n        this.childName = response.data.name;\r\n      } catch (error) {\r\n        console.error('Error fetching child name:', error);\r\n      }\r\n    },\r\n\r\n    async handlePeriodClick(period) {\r\n      this.selectedPeriod = period;\r\n      await this.fetchMilestonesForPeriod(period);\r\n    },\r\n\r\n    async fetchMilestonesForPeriod(period) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/milestones/${this.$route.params.childId}`,\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` }\r\n          }\r\n        );\r\n\r\n        this.milestoneData = response.data.filter(milestone => \r\n          new Date(milestone.milestoneDate) >= this.calculateStartDate(period.start) &&\r\n          new Date(milestone.milestoneDate) < this.calculateEndDate(period.end)\r\n        );\r\n      } catch (error) {\r\n        console.error('Error fetching milestones:', error);\r\n      }\r\n    },\r\n\r\n    calculateStartDate(months) {\r\n      const birthDate = new Date(this.childBirthDate);\r\n      birthDate.setMonth(birthDate.getMonth() + months);\r\n      return birthDate;\r\n    },\r\n\r\n    calculateEndDate(months) {\r\n      const birthDate = new Date(this.childBirthDate);\r\n      birthDate.setMonth(birthDate.getMonth() + months);\r\n      return birthDate;\r\n    },\r\n\r\n    isPeriodFilled(period) {\r\n      return this.periodRecords.some(record => \r\n        record.startAge === period.start && \r\n        record.endAge === period.end\r\n      );\r\n    },\r\n\r\n    formatDate(date) {\r\n      const options = { year: 'numeric', month: 'long', day: 'numeric' };\r\n      return new Date(date).toLocaleDateString(undefined, options);\r\n    },\r\n\r\n    editMilestone(milestoneId) {\r\n      this.$router.push(`/milestone-form/${this.$route.params.childId}/${milestoneId}`);\r\n    },\r\n\r\n    async deleteMilestone(milestoneId) {\r\n      try {\r\n        const token = sessionStorage.getItem('token');\r\n        await axios.delete(`http://localhost:5000/api/milestones/${milestoneId}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n        this.fetchMilestonesForPeriod(this.selectedPeriod);\r\n      } catch (error) {\r\n        console.error('Error deleting milestone:', error);\r\n      }\r\n    },\r\n\r\n    async confirmDelete(milestoneId) {\r\n      if (window.confirm('Da li ste sigurni da želite da obrišete ovaj milestone?')) {\r\n        await this.deleteMilestone(milestoneId);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.development-tracker {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n\r\n  .period-selector {\r\n    margin: 20px 0;\r\n    \r\n    .periods-list {\r\n      display: grid;\r\n      grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));\r\n      gap: 15px;\r\n      margin-top: 20px;\r\n\r\n      .period-item {\r\n        padding: 15px;\r\n        background-color: #f5f5f5;\r\n        border-radius: 8px;\r\n        cursor: pointer;\r\n        transition: all 0.3s ease;\r\n\r\n        &:hover {\r\n          background-color: #e3f2fd;\r\n          transform: translateY(-2px);\r\n        }\r\n\r\n        .status {\r\n          display: block;\r\n          font-size: 0.9em;\r\n          margin-top: 5px;\r\n          \r\n          &.empty {\r\n            color: #f57c00;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  .period-section {\r\n    background-color: #fff;\r\n    padding: 20px;\r\n    margin: 15px 0;\r\n    border-radius: 8px;\r\n    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\r\n\r\n    ul {\r\n      list-style: none;\r\n      padding: 0;\r\n\r\n      li {\r\n        margin: 10px 0;\r\n        \r\n        strong {\r\n          color: #333;\r\n          margin-right: 10px;\r\n        }\r\n      }\r\n    }\r\n\r\n    .milestones-section {\r\n      margin-top: 20px;\r\n\r\n      .milestone-list {\r\n        display: grid;\r\n        grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));\r\n        gap: 15px;\r\n\r\n        .milestone-item {\r\n          background-color: #f5f5f5;\r\n          padding: 15px;\r\n          border-radius: 8px;\r\n          box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\r\n\r\n          h3 {\r\n            margin: 0 0 10px;\r\n          }\r\n\r\n          p {\r\n            margin: 5px 0;\r\n          }\r\n\r\n          .button-group {\r\n            display: flex;\r\n            justify-content: space-between;\r\n            margin-top: 10px;\r\n\r\n            .edit-btn, .delete-btn {\r\n              padding: 5px 10px;\r\n              border: none;\r\n              border-radius: 4px;\r\n              cursor: pointer;\r\n              transition: background-color 0.3s ease;\r\n\r\n              &.edit-btn {\r\n                background-color: #4caf50;\r\n                color: #fff;\r\n\r\n                &:hover {\r\n                  background-color: #45a049;\r\n                }\r\n              }\r\n\r\n              &.delete-btn {\r\n                background-color: #f44336;\r\n                color: #fff;\r\n\r\n                &:hover {\r\n                  background-color: #e53935;\r\n                }\r\n              }\r\n            }\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  .no-records {\r\n    text-align: center;\r\n    color: #666;\r\n    margin: 20px 0;\r\n  }\r\n}\r\n</style>"],"mappings":";;;;AAoDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,EAAE;MACjBC,SAAS,EAAE,EAAE;MACbC,cAAc,EAAE,IAAI;MACpBC,gBAAgB,EAAE,EAAE;MACpBC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EAEDC,QAAQ,EAAE;IACRC,aAAaA,CAAA