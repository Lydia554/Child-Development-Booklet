{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      name: '',\n      birthdate: '',\n      errorMessage: '',\n      nameError: null,\n      birthdateError: null,\n      hasErrors: false\n    };\n  },\n  methods: {\n    // Validation for name\n    validateName() {\n      if (!this.name.trim()) {\n        this.nameError = \"Ime deteta je obavezno.\";\n        this.hasErrors = true;\n      } else {\n        this.nameError = null;\n      }\n    },\n    // Validation for birthdate\n    validateBirthdate() {\n      if (!this.birthdate) {\n        this.birthdateError = \"Datum rođenja deteta je obavezan.\";\n        this.hasErrors = true;\n      } else {\n        this.birthdateError = null;\n      }\n    },\n    // Registration of child\n    async registerChild() {\n      // Validate form fields before submitting\n      this.validateName();\n      this.validateBirthdate();\n\n      // If there are errors, don't submit the form\n      if (this.hasErrors) {\n        return;\n      }\n      try {\n        const response = await axios.post('https://child-development-backend.fly.dev/api/', {\n          name: this.name,\n          birthdate: this.birthdate\n        }, {\n          headers: {\n            'Authorization': `Bearer ${sessionStorage.getItem('token')}`\n          }\n        });\n\n        // On successful registration, redirect to dashboard\n        if (response.data) {\n          // Successful registration, redirect to dashboard\n          this.$router.push('/dashboard');\n        }\n      } catch (error) {\n        // Handle errors\n        console.error('Došlo je do greške pri registraciji deteta:', error);\n        if (error.response) {\n          this.errorMessage = error.response.data.message || 'Došlo je do greške pri registraciji deteta';\n        } else {\n          this.errorMessage = 'Došlo je do greške pri povezivanju sa serverom';\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","name","birthdate","errorMessage","nameError","birthdateError","hasErrors","methods","validateName","trim","validateBirthdate","registerChild","response","post","headers","sessionStorage","getItem","$router","push","error","console","message"],"sources":["C:\\Users\\goldb\\Pro\\Child-Development-Booklet\\frontend\\src\\components\\ChildRegistrationForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"registration-container\">\r\n    <h2>Registracija Deteta</h2>\r\n\r\n    <div v-if=\"errorMessage\" class=\"error-message\">\r\n      {{ errorMessage }}\r\n    </div>\r\n\r\n    <form @submit.prevent=\"registerChild\">\r\n      <input\r\n        type=\"text\"\r\n        v-model=\"name\"\r\n        placeholder=\"Ime deteta\"\r\n        required\r\n        @blur=\"validateName\"\r\n      />\r\n      <div v-if=\"nameError\" class=\"error-message\">{{ nameError }}</div>\r\n\r\n      <input\r\n        type=\"date\"\r\n        v-model=\"birthdate\"\r\n        required\r\n        @blur=\"validateBirthdate\"\r\n      />\r\n      <div v-if=\"birthdateError\" class=\"error-message\">{{ birthdateError }}</div>\r\n\r\n      <button type=\"submit\" :disabled=\"hasErrors\">Registruj Dete</button>\r\n    </form>\r\n\r\n    <div class=\"dashboard-link\">\r\n      <router-link to=\"/dashboard\">Idi na Dashboard</router-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      name: '',\r\n      birthdate: '',\r\n      errorMessage: '',\r\n      nameError: null,\r\n      birthdateError: null,\r\n      hasErrors: false\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    // Validation for name\r\n    validateName() {\r\n      if (!this.name.trim()) {\r\n        this.nameError = \"Ime deteta je obavezno.\";\r\n        this.hasErrors = true;\r\n      } else {\r\n        this.nameError = null;\r\n      }\r\n    },\r\n\r\n    // Validation for birthdate\r\n    validateBirthdate() {\r\n      if (!this.birthdate) {\r\n        this.birthdateError = \"Datum rođenja deteta je obavezan.\";\r\n        this.hasErrors = true;\r\n      } else {\r\n        this.birthdateError = null;\r\n      }\r\n    },\r\n\r\n    // Registration of child\r\n    async registerChild() {\r\n      // Validate form fields before submitting\r\n      this.validateName();\r\n      this.validateBirthdate();\r\n\r\n      // If there are errors, don't submit the form\r\n      if (this.hasErrors) {\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.post(\r\n          'https://child-development-backend.fly.dev/api/',\r\n          {\r\n            name: this.name,\r\n            birthdate: this.birthdate\r\n          },\r\n          {\r\n            headers: {\r\n              'Authorization': `Bearer ${sessionStorage.getItem('token')}`\r\n            }\r\n          }\r\n        );\r\n\r\n        // On successful registration, redirect to dashboard\r\n        if (response.data) {\r\n          // Successful registration, redirect to dashboard\r\n          this.$router.push('/dashboard');\r\n        }\r\n      } catch (error) {\r\n        // Handle errors\r\n        console.error('Došlo je do greške pri registraciji deteta:', error);\r\n        if (error.response) {\r\n          this.errorMessage = error.response.data.message || 'Došlo je do greške pri registraciji deteta';\r\n        } else {\r\n          this.errorMessage = 'Došlo je do greške pri povezivanju sa serverom';\r\n        }\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.registration-container {\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 8px;\r\n  background-color: #fff;\r\n\r\n  h2 {\r\n    text-align: center;\r\n  }\r\n\r\n  input {\r\n    width: 100%;\r\n    padding: 10px;\r\n    margin-bottom: 20px;\r\n    border: 1px solid #ddd;\r\n    bor