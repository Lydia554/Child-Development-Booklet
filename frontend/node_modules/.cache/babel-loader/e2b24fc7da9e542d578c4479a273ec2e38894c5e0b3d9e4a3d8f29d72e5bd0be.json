riods\"\r\n          :key=\"`${period.start}-${period.end}`\"\r\n          :class=\"getPeriodClass(period)\"\r\n          @click=\"handlePeriodSelection(period)\"\r\n          :disabled=\"isFuturePeriod(period)\">\r\n          {{ formatPeriod(period.start, period.end) }}\r\n          <span v-if=\"isFilledPeriod(period)\">‚úî Ispunjeno</span>\r\n          <span v-else-if=\"isCurrentPeriod(period)\">üü¢ Trenutni period</span>\r\n          <span v-else-if=\"isFuturePeriod(period)\">üö´ Nedostupan</span>\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Display milestones for selected period -->\r\n    <div v-if=\"computedSelectedPeriod\" class=\"period-data\">\r\n      <h3>Kljuƒçni momenti za period: {{ formatPeriod(computedSelectedPeriod.start, computedSelectedPeriod.end) }}</h3>\r\n\r\n      <div v-if=\"currentPeriodMilestones.length\" class=\"milestone-list\">\r\n        <div v-for=\"milestone in currentPeriodMilestones\" :key=\"milestone._id\" class=\"milestone-item\">\r\n          <h3>{{ milestone.milestoneType }} - {{ formatDate(milestone.milestoneDate) }}</h3>\r\n          <p><strong>Napredak:</strong> {{ milestone.progress }}</p>\r\n          <p><strong>Napomene:</strong> {{ milestone.notes }}</p>\r\n          <div class=\"button-group\">\r\n            <button @click=\"editMilestone(milestone._id)\" class=\"edit-btn\">Izmeni</button>\r\n            <button @click=\"confirmDelete(milestone._id)\" class=\"delete-btn\">Obri≈°i</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- No milestones message -->\r\n      <div v-else class=\"no-data-message\">\r\n        <p>Nema unetih podataka za ovaj period.</p>\r\n      </div>\r\n\r\n      <button @click=\"addMilestone\" class=\"add-milestone-button\">\r\n        Dodaj Kljuƒçni Momenat\r\n      </button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  props: {\r\n    childId: { type: String, required: true },\r\n    selectedPeriod: { type: Object, default: null }\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      childName: \"\",\r\n      childAge: null,\r\n      availablePeriods: [],\r\n      milestones: []\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    computedSelectedPeriod() {\r\n      return this.selectedPeriod || null; // Ensuring it doesn't break if null\r\n    },\r\n\r\n    currentPeriodMilestones() {\r\n      if (!this.computedSelectedPeriod || !this.computedSelectedPeriod.start) return [];\r\n      return this.milestones.filter(\r\n        (m) => m.startAge === this.computedSelectedPeriod.start && m.endAge === this.computedSelectedPeriod.end\r\n      );\r\n    }\r\n  },\r\n\r\n  async created() {\r\n    await this.fetchChildData();\r\n    await this.fetchMilestones();\r\n  },\r\n\r\n  methods: {\r\n    async fetchChildData() {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(`http://localhost:5000/api/children/${this.childId}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n\r\n        this.childName = response.data.name;\r\n        this.childAge = this.calculateAgeInMonths(response.data.birthDate);\r\n        this.generateAvailablePeriods();\r\n      } catch (error) {\r\n        console.error(\"‚ùå Gre≈°ka pri dobijanju podataka o detetu:\", error);\r\n      }\r\n    },\r\n\r\n    async fetchMilestones() {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(`http://localhost:5000/api/milestones/${this.childId}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n\r\n        this.milestones = response.data;\r\n      } catch (error) {\r\n        console.error(\"‚ùå Gre≈°ka pri dobijanju kljuƒçnih momenata:\", error);\r\n      }\r\n    },\r\n\r\n    handlePeriodSelection(period) {\r\n      this.$emit(\"update:selectedPeriod\", period); // Emit event to update the parent\r\n    },\r\n\r\n    addMilestone() {\r\n      if (!this.computedSelectedPeriod) return;\r\n      this.$router.push({\r\n        path: `/milestone-form/${this.childId}`,\r\n        query: { startAge: this.computedSelectedPeriod.start, endAge: this.computedSelectedPeriod.end }\r\n      });\r\n    },\r\n\r\n    editMilestone(id) {\r\n      this.$router.push(`/milestone-form/${this.childId}/${id}`);\r\n    },\r\n\r\n    async confirmDelete(id) {\r\n      if (window.confirm(\"Da li ste sigurni da ≈æelite da obri≈°ete ovaj kljuƒçni momenat?\")) {\r\n        await this.deleteMilestone(id);\r\n      }\r\n    },\r\n\r\n    async deleteMilestone(id) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        await axios.delete(`http://localhost:5000/api/milestones/${id}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n\r\n        this.milestones = this.milestones.filter((milestone) => milestone._id !== id);\r\n      } catch (error) {\r\n        console.error(\"Error deleting milestone:\", error);\r\n      }\r\n    },\r\n\r\n    formatPeriod(start, end) {\r\n      return `${start}-${end} meseci`;\r\n    },\r\n\r\n    formatDate(value) {\r\n      if (!value) return \"\";\r\n      return new Date(value).toLocaleDateString(\"sr-RS\");\r\n    },\r\n\r\n    calculateAgeInMonths(birthDate) {\r\n      if (!birthDate) return 0;\r\n      const birth = new Date(birthDate);\r\n      const now = new Date();\r\n      return (now.getFullYear() - birth.getFullYear()) * 12 + (now.getMonth() - birth.getMonth());\r\n    },\r\n\r\n    generateAvailablePeriods() {\r\n      if (this.childAge === null) return;\r\n\r\n      this.availablePeriods = [];\r\n      for (let i = 0; i <= 72; i += 6) {\r\n        this.availablePeriods.push({ start: i, end: i + 6 });\r\n      }\r\n    },\r\n\r\n    getPeriodClass(period) {\r\n      if (this.isCurrentPeriod(period)) return \"period-button current\";\r\n      if (this.isFilledPeriod(period)) return \"period-button filled\";\r\n      if (this.isFuturePeriod(period)) return \"period-button future\";\r\n      return \"period-button past\";\r\n    },\r\n\r\n    isCurrentPeriod(period) {\r\n      return this.childAge >= period.start && this.childAge < period.end;\r\n    },\r\n\r\n    isFilledPeriod(period) {\r\n      return this.childAge >= period.end;\r\n    },\r\n\r\n    isFuturePeriod(period) {\r\n      return this.childAge < period.start;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n.milestone-list-container {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f7f7f7;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n\r\n  h2 {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: #333;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .milestone-list {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 15px;\r\n  }\r\n\r\n  .milestone-item {\r\n    background-color: #fff;\r\n    padding: 20px;\r\n    border-radius: 8px;\r\n    box-shadow: 0 2px 6px rgba(0, 0, 0, 0.1);\r\n\r\n    h3 {\r\n      font-size: 20px;\r\n      color: #333;\r\n      margin-bottom: 10px;\r\n    }\r\n\r\n    p {\r\n      margin: 8px 0;\r\n      color: #555;\r\n    }\r\n\r\n    .button-group {\r\n      display: flex;\r\n      gap: 10px;\r\n      margin-top: 15px;\r\n    }\r\n\r\n    .edit-btn,\r\n    .delete-btn {\r\n      padding: 8px 16px;\r\n      border: none;\r\n      border-radius: 4px;\r\n      cursor: pointer;\r\n      font-weight: 500;\r\n      transition: all 0.3s ease;\r\n    }\r\n\r\n    .edit-btn {\r\n      background-color: #28a745;\r\n      color: white;\r\n\r\n      &:hover {\r\n        background-color: #218838;\r\n      }\r\n    }\r\n\r\n    .delete-btn {\r\n      background-color: #dc3545;\r\n      color: white;\r\n\r\n      &:hover {\r\n        background-color: #c82333;\r\n      }\r\n    }\r\n  }\r\n\r\n  .no-data-message {\r\n    text-align: center;\r\n    padding: 20px;\r\n    background-color: #fff;\r\n    border-radius: 8px;\r\n    color: #666;\r\n  }\r\n\r\n  .add-milestone-button {\r\n    display: inline-block;\r\n    margin-top: 20px;\r\n    padding: 12px 24px;\r\n    background-color: #007bff;\r\n    color: white;\r\n    text-decoration: none;\r\n    border-radius: 4px;\r\n    transition: background-color 0.3s ease;\r\n\r\n    &:hover {\r\n      background-color: #0056b3;\r\n    }\r\n  }\r\n}\r\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAA0B;;EAI9BA,KAAK,EAAC;AAAiB;;EAErBA,KAAK,EAAC;AAAc;mBAP/B;;EAAAC,GAAA;AAAA;;EAAAA,GAAA;AAAA;;EAAAA,GAAA;AAAA;;EAAAA,GAAA;EAuBuCD,KAAK,EAAC;;;EAvB7CC,GAAA;EA0BiDD,KAAK,EAAC;;;EAKxCA,KAAK,EAAC;AAAc;oBA/BnC;oBAAA;;uBACEE,mBAAA,CA8CM,OA9CNC,UA8CM,GA7CJC,mBAAA,CAA+D,YAA3D,qBAAmB,GAAAC,gBAAA,CAAGC,KAAA,CAAAC,SAAS,sCAEnCC,mBAAA,qBAAwB,EACxBJ,mBAAA,CAeM,OAfNK,UAeM,G,0BAdJL,mBAAA,CAA0B,YAAtB,mBAAiB,sBACrBA,mBAAA,CAYM,OAZNM,UAYM,I,kBAXJR,mBAAA,CAUSS,SAAA,QAlBjBC,WAAA,CAS2BN,KAAA,CAAAO,gBAAgB,EAA1BC,MAAM;yBADfZ,mBAAA,CAUS;MARND,GAAG,KAAKa,MAAM,CAACC,KAAK,IAAID,MAAM,CAACE,GAAG;MAClChB,KAAK,EAXhBiB,eAAA,CAWkBC,QAAA,CAAAC,cAAc,CAACL,MAAM;MAC5BM,OAAK,EAAAC,MAAA,IAAEH,QAAA,CAAAI,qBAAqB,CAACR,MAAM;MACnCS,QAAQ,EAAEL,QAAA,CAAAM,cAAc,CAACV,MAAM;QAb1CW,gBAAA,CAAApB,gBAAA,CAcaa,QAAA,CAAAQ,YAAY,CAACZ,MAAM,CAACC,KAAK,EAAED,MAAM,CAACE,GAAG,KAAI,GAC5C,iBAAYE,QAAA,CAAAS,cAAc,CAACb,MAAM,K,cAAjCZ,mBAAA,CAAsD,QAfhE0B,UAAA,EAe8C,aAAW,KAC9BV,QAAA,CAAAW,eAAe,CAACf,MAAM,K,cAAvCZ,mBAAA,CAAmE,QAhB7E4B,UAAA,EAgBoD,oBAAkB,KAC3CZ,QAAA,CAAAM,cAAc,CAACV,MAAM,K,cAAtCZ,mBAAA,CAA6D,QAjBvE6B,UAAA,EAiBmD,eAAa,KAjBhEvB,mBAAA,e,yBAAAwB,UAAA;sCAsBIxB,mBAAA,4CAA+C,EACpCU,QAAA,CAAAe,sBAAsB,I,cAAjC/B,mBAAA,CAuBM,OAvBNgC,UAuBM,GAtBJ9B,mBAAA,CAAgH,YAA5G,6BAA2B,GAAAC,gBAAA,CAAGa,QAAA,CAAAQ,YAAY,CAACR,QAAA,CAAAe,sBAAsB,CAAClB,KAAK,EAAEG,QAAA,CAAAe,sBAAsB,CAACjB,GAAG,mBAE5FE,QAAA,CAAAiB,uBAAuB,CAACC,MAAM,I,cAAzClC,mBAAA,CAUM,OAVNmC,UAUM,I,kBATJnC,mBAAA,CAQMS,SAAA,QAnCdC,WAAA,CA2BiCM,QAAA,CAAAiB,uBAAuB,EAApCG,SAAS;yBAArBpC,