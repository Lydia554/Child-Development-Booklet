iv v-else>\r\n        <div v-for=\"info in developmentData\" :key=\"info.ageRange\" class=\"info-section\">\r\n          <h3>{{ info.ageRange }}</h3>\r\n  \r\n          <div class=\"content-box\">\r\n            <h4>Razvojne Prekretnice</h4>\r\n            <ul>\r\n              <li v-for=\"milestone in info.milestones\" :key=\"milestone.description\">\r\n                <strong>{{ milestone.category }}:</strong> {{ milestone.description }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n  \r\n          <div class=\"content-box\">\r\n            <h4>Šta ako dete ne dostigne ove prekretnice?</h4>\r\n            <ul>\r\n              <li v-for=\"concern in info.concerns\" :key=\"concern.sign\">\r\n                <strong>{{ concern.sign }}:</strong> {{ concern.recommendation }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n  \r\n          <div class=\"content-box\">\r\n            <h4>Opšti Saveti</h4>\r\n            <p>{{ info.advice }}</p>\r\n          </div>\r\n  \r\n          <div class=\"content-box\">\r\n            <h4>Predložene Aktivnosti</h4>\r\n            <ul>\r\n              <li v-for=\"activity in info.exampleActivities\" :key=\"activity.title\">\r\n                <strong>{{ activity.title }}:</strong> {{ activity.description }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        loading: false,\r\n        developmentData: [\r\n          {\r\n            ageRange: \"0-6 meseci\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Podigne glavu dok leži na stomaku.\" },\r\n              { category: \"Govor i Jezik\", description: \"Počinje da guče i ispušta zvuke.\" },\r\n              { category: \"Kognitivni razvoj\", description: \"Prati predmete očima i prepoznaje lica.\" },\r\n            ],\r\n            concerns: [\r\n              { sign: \"Ne osmehuje se do 3. meseca\", recommendation: \"Pokušajte češće uspostaviti kontakt očima i pričati nežnim glasom.\" },\r\n              { sign: \"Ne reaguje na glasne zvukove\", recommendation: \"Koristite različite zvuke i proverite reakciju deteta.\" },\r\n            ],\r\n            advice: \"Roditelji treba da provode puno vremena pričajući i igrajući se s bebom, uključujući kontakt koža-na-kožu.\",\r\n            exampleActivities: [\r\n              { title: \"Igra ogledalom\", description: \"Držite bebu ispred ogledala i pravite smešne izraze lica.\" },\r\n              { title: \"Zvukovi i reakcije\", description: \"Koristite zvečke i nežne zvukove kako biste pratili reakciju deteta.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"6-12 meseci\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Počinje da sedi bez podrške.\" },\r\n              { category: \"Govor i Jezik\", description: \"Reaguje na svoje ime i ispušta različite glasove.\" },\r\n              { category: \"Kognitivni razvoj\", description: \"Ispušta predmete namerno da vidi šta će se desiti.\" },\r\n            ],\r\n            concerns: [\r\n              { sign: \"Ne pokazuje interesovanje za igračke\", recommendation: \"Podstaknite dete tako što ćete koristiti šarene igračke sa zvukovima.\" },\r\n              { sign: \"Ne pokušava da puzi\", recommendation: \"Ohrabrite dete stavljajući igračke malo dalje kako bi ih dohvatilo.\" },\r\n            ],\r\n            advice: \"Podstaknite bebu da istražuje okruženje koristeći sigurne igračke i igre sa roditeljima.\",\r\n            exampleActivities: [\r\n              { title: \"Skrivanje predmeta\", description: \"Sakrijte igračku ispod marame i pustite bebu da je pronađe.\" },\r\n              { title: \"Muzičke igre\", description: \"Igrajte muziku i pljeskajte kako bi dete pratilo ritam.\" },\r\n            ],\r\n          },\r\n          {\r\n            ageRange: \"12-18 meseci\",\r\n            milestones: [\r\n              { category: \"Motorika\", description: \"Počinje da hoda uz pridržavanje.\" },\r\n              { category: \"Govor i Jezik\", description: \"Govori nekoliko jednostavnih reči.\" },\r\n              { category: \"Kognitivni razvoj\", description: \"Razume osnovne naredbe i pokazuje interesovanje za igre.\" },\r\n            ],\r\n            concerns: [\r\n              { sign: \"Ne pokušava da stoji\", recommendation: \"Podstičite dete igračkama i pohvalama kada pokuša da ustane.\" },\r\n              { sign: \"Ne reaguje na jednostavne zahteve\", recommendation: \"Proverite da li razume i ponavljajte sa gestovima.\" },\r\n            ],\r\n            advice: \"Pohvalite dete kada pokuša nešto novo i uključite ga u svakodnevne aktivnosti.\",\r\n            exampleActivities: [\r\n              { title: \"Igranje sa lopticama\", description: \"Podstaknite dete da baca i hvata loptu.\" },\r\n              { title: \"Pričanje priča\", description: \"Čitajte jednostavne slikovnice i pokazujte slike.\" },\r\n            ],\r\n          },\r\n        ],\r\n      };\r\n    }\r\n  };\r\n  </script>\r\n  \r\n\r\n\r\n  <style lang=\"scss\" scoped>\r\n  .development-info {\r\n    max-width: 800px;\r\n    margin: 0 auto;\r\n    text-align: center;\r\n    font-family: \"Arial\", sans-serif;\r\n    background-color: #f8f9fa;\r\n    padding: 20px;\r\n    border-radius: 10px;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\r\n  \r\n    h2 {\r\n      color: #2c3e50;\r\n      font-size: 24px;\r\n      margin-bottom: 15px;\r\n    }\r\n  \r\n    .info-section {\r\n      background: white;\r\n      padding: 20px;\r\n      margin: 15px 0;\r\n      border-radius: 10px;\r\n      box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\r\n    }\r\n  \r\n    .content-box {\r\n      background-color: #eef2f3;\r\n      padding: 10px;\r\n      margin: 10px 0;\r\n      border-radius: 5px;\r\n    }\r\n  \r\n  