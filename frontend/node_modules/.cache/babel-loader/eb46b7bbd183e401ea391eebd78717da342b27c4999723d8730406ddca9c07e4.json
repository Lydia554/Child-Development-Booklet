{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  props: ['childId'],\n  data() {\n    return {\n      newDevelopment: {\n        childId: this.childId,\n        physicalDevelopment: '',\n        emotionalDevelopment: '',\n        intellectualDevelopment: '',\n        socialDevelopment: ''\n      },\n      errorMessage: ''\n    };\n  },\n  methods: {\n    async submitDevelopmentForm() {\n      if (!this.newDevelopment.physicalDevelopment || !this.newDevelopment.emotionalDevelopment || !this.newDevelopment.intellectualDevelopment || !this.newDevelopment.socialDevelopment) {\n        this.errorMessage = 'Sva polja su obavezna.';\n        return;\n      }\n      if (!this.newDevelopment.childId) {\n        console.error('childId je undefined');\n        this.errorMessage = 'Greška, childId nije prosleđen.';\n        return;\n      }\n      try {\n        const response = await axios.post(`http://localhost:5000/api/child-development/${this.newDevelopment.childId}`, this.newDevelopment);\n        console.log('Podaci su uspešno poslati:', response);\n        this.$router.push('/child-development-list'); // Redirect after success\n      } catch (error) {\n        console.error('Greška prilikom slanja podataka:', error);\n        this.errorMessage = 'Greška prilikom slanja podataka. Molimo pokušajte ponovo.';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","props","data","newDevelopment","childId","physicalDevelopment","emotionalDevelopment","intellectualDevelopment","socialDevelopment","errorMessage","methods","submitDevelopmentForm","console","error","response","post","log","$router","push"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"child-development-form\">\r\n    <h2 class=\"form-title\">Unos Razvoja Deteta</h2>\r\n    <form @submit.prevent=\"submitDevelopmentForm\" class=\"form-container\">\r\n      <div class=\"form-group\">\r\n        <label for=\"physicalDevelopment\">Fizički razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.physicalDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"emotionalDevelopment\">Emocionalni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.emotionalDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"intellectualDevelopment\">Intelektualni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.intellectualDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"socialDevelopment\">Socijalni razvoj:</label>\r\n        <textarea v-model=\"newDevelopment.socialDevelopment\" required class=\"form-input\"></textarea>\r\n      </div>\r\n      <button type=\"submit\" class=\"submit-btn\">Pošaljite</button>\r\n    </form>\r\n\r\n    <div v-if=\"errorMessage\" class=\"error-message\">\r\n      {{ errorMessage }}\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: ['childId'],\r\n\r\n  data() {\r\n    return {\r\n      newDevelopment: {\r\n        childId: this.childId,  \r\n        physicalDevelopment: '',\r\n        emotionalDevelopment: '',\r\n        intellectualDevelopment: '',\r\n        socialDevelopment: ''\r\n      },\r\n      errorMessage: ''\r\n    };\r\n  },\r\n  methods: {\r\n    async submitDevelopmentForm() {\r\n      if (!this.newDevelopment.physicalDevelopment || !this.newDevelopment.emotionalDevelopment || !this.newDevelopment.intellectualDevelopment || !this.newDevelopment.socialDevelopment) {\r\n        this.errorMessage = 'Sva polja su obavezna.';\r\n        return;\r\n      }\r\n\r\n      if (!this.newDevelopment.childId) {\r\n        console.error('childId je undefined');\r\n        this.errorMessage = 'Greška, childId nije prosleđen.';\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.post(`http://localhost:5000/api/child-development/${this.newDevelopment.childId}`, this.newDevelopment);\r\n        console.log('Podaci su uspešno poslati:', response);\r\n\r\n        this.$router.push('/child-development-list');  // Redirect after success\r\n      } catch (error) {\r\n        console.error('Greška prilikom slanja podataka:', error);\r\n        this.errorMessage = 'Greška prilikom slanja podataka. Molimo pokušajte ponovo.';\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.child-development-form {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f9f9f9;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n  \r\n  .form-title {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: #333;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .form-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .form-group {\r\n    margin-bottom: 15px;\r\n    \r\n    label {\r\n      font-size: 16px;\r\n      font-weight: bold;\r\n      color: #333;\r\n      margin-bottom: 5px;\r\n    }\r\n\r\n    .form-input {\r\n      width: 100%;\r\n      height: 100px;\r\n      padding: 10px;\r\n      font-size: 14px;\r\n      border: 1px solid #ccc;\r\n      border-radius: 4px;\r\n      resize: none;\r\n      box-sizing: border-box;\r\n    }\r\n  }\r\n\r\n  .submit-btn {\r\n    padding: 12px;\r\n    font-size: 16px;\r\n    color: #fff;\r\n    background-color: #4CAF50;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease;\r\n    \r\n    &:hover {\r\n      background-color: #45a049;\r\n    }\r\n  }\r\n\r\n  .error-message {\r\n    margin-top: 20px;\r\n    padding: 10px;\r\n    background-color: #ffdddd;\r\n    border: 1px solid #f5c6cb;\r\n    color: #721c24;\r\n    border-radius: 4px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AA8BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE,CAAC,SAAS,CAAC;EAElBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE;QACdC,OAAO,EAAE,IAAI,CAACA,OAAO;QACrBC,mBAAmB,EAAE,EAAE;QACvBC,oBAAoB,EAAE,EAAE;QACxBC,uBAAuB,EAAE,EAAE;QAC3BC,iBAAiB,EAAE;MACrB,CAAC;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,qBAAqBA,CAAA,EAAG;MAC5B,IAAI,CAAC,IAAI,CAACR,cAAc,CAACE,mBAAkB,IAAK,CAAC,IAAI,CAACF,cAAc,CAACG,oBAAmB,IAAK,CAAC,IAAI,CAACH,cAAc,CAACI,uBAAsB,IAAK,CAAC,IAAI,CAACJ,cAAc,CAACK,iBAAiB,EAAE;QACnL,IAAI,CAACC,YAAW,GAAI,wBAAwB;QAC5C;MACF;MAEA,IAAI,CAAC,IAAI,CAACN,cAAc,CAACC,OAAO,EAAE;QAChCQ,OAAO,CAACC,KAAK,CAAC,sBAAsB,CAAC;QACrC,IAAI,CAACJ,YAAW,GAAI,iCAAiC;QACrD;MACF;MAEA,IAAI;QACF,MAAMK,QAAO,GAAI,MAAMd,KAAK,CAACe,IAAI,CAAC,+CAA+C,IAAI,CAACZ,cAAc,CAACC,OAAO,EAAE,EAAE,IAAI,CAACD,cAAc,CAAC;QACpIS,OAAO,CAACI,GAAG,CAAC,4BAA4B,EAAEF,QAAQ,CAAC;QAEnD,IAAI,CAACG,OAAO,CAACC,IAAI,CAAC,yBAAyB,CAAC,EAAG;MACjD,EAAE,OAAOL,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;QACxD,IAAI,CAACJ,YAAW,GAAI,2DAA2D;MACjF;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}