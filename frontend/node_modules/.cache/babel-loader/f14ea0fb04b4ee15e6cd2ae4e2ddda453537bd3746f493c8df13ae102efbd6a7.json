{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport axios from \"axios\";\nimport jsPDF from \"jspdf\";\nimport autoTable from \"jspdf-autotable\";\nexport default {\n  data() {\n    return {\n      children: [],\n      selectedChildId: \"\",\n      child: {},\n      developmentData: [],\n      milestoneData: [],\n      loading: true,\n      errorMessage: \"\"\n    };\n  },\n  async created() {\n    console.log(\"Učitavam decu...\");\n    await this.fetchChildren();\n    console.log(\"Deca učitana:\", this.children);\n  },\n  methods: {\n    async fetchChildren() {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(\"http://localhost:5000/api\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.children = response.data;\n        console.log(\"Lista dece učitana:\", this.children);\n        if (this.children.length > 0) {\n          this.selectedChildId = this.children[0]._id;\n          await this.fetchChildData(this.selectedChildId);\n          await this.fetchDevelopmentData(this.selectedChildId);\n          await this.fetchMilestones(this.selectedChildId);\n        }\n      } catch (error) {\n        console.error(\"❌ Greška pri učitavanju dece:\", error);\n        this.errorMessage = \"Nije moguće učitati listu dece.\";\n      } finally {\n        this.loading = false;\n      }\n    },\n    async fetchChildData(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (!response.data || Object.keys(response.data).length === 0) {\n          this.errorMessage = \"Podaci o detetu nisu pronađeni.\";\n          return;\n        }\n        this.child = response.data;\n        console.log(\"Podaci o detetu:\", this.child);\n      } catch (error) {\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\n        this.errorMessage = \"Nije moguće učitati podatke o detetu.\";\n      }\n    },\n    async fetchDevelopmentData(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.developmentData = response.data || [];\n      } catch (error) {\n        console.error(\"❌ Greška pri dobijanju podataka o razvoju:\", error);\n        this.errorMessage = \"Nije moguće učitati razvojne podatke.\";\n      }\n    },\n    async fetchMilestones(childId) {\n      try {\n        const token = sessionStorage.getItem(\"token\");\n        const response = await axios.get(`http://localhost:5000/api/milestones/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.milestoneData = response.data || [];\n      } catch (error) {\n        console.error(\"❌ Greška pri dobijanju ključnih momenata:\", error);\n      }\n    },\n    onChildSelect() {\n      console.log(\"Odabrano dete ID:\", this.selectedChildId);\n      this.fetchChildData(this.selectedChildId);\n      this.fetchDevelopmentData(this.selectedChildId);\n      this.fetchMilestones(this.selectedChildId);\n    },\n    downloadPDF() {\n      const doc = new jsPDF();\n      doc.text(`Razvoj deteta: ${this.child.name}`, 10, 10);\n      const developmentTable = [[\"Period\", \"Fizički\", \"Emocionalni\", \"Intelektualni\", \"Socijalni\"], ...this.developmentData.map(period => [`${period.startAge}-${period.endAge} meseci`, period.physicalDevelopment || \"-\", period.emotionalDevelopment || \"-\", period.intellectualDevelopment || \"-\", period.socialDevelopment || \"-\"])];\n      const milestoneTable = [[\"Tip momenta\", \"Datum\", \"Napredak\", \"Napomene\"], ...this.milestoneData.map(milestone => [milestone.milestoneType, this.formatDate(milestone.milestoneDate), milestone.progress || \"-\", milestone.notes || \"-\"])];\n      autoTable(doc, {\n        head: [developmentTable[0]],\n        body: developmentTable.slice(1),\n        startY: 20\n      }); // Popravka\n      autoTable(doc, {\n        head: [milestoneTable[0]],\n        body: milestoneTable.slice(1),\n        startY: doc.lastAutoTable.finalY + 10\n      }); // Popravka\n      doc.save(`Razvoj_${this.child.name}.pdf`);\n    }\n  }\n};","map":{"version":3,"names":["axios","jsPDF","autoTable","data","children","selectedChildId","child","developmentData","milestoneData","loading","errorMessage","created","console","log","fetchChildren","methods","token","sessionStorage","getItem","response","get","headers","Authorization","length","_id","fetchChildData","fetchDevelopmentData","fetchMilestones","error","childId","Object","keys","onChildSelect","downloadPDF","doc","text","name","developmentTable","map","period","startAge","endAge","physicalDevelopment","emotionalDevelopment","intellectualDevelopment","socialDevelopment","milestoneTable","milestone","milestoneType","formatDate","milestoneDate","progress","notes","head","body","slice","startY","lastAutoTable","finalY","save"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildOverview.vue"],"sourcesContent":["<template>\r\n  <div class=\"overview-container\">\r\n    <h2>Pregled razvoja deteta - {{ child.name }}</h2>\r\n\r\n    <div v-if=\"loading\">Učitavanje podataka...</div>\r\n    <div v-else-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</div>\r\n    <div v-else>\r\n      <!-- Dropdown za izbor deteta -->\r\n      <div class=\"select-container\" v-if=\"children.length > 0\">\r\n        <label for=\"child-select\">Izaberite dete:</label>\r\n        <select v-model=\"selectedChildId\" @change=\"onChildSelect\">\r\n          <option v-for=\"child in children\" :key=\"child._id\" :value=\"child._id\">\r\n            {{ child.name }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n      \r\n      <!-- Tabela sa podacima o razvoju -->\r\n      <h3>Razvoj deteta</h3>\r\n      <table v-if=\"developmentData.length > 0\" class=\"development-table\">\r\n        <thead>\r\n          <tr>\r\n            <th>Period</th>\r\n            <th>Fizički razvoj</th>\r\n            <th>Emocionalni razvoj</th>\r\n            <th>Intelektualni razvoj</th>\r\n            <th>Socijalni razvoj</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"period in developmentData\" :key=\"period.startAge\">\r\n            <td>{{ period.startAge }} - {{ period.endAge }} meseci</td>\r\n            <td>{{ period.physicalDevelopment || '-' }}</td>\r\n            <td>{{ period.emotionalDevelopment || '-' }}</td>\r\n            <td>{{ period.intellectualDevelopment || '-' }}</td>\r\n            <td>{{ period.socialDevelopment || '-' }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n\r\n      <!-- Tabela sa milestone podacima -->\r\n      <h3>Ključni momenti</h3>\r\n      <table v-if=\"milestoneData.length > 0\" class=\"milestone-table\">\r\n        <thead>\r\n          <tr>\r\n            <th>Tip momenta</th>\r\n            <th>Datum</th>\r\n            <th>Napredak</th>\r\n            <th>Napomene</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"milestone in milestoneData\" :key=\"milestone._id\">\r\n            <td>{{ milestone.milestoneType }}</td>\r\n            <td>{{ formatDate(milestone.milestoneDate) }}</td>\r\n            <td>{{ milestone.progress || '-' }}</td>\r\n            <td>{{ milestone.notes || '-' }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n\r\n      <!-- Dugme za preuzimanje PDF-a -->\r\n      <button @click=\"downloadPDF\" class=\"pdf-button\">Preuzmi PDF</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport jsPDF from \"jspdf\";\r\nimport autoTable from \"jspdf-autotable\";\r\n\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      children: [],\r\n      selectedChildId: \"\",\r\n      child: {},\r\n      developmentData: [],\r\n      milestoneData: [],\r\n      loading: true,\r\n      errorMessage: \"\",\r\n    };\r\n  },\r\n  async created() {\r\n    console.log(\"Učitavam decu...\");\r\n    await this.fetchChildren();\r\n    console.log(\"Deca učitana:\", this.children);\r\n  },\r\n  methods: {\r\n    async fetchChildren() {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\"http://localhost:5000/api\", {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n\r\n        this.children = response.data;\r\n        console.log(\"Lista dece učitana:\", this.children);\r\n\r\n        if (this.children.length > 0) {\r\n          this.selectedChildId = this.children[0]._id;\r\n          await this.fetchChildData(this.selectedChildId);\r\n          await this.fetchDevelopmentData(this.selectedChildId);\r\n          await this.fetchMilestones(this.selectedChildId);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri učitavanju dece:\", error);\r\n        this.errorMessage = \"Nije moguće učitati listu dece.\";\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    async fetchChildData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n\r\n        if (!response.data || Object.keys(response.data).length === 0) {\r\n          this.errorMessage = \"Podaci o detetu nisu pronađeni.\";\r\n          return;\r\n        }\r\n\r\n        this.child = response.data;\r\n        console.log(\"Podaci o detetu:\", this.child);\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri dobijanju podataka o detetu:\", error);\r\n        this.errorMessage = \"Nije moguće učitati podatke o detetu.\";\r\n      }\r\n    },\r\n\r\n    async fetchDevelopmentData(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        this.developmentData = response.data || [];\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri dobijanju podataka o razvoju:\", error);\r\n        this.errorMessage = \"Nije moguće učitati razvojne podatke.\";\r\n      }\r\n    },\r\n\r\n    async fetchMilestones(childId) {\r\n      try {\r\n        const token = sessionStorage.getItem(\"token\");\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/milestones/${childId}`,\r\n          { headers: { Authorization: `Bearer ${token}` } }\r\n        );\r\n        this.milestoneData = response.data || [];\r\n      } catch (error) {\r\n        console.error(\"❌ Greška pri dobijanju ključnih momenata:\", error);\r\n      }\r\n    },\r\n\r\n    onChildSelect() {\r\n      console.log(\"Odabrano dete ID:\", this.selectedChildId);\r\n      this.fetchChildData(this.selectedChildId);\r\n      this.fetchDevelopmentData(this.selectedChildId);\r\n      this.fetchMilestones(this.selectedChildId);\r\n    },\r\n\r\n    downloadPDF() {\r\n  const doc = new jsPDF();\r\n  doc.text(`Razvoj deteta: ${this.child.name}`, 10, 10);\r\n\r\n  const developmentTable = [\r\n    [\"Period\", \"Fizički\", \"Emocionalni\", \"Intelektualni\", \"Socijalni\"],\r\n    ...this.developmentData.map((period) => [\r\n      `${period.startAge}-${period.endAge} meseci`,\r\n      period.physicalDevelopment || \"-\",\r\n      period.emotionalDevelopment || \"-\",\r\n      period.intellectualDevelopment || \"-\",\r\n      period.socialDevelopment || \"-\",\r\n    ]),\r\n  ];\r\n\r\n  const milestoneTable = [\r\n    [\"Tip momenta\", \"Datum\", \"Napredak\", \"Napomene\"],\r\n    ...this.milestoneData.map((milestone) => [\r\n      milestone.milestoneType,\r\n      this.formatDate(milestone.milestoneDate),\r\n      milestone.progress || \"-\",\r\n      milestone.notes || \"-\",\r\n    ]),\r\n  ];\r\n\r\n  autoTable(doc, { head: [developmentTable[0]], body: developmentTable.slice(1), startY: 20 }); // Popravka\r\n  autoTable(doc, { head: [milestoneTable[0]], body: milestoneTable.slice(1), startY: doc.lastAutoTable.finalY + 10 }); // Popravka\r\n  doc.save(`Razvoj_${this.child.name}.pdf`);\r\n},\r\n\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n.overview-container {\r\n  padding: 20px;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  font-family: Arial, sans-serif;\r\n\r\n  h2 {\r\n    text-align: center;\r\n    font-size: 28px;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  /* Loading and Error Messages */\r\n  .loading, .error-message {\r\n    text-align: center;\r\n    font-size: 18px;\r\n    color: #ff0000;\r\n  }\r\n\r\n  /* Dropdown Selection for Child */\r\n  .select-container {\r\n    margin-bottom: 20px;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n\r\n    label {\r\n      font-size: 18px;\r\n      font-weight: bold;\r\n      margin-bottom: 8px;\r\n    }\r\n\r\n    select {\r\n      width: 60%;\r\n      max-width: 300px;\r\n      padding: 12px;\r\n      font-size: 16px;\r\n      font-weight: bold;\r\n      color: #333;\r\n      background-color: #f0f0f0;\r\n      border: 2px solid #007bff;\r\n      border-radius: 8px;\r\n      cursor: pointer;\r\n      transition: all 0.3s ease-in-out;\r\n\r\n      &:hover {\r\n        background-color: #e0e0e0;\r\n      }\r\n\r\n      &:focus {\r\n        outline: none;\r\n        border-color: #004085;\r\n        box-shadow: 0 0 8px rgba(0, 91, 187, 0.5);\r\n      }\r\n    }\r\n  }\r\n\r\n  /* Development Table */\r\n  h3 {\r\n    font-size: 22px;\r\n    margin-bottom: 10px;\r\n    text-align: center;\r\n    color: #333;\r\n  }\r\n\r\n  .development-table,\r\n  .milestone-table {\r\n    width: 100%;\r\n    margin: 0 auto;\r\n    border-collapse: collapse;\r\n    border: 1px solid #ddd;\r\n    margin-bottom: 20px;\r\n\r\n    th, td {\r\n      padding: 12px;\r\n      text-align: left;\r\n      border: 1px solid #ddd;\r\n    }\r\n\r\n    th {\r\n      background-color: #007bff;\r\n      color: white;\r\n    }\r\n\r\n    td {\r\n      font-size: 14px;\r\n    }\r\n\r\n    td, th {\r\n      word-wrap: break-word;\r\n    }\r\n  }\r\n\r\n  /* Milestone Section */\r\n  .milestone-table {\r\n    margin-top: 20px;\r\n  }\r\n\r\n  /* Button to Download PDF */\r\n  .pdf-button {\r\n    display: block;\r\n    margin: 20px auto;\r\n    padding: 10px 20px;\r\n    background-color: #28a745;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    font-size: 18px;\r\n\r\n    &:hover {\r\n      background-color: #218838;\r\n    }\r\n  }\r\n\r\n  /* Mobile Adjustments */\r\n  @media (max-width: 768px) {\r\n    padding: 15px;\r\n\r\n    h2 {\r\n      font-size: 22px;\r\n    }\r\n\r\n    .select-container {\r\n      width: 100%;\r\n      label {\r\n        font-size: 16px;\r\n      }\r\n\r\n      select {\r\n        width: 100%;\r\n        font-size: 14px;\r\n        padding: 10px;\r\n      }\r\n    }\r\n\r\n    .development-table,\r\n    .milestone-table {\r\n      font-size: 12px;\r\n\r\n      th, td {\r\n        padding: 8px;\r\n      }\r\n    }\r\n\r\n    .pdf-button {\r\n      font-size: 16px;\r\n      padding: 12px 24px;\r\n    }\r\n  }\r\n\r\n  /* Additional Styling for Milestones */\r\n  .milestone-item {\r\n    margin-bottom: 15px;\r\n    padding: