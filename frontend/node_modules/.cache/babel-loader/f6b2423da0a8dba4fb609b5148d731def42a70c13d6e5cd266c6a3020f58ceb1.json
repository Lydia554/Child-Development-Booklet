opment) {\r\n    this.errorMessage = 'Sva polja su obavezna.';\r\n    return;\r\n  }\r\n\r\n  try {\r\n    const response = await axios.post(\r\n      `http://localhost:5000/api/child-development/${this.newDevelopment.childId}`, \r\n      this.newDevelopment // Make sure startAge and endAge are in the body\r\n    );\r\n    console.log('Podaci su uspešno poslati:', response);\r\n    this.$router.push('/child-development-list');\r\n  } catch (error) {\r\n    console.error('Greška prilikom slanja podataka:', error);\r\n    this.errorMessage = 'Greška prilikom slanja podataka. Molimo pokušajte ponovo.';\r\n  }\r\n}\r\n\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.child-development-form {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #f9f9f9;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n  \r\n  .form-title {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    color: #333;\r\n    margin-bottom: 20px;\r\n  }\r\n\r\n  .form-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .form-group {\r\n    margin-bottom: 15px;\r\n    \r\n    label {\r\n      font-size: 16px;\r\n      font-weight: bold;\r\n      color: #333;\r\n      margin-bottom: 5px;\r\n    }\r\n\r\n    .form-input {\r\n      width: 100%;\r\n      height: 100px;\r\n      padding: 10px;\r\n      font-size: 14px;\r\n      border: 1px solid #ccc;\r\n      border-radius: 4px;\r\n      resize: none;\r\n      box-sizing: border-box;\r\n    }\r\n  }\r\n\r\n  .submit-btn {\r\n    padding: 12px;\r\n    font-size: 16px;\r\n    color: #fff;\r\n    background-color: #4CAF50;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease;\r\n    \r\n    &:hover {\r\n      background-color: #45a049;\r\n    }\r\n  }\r\n\r\n  .error-message {\r\n    margin-top: 20px;\r\n    padding: 10px;\r\n    background-color: #ffdddd;\r\n    border: 1px solid #f5c6cb;\r\n    color: #721c24;\r\n    border-radius: 4px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AA8BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE,CAAC,SAAS,EAAE,UAAU,CAAC;EAE9BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACPC,cAAc,EAAE;QACdC,OAAO,EAAE,IAAI,CAACA,OAAO;QACrBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QAAG;QAC1BC,MAAM,EAAE,IAAI,CAACD,QAAO,GAAI,CAAC;QAAE;QAC3BE,mBAAmB,EAAE,EAAE;QACvBC,oBAAoB,EAAE,EAAE;QACxBC,uBAAuB,EAAE,EAAE;QAC3BC,iBAAiB,EAAE;MACrB,CAAC;MACDC,YAAY,EAAE;IAChB,CAAC;EACD,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,qBAAqBA,CAAA,EAAG;MAChC,IAAI,CAAC,IAAI,CAACV,cAAc,CAACI,mBAAkB,IACvC,CAAC,IAAI,CAACJ,cAAc,CAACK,oBAAmB,IACxC,CAAC,IAAI,CAACL,cAAc,CAACM,uBAAsB,IAC3C,CAAC,IAAI,CAACN,cAAc,CAACO,iBAAiB,EAAE;QAC1C,IAAI,CAACC,YAAW,GAAI,wBAAwB;QAC5C;MACF;MAEA,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMd,KAAK,CAACe,IAAI,CAC/B,+CAA+C,IAAI,CAACZ,cAAc,CAACC,OAAO,EAAE,EAC5E,IAAI,CAACD,cAAa,CAAE;QACtB,CAAC;QACDa,OAAO,CAACC,GAAG,CAAC,4BAA4B,EAAEH,QAAQ,CAAC;QACnD,IAAI,CAACI,OAAO,CAACC,IAAI,CAAC,yBAAyB,CAAC;MAC9C,EAAE,OAAOC,KAAK,EAAE;QACdJ,OAAO,CAACI,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;QACxD,IAAI,CAACT,YAAW,GAAI,2DAA2D;MACjF;IACF;EAEE;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}