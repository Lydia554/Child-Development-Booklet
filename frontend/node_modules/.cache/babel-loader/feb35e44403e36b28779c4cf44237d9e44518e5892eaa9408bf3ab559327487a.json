{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport { useAuthStore } from '@/stores/authStore';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      formData: {\n        startAge: null,\n        endAge: null,\n        physicalDevelopment: '',\n        emotionalDevelopment: '',\n        intellectualDevelopment: '',\n        socialDevelopment: ''\n      },\n      childBirthDate: null,\n      error: null,\n      loading: false,\n      physicalError: null,\n      emotionalError: null,\n      intellectualError: null,\n      socialError: null,\n      recommendedDevelopment: {\n        physicalDevelopment: {\n          min: 0,\n          max: 100\n        },\n        emotionalDevelopment: {\n          min: 0,\n          max: 100\n        },\n        intellectualDevelopment: {\n          min: 0,\n          max: 100\n        },\n        socialDevelopment: {\n          min: 0,\n          max: 100\n        }\n      },\n      hasErrors: false\n    };\n  },\n  async created() {\n    try {\n      await this.fetchChildData();\n      await this.loadDevelopmentData();\n    } catch (error) {\n      console.error('Error in created:', error);\n      this.error = 'Gre≈°ka pri uƒçitavanju podataka.';\n    }\n  },\n  methods: {\n    validateInput(field) {\n      this.hasErrors = false;\n      const inputValue = parseInt(this.formData[field]);\n      if (inputValue < this.recommendedDevelopment[field].min) {\n        this[`${field}Error`] = `Preporuƒçeni minimum za ${field} je ${this.recommendedDevelopment[field].min}.`;\n        this.hasErrors = true;\n      } else if (inputValue > this.recommendedDevelopment[field].max) {\n        this[`${field}Error`] = `Preporuƒçeni maksimum za ${field} je ${this.recommendedDevelopment[field].max}.`;\n        this.hasErrors = true;\n      } else {\n        this[`${field}Error`] = null;\n      }\n    },\n    async fetchChildData() {\n      try {\n        const authStore = useAuthStore();\n        const token = authStore.token;\n        const childId = authStore.childId;\n        if (!childId) {\n          console.error(\"‚ùå No child ID found in authStore.\");\n          this.error = \"Nedostaje ID deteta.\";\n          return;\n        }\n        const response = await axios.get(`http://localhost:5000/api/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        console.log(\"‚úÖ API response for child data:\", response.data);\n        if (response.data && response.data.birthDate) {\n          this.childBirthDate = response.data.birthDate;\n          console.log(\"üìÖ Child's birth date:\", this.childBirthDate);\n          this.calculateDevelopmentPeriod();\n        } else {\n          console.warn(\"‚ö†Ô∏è No birthDate found in response!\");\n          this.error = \"Nije moguƒáe dobiti datum roƒëenja.\";\n        }\n      } catch (error) {\n        console.error('‚ùå Error fetching child data:', error.response?.data || error);\n        this.error = 'Gre≈°ka pri uƒçitavanju podataka o detetu.';\n      }\n    },\n    calculateDevelopmentPeriod() {\n      if (!this.childBirthDate) return;\n      const birthDate = new Date(this.childBirthDate);\n      const today = new Date();\n      const ageInMonths = (today.getFullYear() - birthDate.getFullYear()) * 12 + (today.getMonth() - birthDate.getMonth());\n      const startAge = Math.floor(ageInMonths / 6) * 6;\n      const endAge = startAge + 6;\n      console.log(`üìä Calculated period: ${startAge} - ${endAge} months`);\n      this.formData.startAge = startAge;\n      this.formData.endAge = endAge;\n    },\n    async loadDevelopmentData() {\n      try {\n        const authStore = useAuthStore();\n        const token = authStore.token;\n        const childId = authStore.childId;\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (response.data) {\n          const periods = response.data;\n          const currentPeriod = periods.find(p => parseInt(p.startAge) === this.formData.startAge && parseInt(p.endAge) === this.formData.endAge);\n          if (currentPeriod) {\n            this.formData = {\n              startAge: parseInt(currentPeriod.startAge),\n              endAge: parseInt(currentPeriod.endAge),\n              physicalDevelopment: currentPeriod.physicalDevelopment || '',\n              emotionalDevelopment: currentPeriod.emotionalDevelopment || '',\n              intellectualDevelopment: currentPeriod.intellectualDevelopment || '',\n              socialDevelopment: currentPeriod.socialDevelopment || ''\n            };\n          }\n        }\n      } catch (error) {\n        console.error('‚ùå Error loading development data:', error);\n        this.error = 'Gre≈°ka pri uƒçitavanju postojeƒáih podataka.';\n      }\n    },\n    async saveDevelopment() {\n      if (this.hasErrors) {\n        this.error = 'Molimo ispravite gre≈°ke pre nego ≈°to po≈°aljete podatke.';\n        return;\n      }\n      try {\n        this.loading = true;\n        const authStore = useAuthStore();\n        const childId = authStore.childId;\n        const token = authStore.token;\n        console.log(\"üì§ Sending development data:\", this.formData);\n        const response = await axios.post(`http://localhost:5000/api/child-development/${childId}`, this.formData, {\n          headers: {\n            'Authorization': `Bearer ${token}`,\n            'Content-Type': 'application/json'\n          }\n        });\n        if (response.data) {\n          console.log(\"‚úÖ Development data saved:\", response.data);\n          this.$router.push(`/child-development-list/${childId}`);\n        }\n      } catch (error) {\n        console.error('‚ùå Error saving development:', error);\n        this.error = 'Gre≈°ka pri ƒçuvanju podataka.';\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["useAuthStore","axios","data","formData","startAge","endAge","physicalDevelopment","emotionalDevelopment","intellectualDevelopment","socialDevelopment","childBirthDate","error","loading","physicalError","emotionalError","intellectualError","socialError","recommendedDevelopment","min","max","hasErrors","created","fetchChildData","loadDevelopmentData","console","methods","validateInput","field","inputValue","parseInt","authStore","token","childId","response","get","headers","Authorization","log","birthDate","calculateDevelopmentPeriod","warn","Date","today","ageInMonths","getFullYear","getMonth","Math","floor","periods","currentPeriod","find","p","saveDevelopment","post","$router","push"],"sources":["C:\\Users\\goldb\\Pro\\Project\\basic-booklet\\frontend\\src\\components\\ChildDevelopmentForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"development-form\">\r\n    <h2>Unos Razvoja Deteta</h2>\r\n    <h3>Trenutni period: {{ formData.startAge }} - {{ formData.endAge }} meseci</h3>\r\n\r\n    <div v-if=\"error\" class=\"error-message\">\r\n      {{ error }}\r\n    </div>\r\n\r\n    <form @submit.prevent=\"saveDevelopment\">\r\n      <div class=\"form-group\">\r\n        <label for=\"physicalDevelopment\">Fiziƒçki razvoj:</label>\r\n        <textarea\r\n          id=\"physicalDevelopment\"\r\n          v-model=\"formData.physicalDevelopment\"\r\n          required\r\n          @input=\"validateInput('physicalDevelopment')\"\r\n        ></textarea>\r\n        <div v-if=\"physicalError\" class=\"error-message\">{{ physicalError }}</div>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"emotionalDevelopment\">Emocionalni razvoj:</label>\r\n        <textarea\r\n          id=\"emotionalDevelopment\"\r\n          v-model=\"formData.emotionalDevelopment\"\r\n          required\r\n          @input=\"validateInput('emotionalDevelopment')\"\r\n        ></textarea>\r\n        <div v-if=\"emotionalError\" class=\"error-message\">{{ emotionalError }}</div>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"intellectualDevelopment\">Intelektualni razvoj:</label>\r\n        <textarea\r\n          id=\"intellectualDevelopment\"\r\n          v-model=\"formData.intellectualDevelopment\"\r\n          required\r\n          @input=\"validateInput('intellectualDevelopment')\"\r\n        ></textarea>\r\n        <div v-if=\"intellectualError\" class=\"error-message\">{{ intellectualError }}</div>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"socialDevelopment\">Socijalni razvoj:</label>\r\n        <textarea\r\n          id=\"socialDevelopment\"\r\n          v-model=\"formData.socialDevelopment\"\r\n          required\r\n          @input=\"validateInput('socialDevelopment')\"\r\n        ></textarea>\r\n        <div v-if=\"socialError\" class=\"error-message\">{{ socialError }}</div>\r\n      </div>\r\n\r\n      <button type=\"submit\" :disabled=\"loading || hasErrors\">\r\n        {{ loading ? 'ƒåuvanje...' : 'Saƒçuvaj' }}\r\n      </button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useAuthStore } from '@/stores/authStore';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      formData: {\r\n        startAge: null,\r\n        endAge: null,\r\n        physicalDevelopment: '',\r\n        emotionalDevelopment: '',\r\n        intellectualDevelopment: '',\r\n        socialDevelopment: ''\r\n      },\r\n      childBirthDate: null,\r\n      error: null,\r\n      loading: false,\r\n\r\n      physicalError: null,\r\n      emotionalError: null,\r\n      intellectualError: null,\r\n      socialError: null,\r\n\r\n      recommendedDevelopment: {\r\n        physicalDevelopment: { min: 0, max: 100 },\r\n        emotionalDevelopment: { min: 0, max: 100 },\r\n        intellectualDevelopment: { min: 0, max: 100 },\r\n        socialDevelopment: { min: 0, max: 100 }\r\n      },\r\n\r\n      hasErrors: false\r\n    };\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      await this.fetchChildData();\r\n      await this.loadDevelopmentData();\r\n    } catch (error) {\r\n      console.error('Error in created:', error);\r\n      this.error = 'Gre≈°ka pri uƒçitavanju podataka.';\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    validateInput(field) {\r\n      this.hasErrors = false;\r\n\r\n      const inputValue = parseInt(this.formData[field]);\r\n\r\n      if (inputValue < this.recommendedDevelopment[field].min) {\r\n        this[`${field}Error`] = `Preporuƒçeni minimum za ${field} je ${this.recommendedDevelopment[field].min}.`;\r\n        this.hasErrors = true;\r\n      } else if (inputValue > this.recommendedDevelopment[field].max) {\r\n        this[`${field}Error`] = `Preporuƒçeni maksimum za ${field} je ${this.recommendedDevelopment[field].max}.`;\r\n        this.hasErrors = true;\r\n      } else {\r\n        this[`${field}Error`] = null;\r\n      }\r\n    },\r\n\r\n    async fetchChildData() {\r\n      try {\r\n        const authStore = useAuthStore();\r\n        const token = authStore.token;\r\n        const childId = authStore.childId;\r\n\r\n        if (!childId) {\r\n          console.error(\"‚ùå No child ID found in authStore.\");\r\n          this.error = \"Nedostaje ID deteta.\";\r\n          return;\r\n        }\r\n\r\n        const response = await axios.get(`http://localhost:5000/api/${childId}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n\r\n        console.log(\"‚úÖ API response for child data:\", response.data);\r\n\r\n        if (response.data && response.data.birthDate) {\r\n          this.childBirthDate = response.data.birthDate;\r\n          console.log(\"üìÖ Child's birth date:\", this.childBirthDate);\r\n          this.calculateDevelopmentPeriod();\r\n        } else {\r\n          console.warn(\"‚ö†Ô∏è No birthDate found in response!\");\r\n          this.error = \"Nije moguƒáe dobiti datum roƒëenja.\";\r\n        }\r\n      } catch (error) {\r\n        console.error('‚ùå Error fetching child data:', error.response?.data || error);\r\n        this.error = 'Gre≈°ka pri uƒçitavanju podataka o detetu.';\r\n      }\r\n    },\r\n\r\n    calculateDevelopmentPeriod() {\r\n      if (!this.childBirthDate) return;\r\n\r\n      const birthDate = new Date(this.childBirthDate);\r\n      const today = new Date();\r\n      const ageInMonths = (today.getFullYear() - birthDate.getFullYear()) * 12 + (today.getMonth() - birthDate.getMonth());\r\n\r\n      const startAge = Math.floor(ageInMonths / 6) * 6;\r\n      const endAge = startAge + 6;\r\n\r\n      console.log(`üìä Calculated period: ${startAge} - ${endAge} months`);\r\n\r\n      this.formData.startAge = startAge;\r\n      this.formData.endAge = endAge;\r\n    },\r\n\r\n    async loadDevelopmentData() {\r\n      try {\r\n        const authStore = useAuthStore();\r\n        const token = authStore.token;\r\n        const childId = authStore.childId;\r\n\r\n        const response = await axios.get(`http://localhost:5000/api/child-development/${childId}`, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n\r\n        if (response.data) {\r\n          const periods = response.data;\r\n          const currentPeriod = periods.find(p =>\r\n            parseInt(p.startAge) === this.formData.startAge &&\r\n            parseInt(p.endAge) === this.formData.endAge\r\n          );\r\n\r\n          if (currentPeriod) {\r\n            this.formData = {\r\n              startAge: parseInt(currentPeriod.startAge),\r\n              endAge: parseInt(currentPeriod.endAge),\r\n              physicalDevelopment: currentPeriod.physicalDevelopment || '',\r\n              emotionalDevelopment: currentPeriod.emotionalDevelopment || '',\r\n              intellectualDevelopment: currentPeriod.intellectualDevelopment || '',\r\n              socialDevelopment: currentPeriod.socialDevelopment || ''\r\n            };\r\n          }\r\n        }\r\n      } catch (error) {\r\n        console.error('‚ùå Error loading development data:', error);\r\n        this.error = 'Gre≈°ka pri uƒçitavanju postojeƒáih podataka.';\r\n      }\r\n    },\r\n\r\n    async saveDevelopment() {\r\n      if (this.hasErrors) {\r\n        this.error = 'Molimo ispravite gre≈°ke pre nego ≈°to po≈°aljete podatke.';\r\n        return;\r\n      }\r\n\r\n      try {\r\n        this.loading = true;\r\n        const authStore = useAuthStore();\r\n        const childId = authStore.childId;\r\n        const token = authStore.token;\r\n\r\n        console.log(\"üì§ Sending development data:\", this.formData);\r\n\r\n        const response = await axios.post(\r\n          `http://localhost:5000/api/child-development/${childId}`,\r\n          this.formData,\r\n          {\r\n            headers: {\r\n              'Authorization': `Bearer ${token}`,\r\n              'Content-Type': 'application/json'\r\n            }\r\n          }\r\n        );\r\n\r\n        if (response.data) {\r\n          console.log(\"‚úÖ Development data saved:\", response.data);\r\n          this.$router.push(`/child-development-list/${childId}`);\r\n        }\r\n      } catch (error) {\r\n        console.error('‚ùå Error saving development:', error);\r\n        this.error = 'Gre≈°ka pri ƒçuvanju podataka.';\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.error-message {\r\n  color: red;\r\n  font-size: 14px;\r\n}\r\n</style>\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n$primary-color: #4CAF50;\r\n$primary-disabled: #cccccc;\r\n$error-bg: #ffebee;\r\n$error-text: red;\r\n$border-color: #ddd;\r\n$text-color: #333;\r\n$background-light: #ffffff;\r\n\r\n.development-form {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r